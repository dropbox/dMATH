{
  "id": "typeshed",
  "name": "Typeshed",
  "category": "type_stubs",
  "subcategory": "python",

  "description": "Central repository of type stubs for the Python standard library and popular packages",
  "long_description": "Typeshed provides PEP 561 type stub files (*.pyi) for the Python standard library and many third-party packages. Type checkers such as mypy, Pyright, and Pyre rely on Typeshed for accurate interface information when source packages lack inline annotations.",

  "capabilities": [
    "stdlib_stubs",
    "third_party_stubs",
    "protocol_definitions",
    "version_specific_stubs",
    "pep561_support"
  ],
  "property_types": ["type_safety", "developer_experience"],
  "input_languages": ["python", "pyi"],
  "output_formats": ["stubs"],

  "installation": {
    "methods": [
      {"type": "pip", "command": "pip install typeshed"},
      {"type": "git", "url": "https://github.com/python/typeshed"}
    ],
    "dependencies": [],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://github.com/python/typeshed",
    "tutorial": "https://typing.readthedocs.io/en/latest/source/stubs.html",
    "api_reference": "https://github.com/python/typeshed/tree/main/stdlib",
    "examples": "https://github.com/python/typeshed/tree/main/stubs"
  },

  "tactics": [
    {
      "name": "vendor_stub",
      "description": "Vendor missing stubs for a dependency",
      "syntax": "pip install types-pkgname",
      "when_to_use": "Third-party library lacks inline typing",
      "examples": ["pip install types-requests"]
    },
    {
      "name": "contribute_stub",
      "description": "Add or fix a stub upstream",
      "syntax": "python -m mypy.stubtest package",
      "when_to_use": "Validating stubs before PR to Typeshed",
      "examples": ["python -m mypy.stubtest pandas"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "Cannot find implementation or library stub",
      "meaning": "Type checker could not locate a stub for a module",
      "common_causes": ["Dependency missing PEP 561 markers", "Package not installed", "Wrong python version stubs"],
      "fixes": ["Install types- package", "Add py.typed marker", "Pin stub package version"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["type_safety"],
    "cli_command": "dashprove verify --backend typeshed"
  },

  "performance": {
    "typical_runtime": "N/A (data files)",
    "scalability": "Lightweight; loaded by type checkers",
    "memory_usage": "Low"
  },

  "comparisons": {
    "similar_tools": ["pytype stubs", "custom stubs"],
    "advantages": ["Community maintained", "Broad coverage", "Versioned per Python release"],
    "disadvantages": ["May lag newest APIs", "Requires coordination for updates"]
  },

  "metadata": {
    "version": "latest",
    "last_updated": "2025-12-23",
    "maintainer": "Python Typeshed Team",
    "license": "Apache-2.0"
  }
}
