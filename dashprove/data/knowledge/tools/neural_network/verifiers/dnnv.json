{
  "id": "dnnv",
  "name": "DNNV",
  "category": "neural_network_verifier",
  "subcategory": "framework",

  "description": "Deep Neural Network Verification framework",
  "long_description": "DNNV is a framework that provides a common interface to multiple neural network verification tools. It standardizes property specification and supports many backend verifiers.",

  "capabilities": ["unified_interface", "property_language", "multiple_backends"],
  "property_types": ["safety", "robustness"],
  "input_languages": ["dnnp", "onnx"],
  "output_formats": ["verification_result"],

  "installation": {
    "methods": [{"type": "pip", "command": "pip install dnnv"}],
    "dependencies": ["onnx"],
    "platforms": ["linux"]
  },

  "documentation": {
    "official": "https://github.com/dlshriver/dnnv",
    "tutorial": "https://dnnv.readthedocs.io/",
    "api_reference": "https://dnnv.readthedocs.io/en/stable/api/",
    "examples": "https://github.com/dlshriver/dnnv/tree/main/examples"
  },

  "tactics": [
    {"name": "verify", "description": "Run verification", "syntax": "dnnv verify property.dnnp network.onnx", "when_to_use": "For unified access", "examples": []}
  ],

  "error_patterns": [],

  "integration": {"dashprove_backend": true, "usl_property_types": ["nn_safety"], "cli_command": "dashprove verify --backend dnnv"},

  "performance": {"typical_runtime": "backend dependent", "scalability": "Backend dependent", "memory_usage": "Moderate"},

  "comparisons": {"similar_tools": ["vnnlib"], "advantages": ["Unified interface", "Multiple backends"], "disadvantages": ["Overhead"]},

  "metadata": {"version": "0.6.0", "last_updated": "2025-12-22", "maintainer": "David Shriver", "license": "MIT"}
}
