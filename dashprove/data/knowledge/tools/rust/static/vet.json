{
  "id": "vet",
  "name": "cargo-vet",
  "category": "static_analysis",
  "subcategory": "supply_chain",

  "description": "Supply chain security for Rust dependencies",
  "long_description": "cargo-vet helps ensure third-party Rust dependencies have been audited by a trusted entity. It maintains audit records and can import audits from other organizations.",

  "capabilities": [
    "audit_tracking",
    "trust_management",
    "audit_import"
  ],
  "property_types": ["supply_chain_security"],
  "input_languages": ["rust"],
  "output_formats": ["diagnostic"],

  "installation": {
    "methods": [
      {"type": "cargo", "command": "cargo install cargo-vet"}
    ],
    "dependencies": [],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://mozilla.github.io/cargo-vet/",
    "tutorial": "https://mozilla.github.io/cargo-vet/getting-started.html",
    "api_reference": "https://mozilla.github.io/cargo-vet/",
    "examples": "https://github.com/mozilla/cargo-vet"
  },

  "tactics": [
    {
      "name": "check",
      "description": "Check audits",
      "syntax": "cargo vet",
      "when_to_use": "For supply chain check",
      "examples": ["cargo vet check"]
    },
    {
      "name": "certify",
      "description": "Record audit",
      "syntax": "cargo vet certify",
      "when_to_use": "After auditing crate",
      "examples": ["cargo vet certify crate 1.0.0"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "not audited",
      "meaning": "Dependency not audited",
      "common_causes": ["New dependency"],
      "fixes": ["Audit and certify", "Import audit"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["supply_chain"],
    "cli_command": "dashprove verify --backend vet"
  },

  "performance": {
    "typical_runtime": "seconds",
    "scalability": "Fast",
    "memory_usage": "Low"
  },

  "comparisons": {
    "similar_tools": ["cargo-crev"],
    "advantages": [
      "Organization focus",
      "Audit import"
    ],
    "disadvantages": [
      "Requires audit work"
    ]
  },

  "metadata": {
    "version": "0.9.1",
    "last_updated": "2025-12-22",
    "maintainer": "Mozilla",
    "license": "Apache-2.0/MIT"
  }
}
