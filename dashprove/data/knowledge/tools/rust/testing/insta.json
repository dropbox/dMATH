  "description": "Snapshot testing library for Rust",
  "long_description": "insta is a snapshot testing library that records test output and compares against stored snapshots. Great for testing complex outputs like ASTs, serialized data, or CLI output.",

  "capabilities": ["snapshot_testing", "inline_snapshots", "redaction", "review_workflow"],
  "property_types": ["snapshot"],
  "input_languages": ["rust"],
  "output_formats": ["snapshot_file"],

  "installation": {
    "methods": [{"type": "cargo", "command": "cargo add insta --dev"}],
    "dependencies": [],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://insta.rs/",
    "tutorial": "https://insta.rs/docs/",
    "api_reference": "https://docs.rs/insta/",
    "examples": "https://github.com/mitsuhiko/insta"
  },

  "tactics": [
    {"name": "snapshot", "description": "Take snapshot", "syntax": "assert_snapshot!(value)", "when_to_use": "For complex output testing", "examples": ["assert_snapshot!(output)"]}
  ],

  "error_patterns": [],

  "integration": {"dashprove_backend": true, "usl_property_types": ["snapshot"], "cli_command": "dashprove verify --backend insta"},

  "performance": {"typical_runtime": "fast", "scalability": "Good", "memory_usage": "Low"},

