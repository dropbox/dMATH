{
  "id": "tamarin",
  "name": "Tamarin Prover",
  "category": "security_protocol",
  "subcategory": "symbolic",

  "description": "Security protocol verification with equational theories",
  "long_description": "Tamarin is a powerful tool for the symbolic analysis of security protocols. It supports Diffie-Hellman exponentiation and other equational theories, and can prove properties including secrecy, authentication, and complex trace properties.",

  "capabilities": ["protocol_verification", "equational_theories", "trace_properties", "observational_equivalence"],
  "property_types": ["secrecy", "authentication", "trace"],
  "input_languages": ["tamarin_spthy"],
  "output_formats": ["proof", "attack_trace"],

  "installation": {
    "methods": [{"type": "brew", "command": "brew install tamarin-prover"}, {"type": "apt", "command": "apt install tamarin-prover"}],
    "dependencies": ["maude"],
    "platforms": ["linux", "macos"]
  },

  "documentation": {
    "official": "https://tamarin-prover.github.io/",
    "tutorial": "https://tamarin-prover.github.io/manual/",
    "api_reference": "https://tamarin-prover.github.io/manual/",
    "examples": "https://github.com/tamarin-prover/tamarin-prover/tree/develop/examples"
  },

  "tactics": [
    {"name": "autoprove", "description": "Automatic proving", "syntax": "--prove", "when_to_use": "For automatic proofs", "examples": ["tamarin-prover --prove protocol.spthy"]}
  ],

  "error_patterns": [{"pattern": "attack found", "meaning": "Protocol flaw", "common_causes": ["Missing authentication"], "fixes": ["Add nonce", "Fix handshake"]}],

  "integration": {"dashprove_backend": true, "usl_property_types": ["security"], "cli_command": "dashprove verify --backend tamarin"},

  "performance": {"typical_runtime": "seconds to hours", "scalability": "Good", "memory_usage": "Moderate"},

  "comparisons": {"similar_tools": ["proverif", "verifpal"], "advantages": ["Equational theories", "Interactive proofs"], "disadvantages": ["Learning curve"]},

  "metadata": {"version": "1.8.0", "last_updated": "2025-12-22", "maintainer": "ETH Zurich", "license": "GPL-3.0"}
}
