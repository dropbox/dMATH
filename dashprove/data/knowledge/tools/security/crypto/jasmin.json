{
  "id": "jasmin",
  "name": "Jasmin",
  "category": "cryptographic_verification",
  "subcategory": "implementation",

  "description": "Language for high-assurance cryptographic implementations",
  "long_description": "Jasmin is a framework for developing high-speed and high-assurance cryptographic implementations. It provides a language that compiles to efficient assembly while enabling formal verification.",

  "capabilities": ["verified_crypto", "assembly_generation", "constant_time"],
  "property_types": ["functional_correctness", "constant_time"],
  "input_languages": ["jasmin"],
  "output_formats": ["assembly", "easycrypt_spec"],

  "installation": {
    "methods": [{"type": "opam", "command": "opam install jasmin"}],
    "dependencies": ["ocaml"],
    "platforms": ["linux"]
  },

  "documentation": {
    "official": "https://github.com/jasmin-lang/jasmin",
    "tutorial": "https://github.com/jasmin-lang/jasmin/wiki",
    "api_reference": "https://github.com/jasmin-lang/jasmin/wiki",
    "examples": "https://github.com/jasmin-lang/jasmin/tree/main/compiler/examples"
  },

  "tactics": [],

  "error_patterns": [],

  "integration": {"dashprove_backend": true, "usl_property_types": ["crypto_impl"], "cli_command": "dashprove verify --backend jasmin"},

  "performance": {"typical_runtime": "seconds", "scalability": "Good", "memory_usage": "Low"},

  "comparisons": {"similar_tools": ["fiat-crypto", "hacl*"], "advantages": ["Fast code", "Verified"], "disadvantages": ["Complex"]},

