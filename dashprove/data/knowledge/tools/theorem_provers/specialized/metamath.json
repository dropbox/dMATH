{
  "id": "metamath",
  "name": "Metamath",
  "category": "theorem_prover",
  "subcategory": "minimal",

  "description": "Minimalist formal proof language and verifier",
  "long_description": "Metamath is a tiny language for formal proofs with an extremely simple verifier. The Metamath Proof Explorer contains thousands of proofs from axioms of set theory, making it valuable for proof verification and education.",

  "capabilities": [
    "minimal_verifier",
    "substitution_based",
    "proof_explorer",
    "multiple_foundations"
  ],
  "property_types": ["theorem"],
  "input_languages": ["metamath"],
  "output_formats": ["proof_tree"],

  "installation": {
    "methods": [
      {"type": "source", "url": "https://github.com/metamath/metamath-exe"}
    ],
    "dependencies": [],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "http://metamath.org/",
    "tutorial": "http://us.metamath.org/mpeuni/mmset.html",
    "api_reference": "http://us.metamath.org/downloads/metamath.pdf",
    "examples": "http://us.metamath.org/mpeuni/mmset.html"
  },

  "tactics": [
    {
      "name": "prove",
      "description": "Enter proof mode",
      "syntax": "prove label",
      "when_to_use": "To prove theorem",
      "examples": ["prove my_theorem"]
    },
    {
      "name": "assign",
      "description": "Apply theorem",
      "syntax": "assign step label",
      "when_to_use": "To use existing theorem",
      "examples": ["assign 1 ax-mp"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "Proof incomplete",
      "meaning": "Proof not finished",
      "common_causes": ["Missing steps"],
      "fixes": ["Complete proof"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["theorem"],
    "cli_command": "dashprove verify --backend metamath"
  },

  "performance": {
    "typical_runtime": "milliseconds",
    "scalability": "Large databases",
    "memory_usage": "Low"
  },

  "comparisons": {
    "similar_tools": ["mizar", "automath"],
    "advantages": [
      "Ultra-simple verifier",
      "Educational value",
      "Large proof database"
    ],
    "disadvantages": [
      "Verbose proofs",
      "No tactics"
    ]
  },

  "metadata": {
    "version": "0.199",
    "last_updated": "2025-12-22",
    "maintainer": "Metamath Project",
    "license": "CC0/GPL"
  }
}
