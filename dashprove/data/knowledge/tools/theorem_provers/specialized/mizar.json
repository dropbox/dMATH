{
  "id": "mizar",
  "name": "Mizar",
  "category": "theorem_prover",
  "subcategory": "mathematical",

  "description": "Proof assistant for formalized mathematics",
  "long_description": "Mizar is a proof assistant designed for writing mathematical proofs in a style close to natural mathematical language. The Mizar Mathematical Library (MML) is one of the largest repositories of formalized mathematics.",

  "capabilities": [
    "declarative_proofs",
    "natural_language_style",
    "large_math_library",
    "automatic_proof_checking"
  ],
  "property_types": ["theorem", "definition"],
  "input_languages": ["mizar"],
  "output_formats": ["proof_validation"],

  "installation": {
    "methods": [
      {"type": "download", "url": "http://mizar.org/"}
    ],
    "dependencies": [],
    "platforms": ["linux", "windows"]
  },

  "documentation": {
    "official": "http://mizar.org/",
    "tutorial": "http://mizar.org/project/mizar-in-nutshell.pdf",
    "api_reference": "http://mizar.org/library/",
    "examples": "http://mizar.org/library/"
  },

  "tactics": [
    {
      "name": "by",
      "description": "Justify step",
      "syntax": "by theorem_name",
      "when_to_use": "To cite justification",
      "examples": ["hence thesis by A1,A2;"]
    },
    {
      "name": "proof",
      "description": "Block proof",
      "syntax": "proof ... end;",
      "when_to_use": "For structured proofs",
      "examples": ["proof assume A1: x > 0; ... end;"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "This inference is not accepted",
      "meaning": "Proof step not justified",
      "common_causes": ["Missing reference", "Logic gap"],
      "fixes": ["Add justification"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["theorem"],
    "cli_command": "dashprove verify --backend mizar"
  },

  "performance": {
    "typical_runtime": "seconds",
    "scalability": "Large library",
    "memory_usage": "Moderate"
  },

  "comparisons": {
    "similar_tools": ["isabelle", "coq", "metamath"],
    "advantages": [
      "Natural proof style",
      "Large MML",
      "Math focus"
    ],
    "disadvantages": [
      "Limited tactics",
      "Older tooling"
    ]
  },

  "metadata": {
    "version": "8.1.11",
    "last_updated": "2025-12-22",
    "maintainer": "Mizar Project",
    "license": "GPL"
  }
}
