{
  "id": "fstar",
  "name": "F*",
  "category": "theorem_prover",
  "subcategory": "dependent_types",

  "description": "ML-like language with dependent types and SMT-based verification",
  "long_description": "F* (pronounced F-star) is a functional programming language with dependent types, refinement types, and effects. It combines proof-assistant capabilities with SMT-based automation, making it suitable for verified systems programming including cryptographic implementations.",

  "capabilities": [
    "dependent_types",
    "refinement_types",
    "smt_automation",
    "effect_system",
    "kremlin_extraction"
  ],
  "property_types": ["refinement", "lemma", "theorem"],
  "input_languages": ["fstar"],
  "output_formats": ["ocaml", "c", "wasm"],

  "installation": {
    "methods": [
      {"type": "binary", "url": "https://github.com/FStarLang/FStar/releases"},
      {"type": "opam", "command": "opam install fstar"},
      {"type": "source", "url": "https://github.com/FStarLang/FStar"}
    ],
    "dependencies": ["z3", "ocaml"],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://www.fstar-lang.org/",
    "tutorial": "https://raw.githubusercontent.com/FStarLang/FStar/master/doc/book/README.md",
    "api_reference": "https://fstar-lang.org/docs/",
    "examples": "https://github.com/FStarLang/FStar/tree/master/examples"
  },

  "tactics": [
    {
      "name": "smt",
      "description": "Call SMT solver",
      "syntax": "SMT() or assert by SMT",
      "when_to_use": "For arithmetic, decidable properties",
      "examples": ["assert (x + y = y + x) by SMT()"]
    },
    {
      "name": "norm",
      "description": "Normalize term",
      "syntax": "norm [steps]",
      "when_to_use": "To compute/simplify",
      "examples": ["norm [delta; zeta; primops]"]
    },
    {
      "name": "admit",
      "description": "Admit without proof",
      "syntax": "admit()",
      "when_to_use": "Temporary placeholder",
      "examples": ["let proof = admit()"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "Subtyping check failed",
      "meaning": "Refinement not satisfied",
      "common_causes": ["Precondition not met", "SMT can't prove"],
      "fixes": ["Add lemma", "Strengthen context"]
    },
    {
      "pattern": "Z3 returned unknown",
      "meaning": "SMT solver couldn't decide",
      "common_causes": ["Complex formula", "Quantifiers"],
      "fixes": ["Add hints", "Use manual proof"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["refinement", "theorem"],
    "cli_command": "dashprove verify --backend fstar"
  },

  "performance": {
    "typical_runtime": "seconds to minutes",
    "scalability": "Good with SMT",
    "memory_usage": "Moderate"
  },

  "comparisons": {
    "similar_tools": ["dafny", "liquid_haskell", "coq"],
    "advantages": [
      "SMT automation",
      "Verified crypto (HACL*)",
      "C extraction"
    ],
    "disadvantages": [
      "Steep learning curve",
      "SMT unpredictability"
    ]
  },

  "metadata": {
    "version": "2024.09.05",
    "last_updated": "2025-12-22",
    "maintainer": "Microsoft Research and Inria",
    "license": "Apache-2.0"
  }
}
