{
  "id": "mathsat",
  "name": "MathSAT",
  "category": "smt_solver",
  "subcategory": "general",

  "description": "SMT solver with strong support for optimization and interpolation",
  "long_description": "MathSAT 5 is an SMT solver developed at FBK and University of Trento. It provides excellent support for linear arithmetic, optimization (via OptiMathSAT), and Craig interpolation. MathSAT is widely used in model checking and program verification tools.",

  "capabilities": [
    "sat_solving",
    "linear_arithmetic",
    "bit_vectors",
    "arrays",
    "interpolation",
    "optimization",
    "allsat",
    "model_generation"
  ],
  "property_types": ["formula", "assertion", "constraint", "optimization_objective"],
  "input_languages": ["smtlib2", "dimacs"],
  "output_formats": ["sat/unsat", "model", "interpolant", "optimal_value"],

  "installation": {
    "methods": [
      {"type": "download", "url": "https://mathsat.fbk.eu/download.html"},
      {"type": "pip", "command": "pip install mathsat"}
    ],
    "dependencies": ["gmp"],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://mathsat.fbk.eu/",
    "tutorial": "https://mathsat.fbk.eu/getting_started.html",
    "api_reference": "https://mathsat.fbk.eu/api.html",
    "examples": "https://mathsat.fbk.eu/examples.html"
  },

  "tactics": [
    {
      "name": "interpolation",
      "description": "Compute Craig interpolants",
      "syntax": "(get-interpolant (A) (B))",
      "when_to_use": "For abstraction refinement",
      "examples": ["(compute-interpolant)"]
    },
    {
      "name": "allsat",
      "description": "Enumerate all satisfying assignments",
      "syntax": "(all-sat (vars))",
      "when_to_use": "For complete enumeration",
      "examples": ["(all-sat (x y z))"]
    },
    {
      "name": "optimization",
      "description": "Optimize objective function",
      "syntax": "(minimize term) or (maximize term)",
      "when_to_use": "For optimization problems",
      "examples": ["(minimize (+ x y))"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "out of memory",
      "meaning": "Memory exhausted",
      "common_causes": ["Large formula", "AllSAT with many solutions"],
      "fixes": ["Simplify formula", "Add bounds"]
    },
    {
      "pattern": "unsupported",
      "meaning": "Feature not available",
      "common_causes": ["Nonlinear arithmetic", "Quantifiers"],
      "fixes": ["Use different solver", "Encode linearly"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["constraint", "assertion"],
    "cli_command": "dashprove verify --backend mathsat"
  },

  "performance": {
    "typical_runtime": "milliseconds to minutes",
    "scalability": "Very good for LA and interpolation",
    "memory_usage": "Moderate"
  },

  "comparisons": {
    "similar_tools": ["z3", "cvc5", "yices"],
    "advantages": [
      "Excellent interpolation support",
      "OptiMathSAT for optimization",
      "Strong LA solver"
    ],
    "disadvantages": [
      "Non-commercial license restrictions",
      "Limited quantifier support"
    ]
  },

  "metadata": {
    "version": "5.6.10",
    "last_updated": "2025-12-22",
    "maintainer": "FBK and University of Trento",
    "license": "Non-commercial"
  }
}
