{
  "id": "cvc5",
  "name": "CVC5",
  "category": "smt_solver",
  "subcategory": "general",

  "description": "SMT solver with strong quantifier handling",
  "long_description": "CVC5 is a high-performance SMT solver that excels at quantifier reasoning and higher-order logic. It's the successor to CVC4 and is known for its strong performance on verification benchmarks, particularly those involving quantifiers and algebraic datatypes.",

  "capabilities": [
    "sat_solving",
    "quantifier_instantiation",
    "higher_order",
    "strings",
    "sequences",
    "algebraic_datatypes",
    "finite_model_finding"
  ],
  "property_types": ["formula", "assertion", "constraint"],
  "input_languages": ["smtlib2", "python", "cpp", "java"],
  "output_formats": ["sat/unsat", "model", "proof", "unsat_core"],

  "installation": {
    "methods": [
      {"type": "brew", "command": "brew install cvc5"},
      {"type": "pip", "command": "pip install cvc5"},
      {"type": "source", "url": "https://github.com/cvc5/cvc5"}
    ],
    "dependencies": [],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://raw.githubusercontent.com/cvc5/cvc5/main/docs/index.rst",
    "tutorial": "https://raw.githubusercontent.com/cvc5/cvc5/main/docs/options.rst",
    "api_reference": "https://raw.githubusercontent.com/cvc5/cvc5/main/docs/statistics.rst",
    "examples": "https://github.com/cvc5/cvc5/tree/main/examples"
  },

  "tactics": [
    {
      "name": "default",
      "description": "Standard SMT solving",
      "syntax": "(check-sat)",
      "when_to_use": "General purpose",
      "examples": ["(check-sat)"]
    },
    {
      "name": "finite-model-find",
      "description": "Find finite models",
      "syntax": "--finite-model-find",
      "when_to_use": "For quantified formulas with finite domains",
      "examples": ["cvc5 --finite-model-find file.smt2"]
    },
    {
      "name": "strings",
      "description": "String theory reasoning",
      "syntax": "(set-logic QF_SLIA)",
      "when_to_use": "For string constraints",
      "examples": ["(declare-const s String)", "(assert (str.contains s \"abc\"))"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "timeout",
      "meaning": "Exceeded time limit",
      "common_causes": ["Hard quantifier problem", "Large search space"],
      "fixes": ["Add finite bounds", "Simplify formula"]
    },
    {
      "pattern": "incomplete",
      "meaning": "Using incomplete theory",
      "common_causes": ["Nonlinear arithmetic", "Unbounded quantifiers"],
      "fixes": ["Use complete fragment", "Add bounds"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["constraint", "assertion"],
    "cli_command": "dashprove verify --backend cvc5"
  },

  "performance": {
    "typical_runtime": "milliseconds to hours",
    "scalability": "Excellent for quantifiers",
    "memory_usage": "Moderate"
  },

  "comparisons": {
    "similar_tools": ["z3", "yices", "vampire"],
    "advantages": [
      "Best quantifier reasoning",
      "Strong string theory",
      "Good proof production"
    ],
    "disadvantages": [
      "Slower than Z3 on some benchmarks"
    ]
  },

  "metadata": {
    "version": "1.0.8",
    "last_updated": "2025-12-20",
    "maintainer": "Stanford/Iowa",
    "license": "BSD-3"
  }
}
