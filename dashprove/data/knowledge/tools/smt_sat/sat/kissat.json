{
  "id": "kissat",
  "name": "Kissat",
  "category": "sat_solver",
  "subcategory": "cdcl",

  "description": "Highly optimized SAT solver (CaDiCaL derivative)",
  "long_description": "Kissat is a highly optimized SAT solver derived from CaDiCaL. It won the SAT Competition 2020, 2021, and 2022 main tracks. Kissat focuses on raw solving performance with optimized data structures and algorithms, making it the current state-of-the-art for sequential SAT solving.",

  "capabilities": [
    "sat_solving",
    "conflict_driven_clause_learning",
    "inprocessing",
    "vivification",
    "failed_literal_probing",
    "proof_logging"
  ],
  "property_types": ["cnf_formula"],
  "input_languages": ["dimacs_cnf"],
  "output_formats": ["sat/unsat", "model", "drat_proof"],

  "installation": {
    "methods": [
      {"type": "source", "url": "https://github.com/arminbiere/kissat"}
    ],
    "dependencies": [],
    "platforms": ["linux", "macos"]
  },

  "documentation": {
    "official": "https://github.com/arminbiere/kissat",
    "tutorial": "https://github.com/arminbiere/kissat#readme",
    "api_reference": "https://github.com/arminbiere/kissat/blob/master/src/kissat.h",
    "examples": "https://github.com/arminbiere/kissat/tree/master/test"
  },

  "tactics": [
    {
      "name": "stable",
      "description": "Stable mode with focused restarts",
      "syntax": "--stable=1",
      "when_to_use": "For hard satisfiable instances",
      "examples": ["kissat --stable=1 input.cnf"]
    },
    {
      "name": "tier",
      "description": "Tiered clause management",
      "syntax": "Internal",
      "when_to_use": "Automatically manages clause quality tiers",
      "examples": []
    }
  ],

  "error_patterns": [
    {
      "pattern": "s UNSATISFIABLE",
      "meaning": "Formula is unsatisfiable",
      "common_causes": ["Contradictory constraints"],
      "fixes": ["Check encoding"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["boolean_satisfiability"],
    "cli_command": "dashprove verify --backend kissat"
  },

  "performance": {
    "typical_runtime": "milliseconds to hours",
    "scalability": "Best in class sequential",
    "memory_usage": "Optimized"
  },

  "comparisons": {
    "similar_tools": ["cadical", "glucose", "cryptominisat"],
    "advantages": [
      "Multiple SAT Competition wins",
      "Highly optimized",
      "State-of-the-art"
    ],
    "disadvantages": [
      "C only (no C++ API)",
      "No parallel version"
    ]
  },

  "metadata": {
    "version": "3.1.1",
    "last_updated": "2025-12-22",
    "maintainer": "Armin Biere",
    "license": "MIT"
  }
}
