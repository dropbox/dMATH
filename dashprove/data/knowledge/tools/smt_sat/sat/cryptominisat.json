{
  "id": "cryptominisat",
  "name": "CryptoMiniSat",
  "category": "sat_solver",
  "subcategory": "cdcl",

  "description": "SAT solver with XOR reasoning for cryptographic problems",
  "long_description": "CryptoMiniSat is a SAT solver designed for problems with XOR constraints, common in cryptographic applications. It uses Gaussian elimination on XOR clauses and integrates them with CDCL search. The solver also works well on general SAT instances.",

  "capabilities": [
    "sat_solving",
    "xor_reasoning",
    "gaussian_elimination",
    "conflict_driven_clause_learning",
    "parallel_solving",
    "model_generation"
  ],
  "property_types": ["cnf_formula", "xor_cnf"],
  "input_languages": ["dimacs_cnf", "xor_cnf"],
  "output_formats": ["sat/unsat", "model", "drat_proof"],

  "installation": {
    "methods": [
      {"type": "pip", "command": "pip install pycryptosat"},
      {"type": "apt", "command": "apt install cryptominisat"},
      {"type": "source", "url": "https://github.com/msoos/cryptominisat"}
    ],
    "dependencies": ["cmake", "boost"],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://github.com/msoos/cryptominisat",
    "tutorial": "https://github.com/msoos/cryptominisat#readme",
    "api_reference": "https://github.com/msoos/cryptominisat/blob/master/src/cryptominisat.h",
    "examples": "https://github.com/msoos/cryptominisat/tree/master/tests"
  },

  "tactics": [
    {
      "name": "xor",
      "description": "XOR clause reasoning",
      "syntax": "x1 2 3 0 (XOR of vars 1,2,3 = true)",
      "when_to_use": "For cryptographic problems",
      "examples": ["c this is an XOR clause", "x1 2 3 0"]
    },
    {
      "name": "gauss",
      "description": "Gaussian elimination",
      "syntax": "--gaussuntil=N",
      "when_to_use": "For problems with many XORs",
      "examples": ["cryptominisat --gaussuntil=100 input.cnf"]
    },
    {
      "name": "parallel",
      "description": "Multi-threaded solving",
      "syntax": "-t N",
      "when_to_use": "For multi-core machines",
      "examples": ["cryptominisat -t 4 input.cnf"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "s UNSATISFIABLE",
      "meaning": "Formula is unsatisfiable",
      "common_causes": ["Contradictory constraints", "XOR conflicts"],
      "fixes": ["Check encoding"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["boolean_satisfiability"],
    "cli_command": "dashprove verify --backend cryptominisat"
  },

  "performance": {
    "typical_runtime": "milliseconds to hours",
    "scalability": "Excellent for cryptographic SAT",
    "memory_usage": "Moderate"
  },

  "comparisons": {
    "similar_tools": ["kissat", "cadical", "glucose"],
    "advantages": [
      "Native XOR support",
      "Gaussian elimination",
      "Python bindings"
    ],
    "disadvantages": [
      "Slightly slower on pure CNF",
      "More complex codebase"
    ]
  },

  "metadata": {
    "version": "5.11.15",
    "last_updated": "2025-12-22",
    "maintainer": "Mate Soos",
    "license": "MIT"
  }
}
