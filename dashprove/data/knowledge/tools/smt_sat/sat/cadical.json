{
  "id": "cadical",
  "name": "CaDiCaL",
  "category": "sat_solver",
  "subcategory": "cdcl",

  "description": "State-of-the-art SAT solver with inprocessing",
  "long_description": "CaDiCaL is a modern CDCL SAT solver developed by Armin Biere that incorporates extensive inprocessing techniques. It won the SAT Competition 2019 main track and remains one of the top-performing solvers. CaDiCaL serves as the backend for Kissat.",

  "capabilities": [
    "sat_solving",
    "conflict_driven_clause_learning",
    "inprocessing",
    "bounded_variable_elimination",
    "vivification",
    "proof_logging"
  ],
  "property_types": ["cnf_formula"],
  "input_languages": ["dimacs_cnf"],
  "output_formats": ["sat/unsat", "model", "drat_proof"],

  "installation": {
    "methods": [
      {"type": "source", "url": "https://github.com/arminbiere/cadical"},
      {"type": "apt", "command": "apt install cadical"}
    ],
    "dependencies": [],
    "platforms": ["linux", "macos"]
  },

  "documentation": {
    "official": "https://github.com/arminbiere/cadical",
    "tutorial": "https://github.com/arminbiere/cadical#readme",
    "api_reference": "https://github.com/arminbiere/cadical/blob/master/src/cadical.hpp",
    "examples": "https://github.com/arminbiere/cadical/tree/master/test"
  },

  "tactics": [
    {
      "name": "inprocessing",
      "description": "Simplification during search",
      "syntax": "--inprocessing",
      "when_to_use": "Default enabled, improves on hard instances",
      "examples": []
    },
    {
      "name": "bve",
      "description": "Bounded variable elimination",
      "syntax": "--elim",
      "when_to_use": "Reduces formula size",
      "examples": ["cadical --elim input.cnf"]
    },
    {
      "name": "drat-proof",
      "description": "Generate DRAT proof",
      "syntax": "--proof=output.drat",
      "when_to_use": "For proof checking",
      "examples": ["cadical input.cnf --proof=proof.drat"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "s UNSATISFIABLE",
      "meaning": "Formula is unsatisfiable",
      "common_causes": ["Contradictory constraints"],
      "fixes": ["Check encoding"]
    },
    {
      "pattern": "out of memory",
      "meaning": "Memory exhausted",
      "common_causes": ["Very large formula"],
      "fixes": ["Increase memory", "Simplify formula"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["boolean_satisfiability"],
    "cli_command": "dashprove verify --backend cadical"
  },

  "performance": {
    "typical_runtime": "milliseconds to hours",
    "scalability": "Best in class",
    "memory_usage": "Moderate"
  },

  "comparisons": {
    "similar_tools": ["kissat", "glucose", "minisat"],
    "advantages": [
      "State-of-the-art performance",
      "Excellent inprocessing",
      "Clean C++ API"
    ],
    "disadvantages": [
      "No parallel version"
    ]
  },

  "metadata": {
    "version": "1.9.5",
    "last_updated": "2025-12-22",
    "maintainer": "Armin Biere",
    "license": "MIT"
  }
}
