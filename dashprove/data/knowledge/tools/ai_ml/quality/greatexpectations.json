{
  "id": "greatexpectations",
  "name": "Great Expectations",
  "category": "ai_ml",
  "subcategory": "data_quality",
  "description": "Python library for validating, documenting, and profiling data",
  "long_description": "Great Expectations is the leading Python library for data validation, documentation, and profiling. It helps data teams eliminate pipeline debt through shared, documented expectations about data. Features include data profiling, automatic documentation, and integration with data pipelines.",
  "capabilities": [
    "data_validation",
    "expectation_suites",
    "data_profiling",
    "auto_documentation",
    "checkpoint_validation",
    "pipeline_integration",
    "custom_expectations",
    "data_docs"
  ],
  "property_types": [
    "data_quality",
    "schema",
    "completeness"
  ],
  "input_languages": [
    "python"
  ],
  "output_formats": [
    "validation_result",
    "data_docs",
    "json"
  ],
  "installation": {
    "methods": [
      {
        "type": "pip",
        "command": "pip install great_expectations"
      },
      {
        "type": "source",
        "url": "https://github.com/great-expectations/great_expectations"
      }
    ],
    "dependencies": [
      "pandas",
      "sqlalchemy"
    ],
    "platforms": [
      "linux",
      "macos",
      "windows"
    ]
  },
  "documentation": {
    "official": "https://greatexpectations.io/",
    "tutorial": "https://docs.greatexpectations.io/docs/tutorials/",
    "api_reference": "https://docs.greatexpectations.io/docs/reference/api/",
    "examples": "https://github.com/great-expectations/great_expectations/tree/develop/examples"
  },
  "tactics": [
    {
      "name": "expect_column",
      "description": "Define column expectation",
      "syntax": "expect_column_values_to_be_between(column, min, max)",
      "when_to_use": "Validating column constraints",
      "examples": [
        "validator.expect_column_values_to_be_between('age', 0, 120)"
      ]
    },
    {
      "name": "expect_table",
      "description": "Define table expectation",
      "syntax": "expect_table_row_count_to_be_between(min, max)",
      "when_to_use": "Validating table properties",
      "examples": [
        "validator.expect_table_row_count_to_be_between(1000, 10000)"
      ]
    },
    {
      "name": "checkpoint",
      "description": "Run validation checkpoint",
      "syntax": "context.run_checkpoint(checkpoint_name)",
      "when_to_use": "Validating data in pipeline",
      "examples": [
        "result = context.run_checkpoint('my_checkpoint')"
      ]
    },
    {
      "name": "profile",
      "description": "Auto-generate expectations from data",
      "syntax": "profiler.profile(batch_request)",
      "when_to_use": "Discovering data characteristics",
      "examples": [
        "suite = profiler.profile(batch_request=batch_request)"
      ]
    }
  ],
  "error_patterns": [
    {
      "pattern": "Validation failed",
      "meaning": "Data did not meet expectations",
      "common_causes": [
        "Data quality issue",
        "Schema change"
      ],
      "fixes": [
        "Fix source data",
        "Update expectations"
      ]
    },
    {
      "pattern": "Expectation suite not found",
      "meaning": "Suite does not exist",
      "common_causes": [
        "Typo in name",
        "Not created"
      ],
      "fixes": [
        "Check suite name",
        "Create suite first"
      ]
    }
  ],
  "integration": {
    "dashprove_backend": true,
    "usl_property_types": [
      "data_quality"
    ],
    "cli_command": "dashprove verify --backend greatexpectations"
  },
  "performance": {
    "typical_runtime": "Seconds to minutes",
    "scalability": "Handles large datasets",
    "memory_usage": "Depends on dataset"
  },
  "comparisons": {
    "similar_tools": [
      "deepchecks",
      "evidently",
      "deequ"
    ],
    "advantages": [
      "Comprehensive expectation library",
      "Excellent documentation generation",
      "Good pipeline integration",
      "Large community"
    ],
    "disadvantages": [
      "Setup complexity",
      "Learning curve"
    ]
  },
  "metadata": {
    "version": "0.18.0",
    "last_updated": "2025-12-20",
    "maintainer": "Great Expectations",
    "license": "Apache-2.0"
  }
}
