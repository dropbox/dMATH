{
  "id": "interpretml",
  "name": "InterpretML",
  "category": "ai_ml",
  "subcategory": "interpretability",
  "description": "Microsoft toolkit for interpretable machine learning",
  "long_description": "InterpretML is Microsoft's open-source toolkit for training interpretable models and explaining black-box systems. It includes Explainable Boosting Machines (EBMs), which are as accurate as black-box models while being fully interpretable. Also provides unified API for various explanation methods.",
  "capabilities": [
    "explainable_boosting_machine",
    "glass_box_models",
    "black_box_explanations",
    "shap_integration",
    "lime_integration",
    "unified_api",
    "interactive_dashboards",
    "global_explanations"
  ],
  "property_types": [
    "interpretability",
    "explanation",
    "feature_importance"
  ],
  "input_languages": [
    "python"
  ],
  "output_formats": [
    "explanation",
    "dashboard",
    "visualization"
  ],
  "installation": {
    "methods": [
      {
        "type": "pip",
        "command": "pip install interpret"
      },
      {
        "type": "source",
        "url": "https://github.com/interpretml/interpret"
      }
    ],
    "dependencies": [
      "numpy",
      "pandas",
      "scikit-learn"
    ],
    "platforms": [
      "linux",
      "macos",
      "windows"
    ]
  },
  "documentation": {
    "official": "https://interpret.ml/",
    "tutorial": "https://interpret.ml/docs/getting-started",
    "api_reference": "https://interpret.ml/docs/",
    "examples": "https://github.com/interpretml/interpret/tree/develop/examples"
  },
  "tactics": [
    {
      "name": "train_ebm",
      "description": "Train Explainable Boosting Machine",
      "syntax": "ExplainableBoostingClassifier().fit(X, y)",
      "when_to_use": "Training interpretable model",
      "examples": [
        "ebm = ExplainableBoostingClassifier(); ebm.fit(X_train, y_train)"
      ]
    },
    {
      "name": "explain_global",
      "description": "Get global explanation",
      "syntax": "model.explain_global()",
      "when_to_use": "Understanding overall model behavior",
      "examples": [
        "global_exp = ebm.explain_global()"
      ]
    },
    {
      "name": "explain_local",
      "description": "Get local explanation",
      "syntax": "model.explain_local(X)",
      "when_to_use": "Understanding individual prediction",
      "examples": [
        "local_exp = ebm.explain_local(X_test[:5])"
      ]
    },
    {
      "name": "show",
      "description": "Visualize explanation",
      "syntax": "show(explanation)",
      "when_to_use": "Interactive exploration",
      "examples": [
        "show(global_exp); show(local_exp)"
      ]
    }
  ],
  "error_patterns": [
    {
      "pattern": "Model not interpretable",
      "meaning": "Cannot explain this model type",
      "common_causes": [
        "Unsupported model",
        "Wrong wrapper"
      ],
      "fixes": [
        "Use black-box explainer",
        "Wrap model correctly"
      ]
    },
    {
      "pattern": "Visualization error",
      "meaning": "Cannot render dashboard",
      "common_causes": [
        "Missing dependencies",
        "Jupyter issue"
      ],
      "fixes": [
        "Install plotly",
        "Check Jupyter setup"
      ]
    }
  ],
  "integration": {
    "dashprove_backend": true,
    "usl_property_types": [
      "interpretability"
    ],
    "cli_command": "dashprove verify --backend interpretml"
  },
  "performance": {
    "typical_runtime": "Fast training and explanation",
    "scalability": "Good for tabular data",
    "memory_usage": "Moderate"
  },
  "comparisons": {
    "similar_tools": [
      "shap",
      "lime",
      "captum"
    ],
    "advantages": [
      "EBM is accurate AND interpretable",
      "Unified API for many methods",
      "Good visualizations",
      "Microsoft-backed"
    ],
    "disadvantages": [
      "EBM mainly for tabular data",
      "Less focus on deep learning"
    ]
  },
  "metadata": {
    "version": "0.5.0",
    "last_updated": "2025-12-20",
    "maintainer": "Microsoft",
    "license": "MIT"
  }
}
