{
  "id": "captum",
  "name": "Captum",
  "category": "ai_ml",
  "subcategory": "interpretability",
  "description": "PyTorch library for model interpretability and explanations",
  "long_description": "Captum is a model interpretability library for PyTorch developed by Facebook. It provides implementations of various attribution methods including Integrated Gradients, DeepLIFT, GradCAM, and SHAP. Captum helps understand which features contribute to model predictions.",
  "capabilities": [
    "integrated_gradients",
    "deeplift",
    "gradcam",
    "layer_attribution",
    "neuron_attribution",
    "feature_ablation",
    "occlusion",
    "shapley_values"
  ],
  "property_types": [
    "attribution",
    "feature_importance",
    "explanation"
  ],
  "input_languages": [
    "pytorch"
  ],
  "output_formats": [
    "attribution_scores",
    "visualization",
    "insight"
  ],
  "installation": {
    "methods": [
      {
        "type": "pip",
        "command": "pip install captum"
      },
      {
        "type": "source",
        "url": "https://github.com/pytorch/captum"
      }
    ],
    "dependencies": [
      "pytorch",
      "numpy"
    ],
    "platforms": [
      "linux",
      "macos",
      "windows"
    ]
  },
  "documentation": {
    "official": "https://captum.ai/",
    "tutorial": "https://captum.ai/tutorials/",
    "api_reference": "https://captum.ai/api/",
    "examples": "https://github.com/pytorch/captum/tree/main/tutorials"
  },
  "tactics": [
    {
      "name": "integrated_gradients",
      "description": "Compute integrated gradients attribution",
      "syntax": "IntegratedGradients(model).attribute(input, target)",
      "when_to_use": "Feature attribution for differentiable models",
      "examples": [
        "ig = IntegratedGradients(model); attr = ig.attribute(input, target=class_idx)"
      ]
    },
    {
      "name": "layer_gradcam",
      "description": "Compute GradCAM for layer",
      "syntax": "LayerGradCam(model, layer).attribute(input, target)",
      "when_to_use": "Visualizing CNN attention",
      "examples": [
        "gc = LayerGradCam(model, model.layer4); attr = gc.attribute(input, target=idx)"
      ]
    },
    {
      "name": "deeplift",
      "description": "DeepLIFT attribution",
      "syntax": "DeepLift(model).attribute(input, baselines)",
      "when_to_use": "Comparing to reference baseline",
      "examples": [
        "dl = DeepLift(model); attr = dl.attribute(input, baselines=baseline)"
      ]
    },
    {
      "name": "feature_ablation",
      "description": "Ablation-based attribution",
      "syntax": "FeatureAblation(model).attribute(input, baselines, feature_mask)",
      "when_to_use": "Model-agnostic feature importance",
      "examples": [
        "fa = FeatureAblation(model); attr = fa.attribute(input, baselines, mask)"
      ]
    }
  ],
  "error_patterns": [
    {
      "pattern": "Gradient computation error",
      "meaning": "Cannot compute gradients",
      "common_causes": [
        "Non-differentiable ops",
        "Detached tensor"
      ],
      "fixes": [
        "Use ablation methods",
        "Ensure differentiability"
      ]
    },
    {
      "pattern": "Memory error",
      "meaning": "Attribution computation too expensive",
      "common_causes": [
        "Large input",
        "Many integration steps"
      ],
      "fixes": [
        "Reduce n_steps",
        "Use smaller batch"
      ]
    }
  ],
  "integration": {
    "dashprove_backend": true,
    "usl_property_types": [
      "interpretability"
    ],
    "cli_command": "dashprove verify --backend captum"
  },
  "performance": {
    "typical_runtime": "Seconds per sample",
    "scalability": "Depends on model size",
    "memory_usage": "Moderate to high"
  },
  "comparisons": {
    "similar_tools": [
      "shap",
      "lime",
      "interpret"
    ],
    "advantages": [
      "Native PyTorch integration",
      "Many attribution methods",
      "Good documentation",
      "Active development"
    ],
    "disadvantages": [
      "PyTorch only",
      "Some methods expensive"
    ]
  },
  "metadata": {
    "version": "0.7.0",
    "last_updated": "2025-12-20",
    "maintainer": "Meta (Facebook)",
    "license": "BSD-3-Clause"
  }
}
