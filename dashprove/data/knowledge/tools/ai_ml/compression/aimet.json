{
  "id": "aimet",
  "name": "AI Model Efficiency Toolkit (AIMET)",
  "category": "ai_ml",
  "subcategory": "model_compression",
  "description": "Qualcomm toolkit for quantization and compression of trained models",
  "long_description": "AIMET (AI Model Efficiency Toolkit) is a library from Qualcomm for state-of-the-art model quantization and compression. It provides advanced techniques like AdaRound, Cross-Layer Equalization, and Bias Correction to minimize accuracy loss during quantization, especially important for edge deployment.",
  "capabilities": [
    "post_training_quantization",
    "quantization_aware_training",
    "adaround",
    "cross_layer_equalization",
    "bias_correction",
    "channel_pruning",
    "spatial_svd",
    "auto_quant"
  ],
  "property_types": [
    "accuracy",
    "compression_ratio",
    "performance"
  ],
  "input_languages": [
    "pytorch",
    "tensorflow",
    "onnx"
  ],
  "output_formats": [
    "quantized_model",
    "encodings",
    "onnx"
  ],
  "installation": {
    "methods": [
      {
        "type": "pip",
        "command": "pip install aimet-torch"
      },
      {
        "type": "source",
        "url": "https://github.com/quic/aimet"
      }
    ],
    "dependencies": [
      "pytorch",
      "tensorflow"
    ],
    "platforms": [
      "linux"
    ]
  },
  "documentation": {
    "official": "https://quic.github.io/aimet-pages/",
    "tutorial": "https://quic.github.io/aimet-pages/releases/latest/user_guide/",
    "api_reference": "https://quic.github.io/aimet-pages/releases/latest/api_docs/",
    "examples": "https://github.com/quic/aimet/tree/develop/Examples"
  },
  "tactics": [
    {
      "name": "auto_quant",
      "description": "Automatic quantization pipeline",
      "syntax": "AutoQuant(model, dummy_input, data_loader)",
      "when_to_use": "Automatic best quantization",
      "examples": [
        "auto_quant = AutoQuant(model, input, calib_loader); model = auto_quant.apply()"
      ]
    },
    {
      "name": "adaround",
      "description": "Adaptive rounding for quantization",
      "syntax": "Adaround.apply_adaround(model, data_loader)",
      "when_to_use": "Better weight rounding",
      "examples": [
        "model = Adaround.apply_adaround(sim, params, data_loader)"
      ]
    },
    {
      "name": "cle",
      "description": "Cross-layer equalization",
      "syntax": "equalize_model(model)",
      "when_to_use": "Pre-processing for better quantization",
      "examples": [
        "equalize_model(model, input_shape)"
      ]
    },
    {
      "name": "bias_correction",
      "description": "Correct quantization bias",
      "syntax": "BiasCorrection.correct_bias(model, data_loader)",
      "when_to_use": "Reducing quantization error",
      "examples": [
        "BiasCorrection.correct_bias(sim, params, data_loader)"
      ]
    }
  ],
  "error_patterns": [
    {
      "pattern": "Large accuracy drop",
      "meaning": "Quantization significantly degraded accuracy",
      "common_causes": [
        "Sensitive model",
        "Wrong configuration"
      ],
      "fixes": [
        "Use AdaRound",
        "Apply CLE first",
        "Mixed precision"
      ]
    },
    {
      "pattern": "Module not found",
      "meaning": "Layer not supported",
      "common_causes": [
        "Custom module",
        "Dynamic shapes"
      ],
      "fixes": [
        "Register custom quantizer",
        "Simplify model"
      ]
    }
  ],
  "integration": {
    "dashprove_backend": true,
    "usl_property_types": [
      "model_compression"
    ],
    "cli_command": "dashprove verify --backend aimet"
  },
  "performance": {
    "typical_runtime": "Minutes to hours",
    "scalability": "Good for edge deployment",
    "memory_usage": "Moderate"
  },
  "comparisons": {
    "similar_tools": [
      "neuralcompressor",
      "nncf",
      "brevitas"
    ],
    "advantages": [
      "State-of-the-art techniques (AdaRound, CLE)",
      "Good for edge deployment",
      "Qualcomm hardware optimization",
      "Auto quantization pipeline"
    ],
    "disadvantages": [
      "Linux only",
      "Complex setup",
      "Less community support"
    ]
  },
  "metadata": {
    "version": "1.31.0",
    "last_updated": "2025-12-20",
    "maintainer": "Qualcomm",
    "license": "BSD-3-Clause"
  }
}
