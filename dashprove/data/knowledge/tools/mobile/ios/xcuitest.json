{
  "id": "xcuitest",
  "name": "XCUITest",
  "category": "ios_testing",
  "subcategory": "ui_testing",

  "description": "Apple's UI testing framework for iOS, macOS, tvOS",
  "long_description": "XCUITest is Apple's native UI testing framework that enables automated testing of user interfaces. It runs tests in a separate process from the app, interacting with UI elements through accessibility. XCUITest supports recording user interactions, querying UI elements, simulating gestures, and testing accessibility.",

  "capabilities": [
    "ui_automation",
    "accessibility_testing",
    "gesture_simulation",
    "element_querying",
    "screenshot_capture",
    "app_launch_testing",
    "biometric_simulation",
    "push_notification_testing"
  ],
  "property_types": ["ui_assertion", "element_query", "accessibility_check"],
  "input_languages": ["swift", "objective_c"],
  "output_formats": ["xcode_report", "xcresult", "screenshots", "video"],

  "installation": {
    "methods": [
      {"type": "builtin", "command": "Included in Xcode (UI Testing bundle)"}
    ],
    "dependencies": ["xcode"],
    "platforms": ["macos"]
  },

  "documentation": {
    "official": "https://developer.apple.com/documentation/xctest/user_interface_tests",
    "tutorial": "https://developer.apple.com/documentation/xctest/xcuielement",
    "queries": "https://developer.apple.com/documentation/xctest/xcuielementquery",
    "examples": "https://developer.apple.com/library/archive/documentation/DeveloperTools/Conceptual/testing_with_xcode/chapters/09-ui_testing.html"
  },

  "tactics": [
    {
      "name": "launch_app",
      "description": "Launch app for testing",
      "syntax": "XCUIApplication().launch()",
      "when_to_use": "Start of every UI test",
      "examples": ["let app = XCUIApplication()\napp.launch()", "app.launchArguments = [\"--uitesting\"]\napp.launch()"]
    },
    {
      "name": "query_elements",
      "description": "Find UI elements",
      "syntax": "app.buttons[identifier]",
      "when_to_use": "Interacting with specific UI elements",
      "examples": ["app.buttons[\"Login\"]", "app.textFields[\"email\"]", "app.staticTexts.matching(identifier: \"title\")"]
    },
    {
      "name": "tap_element",
      "description": "Tap a UI element",
      "syntax": "element.tap()",
      "when_to_use": "Simulating button presses",
      "examples": ["app.buttons[\"Submit\"].tap()", "app.cells.firstMatch.tap()"]
    },
    {
      "name": "type_text",
      "description": "Enter text in field",
      "syntax": "element.typeText(string)",
      "when_to_use": "Filling in text fields",
      "examples": ["app.textFields[\"email\"].tap()\napp.textFields[\"email\"].typeText(\"test@example.com\")"]
    },
    {
      "name": "wait_for_element",
      "description": "Wait for element to appear",
      "syntax": "element.waitForExistence(timeout:)",
      "when_to_use": "Handling async UI updates",
      "examples": ["XCTAssertTrue(app.buttons[\"Done\"].waitForExistence(timeout: 5))"]
    },
    {
      "name": "swipe_gesture",
      "description": "Perform swipe gesture",
      "syntax": "element.swipeUp/Down/Left/Right()",
      "when_to_use": "Scrolling or dismissing",
      "examples": ["app.tables.firstMatch.swipeUp()", "app.collectionViews.firstMatch.swipeLeft()"]
    }
  ],

  "element_queries": [
    {
      "name": "buttons",
      "description": "Query button elements",
      "examples": ["app.buttons[\"Login\"]", "app.buttons.count"]
    },
    {
      "name": "textFields",
      "description": "Query text field elements",
      "examples": ["app.textFields[\"email\"]", "app.secureTextFields[\"password\"]"]
    },
    {
      "name": "staticTexts",
      "description": "Query label/text elements",
      "examples": ["app.staticTexts[\"Welcome\"]", "app.staticTexts.containing(.text, \"Error\")"]
    },
    {
      "name": "cells",
      "description": "Query table/collection cells",
      "examples": ["app.cells.count", "app.cells.element(boundBy: 0)"]
    },
    {
      "name": "navigationBars",
      "description": "Query navigation bars",
      "examples": ["app.navigationBars[\"Settings\"]", "app.navigationBars.buttons[\"Back\"]"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "No matches found for.*",
      "meaning": "UI element not found",
      "common_causes": ["Wrong identifier", "Element not visible", "Element not loaded yet"],
      "fixes": ["Check accessibility identifier", "Add waitForExistence", "Scroll to element"]
    },
    {
      "pattern": "Multiple matches found",
      "meaning": "Query matches multiple elements",
      "common_causes": ["Non-unique identifier", "Generic query"],
      "fixes": ["Use more specific query", "Use element(boundBy:)", "Add unique accessibility identifier"]
    },
    {
      "pattern": "Element is not hittable",
      "meaning": "Element exists but can't be tapped",
      "common_causes": ["Element obscured", "Element disabled", "Element off-screen"],
      "fixes": ["Scroll element into view", "Wait for animations", "Check element is enabled"]
    }
  ],

  "integration": {
    "dashprove_backend": false,
    "ide_support": ["xcode"],
    "ci_integration": ["xcode_cloud", "github_actions", "jenkins", "fastlane", "bitrise"]
  },

  "performance": {
    "typical_runtime": "seconds to minutes per test",
    "scalability": "Slower than unit tests, run in parallel on device farm",
    "memory_usage": "Moderate (runs separate process)"
  },

  "comparisons": {
    "similar_tools": ["appium", "detox", "maestro", "earl_grey"],
    "advantages": [
      "Native Apple framework - most reliable",
      "Full device feature access",
      "Xcode recording feature",
      "No external dependencies"
    ],
    "disadvantages": [
      "macOS/Xcode only",
      "Slower than unit tests",
      "Flaky if not properly waiting",
      "No cross-platform support"
    ]
  },

  "metadata": {
    "version": "Xcode 15+",
    "last_updated": "2025-12-22",
    "maintainer": "Apple",
    "license": "Proprietary"
  }
}
