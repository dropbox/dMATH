{
  "tool": "nnv",
  "version": "2.x",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "timeout",
      "pattern": "timeout",
      "message": "Verification timed out",
      "cause": "Reachability analysis too slow",
      "solutions": [
        {
          "approach": "Use approximation",
          "code": "reachMethod = 'approx-star'",
          "when": "Exact analysis too slow"
        },
        {
          "approach": "Reduce input set",
          "code": "% Smaller perturbation",
          "when": "Large input region"
        },
        {
          "approach": "Use GPU",
          "code": "% Enable GPU computation",
          "when": "CPU too slow"
        }
      ]
    },
    {
      "id": "memory_error",
      "pattern": "Out of memory",
      "message": "MATLAB out of memory",
      "cause": "Reachable set too large",
      "solutions": [
        {
          "approach": "Use zonotope",
          "code": "reachMethod = 'approx-zono'",
          "when": "Star sets too expensive"
        },
        {
          "approach": "Increase Java heap",
          "code": "java.opts: -Xmx16g",
          "when": "Need more memory"
        },
        {
          "approach": "Reduce precision",
          "code": "% Use single precision",
          "when": "Double precision too expensive"
        }
      ]
    },
    {
      "id": "layer_error",
      "pattern": "layer",
      "message": "Unsupported layer type",
      "cause": "Layer not implemented in NNV",
      "solutions": [
        {
          "approach": "Check supported layers",
          "code": "% ReLU, sigmoid, tanh, linear, conv supported",
          "when": "Using exotic layer"
        },
        {
          "approach": "Approximate layer",
          "code": "% Replace with supported equivalent",
          "when": "Can approximate"
        }
      ]
    },
    {
      "id": "star_error",
      "pattern": "Star",
      "message": "Star set computation error",
      "cause": "Error in star set operations",
      "solutions": [
        {
          "approach": "Check basis",
          "code": "% Verify star basis is valid",
          "when": "Invalid star"
        },
        {
          "approach": "Check constraints",
          "code": "% Verify predicate constraints",
          "when": "Infeasible star"
        },
        {
          "approach": "Use zonotope",
          "code": "% Fall back to zonotope reachability",
          "when": "Star operations failing"
        }
      ]
    },
    {
      "id": "verification_error",
      "pattern": "verification",
      "message": "Verification failed",
      "cause": "Could not verify property",
      "solutions": [
        {
          "approach": "Check specification",
          "code": "% Verify output set specification",
          "when": "Spec error"
        },
        {
          "approach": "Use exact method",
          "code": "reachMethod = 'exact-star'",
          "when": "Approximation too loose"
        },
        {
          "approach": "Reduce epsilon",
          "code": "% Smaller perturbation",
          "when": "Property hard to verify"
        }
      ]
    },
    {
      "id": "import_error",
      "pattern": "import",
      "message": "Network import error",
      "cause": "Cannot import neural network",
      "solutions": [
        {
          "approach": "Check format",
          "code": "net = importNetworkFromONNX('model.onnx')",
          "when": "Using wrong import function"
        },
        {
          "approach": "Use NNV format",
          "code": "net = Load('model.mat')",
          "when": "Have NNV format"
        },
        {
          "approach": "Convert manually",
          "code": "% Build network layer by layer",
          "when": "Import failing"
        }
      ]
    },
    {
      "id": "lp_error",
      "pattern": "LP",
      "message": "Linear programming error",
      "cause": "LP solver failed",
      "solutions": [
        {
          "approach": "Check solver",
          "code": "% Verify LP solver is installed",
          "when": "Solver not available"
        },
        {
          "approach": "Check feasibility",
          "code": "% Constraints may be infeasible",
          "when": "Infeasible LP"
        }
      ]
    },
    {
      "id": "imagestar_error",
      "pattern": "ImageStar",
      "message": "ImageStar error",
      "cause": "Error in convolutional layer analysis",
      "solutions": [
        {
          "approach": "Check image dimensions",
          "code": "% Verify input shape matches network",
          "when": "Dimension mismatch"
        },
        {
          "approach": "Check channel order",
          "code": "% MATLAB uses HWCN format",
          "when": "Wrong tensor format"
        }
      ]
    }
  ]
}
