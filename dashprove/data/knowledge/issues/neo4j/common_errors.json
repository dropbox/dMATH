{
  "tool": "neo4j",
  "category": "database",
  "common_errors": [
    {
      "pattern": "authentication failure",
      "meaning": "Neo4j authentication failed",
      "causes": ["Wrong username/password", "Password expired", "Auth not configured"],
      "solutions": ["Verify credentials", "Reset password via :server user add", "Check dbms.security settings"]
    },
    {
      "pattern": "unable to connect",
      "meaning": "Cannot connect to Neo4j",
      "causes": ["Server not running", "Wrong port", "Bolt connector disabled"],
      "solutions": ["Start Neo4j", "Check port (7687 for Bolt)", "Enable Bolt connector"]
    },
    {
      "pattern": "OutOfMemoryError",
      "meaning": "JVM heap exhausted",
      "causes": ["Query too memory-intensive", "Heap too small", "Large transaction"],
      "solutions": ["Increase dbms.memory.heap.max_size", "Add LIMIT to queries", "Batch large operations"]
    },
    {
      "pattern": "ConstraintViolation",
      "meaning": "Unique constraint or node key violated",
      "causes": ["Duplicate property value", "Concurrent creates", "Merge race condition"],
      "solutions": ["Use MERGE instead of CREATE", "Handle exception and retry", "Check existing data"]
    },
    {
      "pattern": "DeadlockDetected",
      "meaning": "Transaction deadlock detected",
      "causes": ["Concurrent transactions locking nodes in different order"],
      "solutions": ["Retry transaction", "Lock nodes in consistent order", "Reduce transaction scope"]
    },
    {
      "pattern": "TransientError.*Leader",
      "meaning": "Cluster leadership change during query",
      "causes": ["Leader election in progress", "Network partition", "Leader overloaded"],
      "solutions": ["Retry transaction", "Check cluster health", "Route reads to followers"]
    },
    {
      "pattern": "store.*locked",
      "meaning": "Database store locked by another process",
      "causes": ["Multiple Neo4j processes", "Unclean shutdown", "Lock file stuck"],
      "solutions": ["Stop other Neo4j instances", "Delete lock file after clean shutdown", "Check for zombie processes"]
    },
    {
      "pattern": "Index.*failed.*populate",
      "meaning": "Index creation failed",
      "causes": ["Duplicate values for unique index", "Out of memory", "Corruption"],
      "solutions": ["Check for duplicates first", "Increase memory", "Drop and recreate index"]
    },
    {
      "pattern": "procedure.*not found",
      "meaning": "Stored procedure doesn't exist",
      "causes": ["Plugin not installed", "Wrong procedure name", "Version mismatch"],
      "solutions": ["Install APOC or required plugin", "Check procedure name: CALL dbms.procedures()", "Match plugin to Neo4j version"]
    },
    {
      "pattern": "Cartesian product",
      "meaning": "Query creates unintended Cartesian product",
      "causes": ["Missing relationship in MATCH", "Disconnected patterns", "Query design issue"],
      "solutions": ["Add connecting relationship", "Use OPTIONAL MATCH", "Review query with EXPLAIN"]
    }
  ],
  "debugging_commands": [
    "CALL dbms.listConfig();",
    "CALL dbms.queryJmx('org.neo4j:*');",
    "EXPLAIN <query>;",
    "PROFILE <query>;",
    ":sysinfo"
  ],
  "documentation_urls": [
    "https://neo4j.com/docs/",
    "https://github.com/neo4j/neo4j"
  ]
}
