{
  "tool": "ray",
  "category": "ml_ops",
  "common_errors": [
    {
      "pattern": "RaySystemError.*raylet.*died",
      "meaning": "Ray worker node crashed",
      "causes": ["Out of memory", "Segmentation fault in native code", "Node hardware failure"],
      "solutions": ["Increase memory limits", "Check for memory leaks in tasks", "Review worker logs before crash"]
    },
    {
      "pattern": "ObjectStoreFullError",
      "meaning": "Ray object store ran out of memory",
      "causes": ["Too many objects pinned", "Large objects not freed", "Object store size too small"],
      "solutions": ["Use ray.put() sparingly", "Delete unused object references", "Increase object_store_memory in ray.init()"]
    },
    {
      "pattern": "RayTaskError.*Task.*failed",
      "meaning": "Remote task execution failed",
      "causes": ["Exception in task code", "Worker process killed", "Dependency not available"],
      "solutions": ["Check task exception with ray.get()", "Review worker logs", "Ensure dependencies in runtime_env"]
    },
    {
      "pattern": "cannot schedule task.*no available resources",
      "meaning": "Insufficient cluster resources for task",
      "causes": ["Resource request exceeds available", "Resources held by other tasks", "Cluster autoscaling delayed"],
      "solutions": ["Reduce task resource requirements", "Wait for other tasks to complete", "Configure autoscaler min_workers"]
    },
    {
      "pattern": "RayActorError.*actor.*died",
      "meaning": "Ray actor process terminated unexpectedly",
      "causes": ["Unhandled exception in actor", "Actor node failed", "Out of memory"],
      "solutions": ["Add exception handling in actor methods", "Set max_restarts for fault tolerance", "Increase actor memory"]
    },
    {
      "pattern": "ray.init.*connection refused",
      "meaning": "Cannot connect to Ray cluster",
      "causes": ["Ray head node not running", "Wrong address specified", "Firewall blocking connection"],
      "solutions": ["Start Ray head: ray start --head", "Verify RAY_ADDRESS environment variable", "Check network/firewall rules"]
    },
    {
      "pattern": "PlacementGroupSchedulingError",
      "meaning": "Cannot place resources according to strategy",
      "causes": ["Insufficient resources for bundle", "Conflicting placement constraints", "Cluster too small"],
      "solutions": ["Reduce bundle resource requirements", "Use SPREAD instead of STRICT_SPREAD", "Add more nodes to cluster"]
    },
    {
      "pattern": "ray.tune.*trial.*ERROR",
      "meaning": "Ray Tune hyperparameter trial failed",
      "causes": ["Training function raised exception", "Resource exhaustion", "Invalid hyperparameter combination"],
      "solutions": ["Check trial error in Tune results", "Add error handling in trainable", "Constrain hyperparameter search space"]
    },
    {
      "pattern": "ServeDeploymentException",
      "meaning": "Ray Serve deployment failed",
      "causes": ["Invalid deployment configuration", "Model loading error", "Health check failed"],
      "solutions": ["Validate deployment config", "Check model initialization code", "Review serve logs"]
    },
    {
      "pattern": "RuntimeEnvSetupError",
      "meaning": "Failed to set up task/actor runtime environment",
      "causes": ["pip install failed", "Working directory not found", "Container image pull failed"],
      "solutions": ["Check pip requirements are valid", "Verify working_dir exists", "Ensure container image is accessible"]
    }
  ],
  "debugging_commands": [
    "ray status",
    "ray logs",
    "ray memory",
    "ray timeline",
    "ray dashboard"
  ],
  "documentation_urls": [
    "https://docs.ray.io/",
    "https://github.com/ray-project/ray"
  ]
}
