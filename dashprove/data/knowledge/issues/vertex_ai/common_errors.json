{
  "tool": "vertex_ai",
  "category": "ml_ops",
  "common_errors": [
    {
      "pattern": "PERMISSION_DENIED.*aiplatform",
      "meaning": "Missing Vertex AI API permissions",
      "causes": ["Service account lacks roles", "API not enabled", "Project access denied"],
      "solutions": ["Grant roles/aiplatform.user role", "Enable Vertex AI API in console", "Verify project access"]
    },
    {
      "pattern": "INVALID_ARGUMENT.*container",
      "meaning": "Invalid training container configuration",
      "causes": ["Image URI invalid", "Container not in allowed list", "Wrong accelerator config"],
      "solutions": ["Use pre-built or custom container", "Verify image exists in GCR/AR", "Check accelerator compatibility"]
    },
    {
      "pattern": "RESOURCE_EXHAUSTED",
      "meaning": "Quota or resource limit exceeded",
      "causes": ["GPU quota exceeded", "Too many concurrent jobs", "Region capacity"],
      "solutions": ["Request quota increase", "Wait for jobs to complete", "Try different region"]
    },
    {
      "pattern": "FAILED_PRECONDITION.*training",
      "meaning": "Training job prerequisites not met",
      "causes": ["Dataset not ready", "Model not found", "Required fields missing"],
      "solutions": ["Verify dataset is imported", "Check model exists", "Provide all required parameters"]
    },
    {
      "pattern": "CustomJob.*failed",
      "meaning": "Custom training job failed",
      "causes": ["Training code error", "Resource allocation failed", "Dependency error"],
      "solutions": ["Check job logs in console", "Verify container has all deps", "Test locally first"]
    },
    {
      "pattern": "Endpoint.*deployment.*failed",
      "meaning": "Model deployment to endpoint failed",
      "causes": ["Model format unsupported", "Container startup failed", "Health check failed"],
      "solutions": ["Verify model format (SavedModel, etc)", "Check container logs", "Increase startup timeout"]
    },
    {
      "pattern": "Pipeline.*run.*FAILED",
      "meaning": "Vertex Pipelines run failed",
      "causes": ["Component execution error", "Invalid pipeline spec", "Artifact storage issue"],
      "solutions": ["Check component logs", "Validate pipeline YAML", "Verify GCS bucket access"]
    },
    {
      "pattern": "Dataset.*import.*failed",
      "meaning": "Dataset import operation failed",
      "causes": ["Data format mismatch", "GCS path invalid", "Schema error"],
      "solutions": ["Verify data format matches dataset type", "Check GCS path exists", "Review schema definition"]
    },
    {
      "pattern": "Model.*evaluation.*failed",
      "meaning": "Model evaluation job failed",
      "causes": ["Evaluation dataset invalid", "Model not compatible", "Evaluation config error"],
      "solutions": ["Verify evaluation dataset format", "Check model supports evaluation", "Review evaluation parameters"]
    },
    {
      "pattern": "AutoML.*training.*terminated",
      "meaning": "AutoML training stopped unexpectedly",
      "causes": ["Budget exhausted", "No valid model found", "Data quality issues"],
      "solutions": ["Increase budget", "Review training logs", "Improve data quality"]
    }
  ],
  "debugging_commands": [
    "gcloud ai custom-jobs list",
    "gcloud ai custom-jobs describe <job-id>",
    "gcloud ai custom-jobs stream-logs <job-id>",
    "gcloud ai endpoints list",
    "gcloud ai models list"
  ],
  "documentation_urls": [
    "https://cloud.google.com/vertex-ai/docs",
    "https://github.com/googleapis/python-aiplatform"
  ]
}
