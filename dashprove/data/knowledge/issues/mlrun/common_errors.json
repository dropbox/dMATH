{
  "tool": "mlrun",
  "category": "ml_ops",
  "common_errors": [
    {
      "pattern": "MLRunInvalidArgumentError",
      "meaning": "Invalid function argument or configuration",
      "causes": ["Wrong parameter type", "Missing required parameter", "Invalid handler signature"],
      "solutions": ["Check function signature matches expected types", "Provide all required parameters", "Verify handler function exists in code"]
    },
    {
      "pattern": "MLRunRuntimeError.*function.*not found",
      "meaning": "Function not registered in project",
      "causes": ["Function not saved to project", "Project name mismatch", "Function deleted"],
      "solutions": ["Run project.set_function() to register", "Verify project name is correct", "Re-create and save function"]
    },
    {
      "pattern": "failed to build.*image",
      "meaning": "Function image build failure",
      "causes": ["Dockerfile syntax error", "Base image not found", "Registry authentication failed"],
      "solutions": ["Check Dockerfile/requirements.txt syntax", "Verify base image exists", "Configure registry credentials"]
    },
    {
      "pattern": "artifact.*store.*connection",
      "meaning": "Cannot connect to artifact store",
      "causes": ["S3/V3IO credentials missing", "Endpoint URL wrong", "Network connectivity issue"],
      "solutions": ["Set artifact path credentials", "Verify endpoint URL configuration", "Check network access to store"]
    },
    {
      "pattern": "run.*state.*error",
      "meaning": "Job run failed during execution",
      "causes": ["Code exception", "Resource limit exceeded", "Dependency import error"],
      "solutions": ["Check run logs for stack trace", "Increase resource limits", "Verify all dependencies in requirements"]
    },
    {
      "pattern": "project.*sync.*failed",
      "meaning": "Project sync with Git failed",
      "causes": ["Git credentials invalid", "Repository not found", "Branch doesn't exist"],
      "solutions": ["Update Git credentials/token", "Verify repository URL", "Check branch name exists"]
    },
    {
      "pattern": "serving.*endpoint.*not ready",
      "meaning": "Model serving endpoint failed to deploy",
      "causes": ["Model file not found", "Nuclio build failed", "Resource allocation error"],
      "solutions": ["Verify model artifact exists", "Check Nuclio function logs", "Adjust resource configuration"]
    },
    {
      "pattern": "feature.*store.*schema.*mismatch",
      "meaning": "Feature store schema doesn't match data",
      "causes": ["Column types changed", "Missing columns", "Extra unexpected columns"],
      "solutions": ["Update feature set schema", "Ensure data matches expected schema", "Re-ingest with correct schema"]
    },
    {
      "pattern": "workflow.*step.*failed",
      "meaning": "Pipeline workflow step execution failed",
      "causes": ["Step dependency failed", "Invalid step configuration", "Runtime exception in step"],
      "solutions": ["Check upstream step status", "Validate step parameters", "Review step logs for errors"]
    },
    {
      "pattern": "MLRunHTTPError.*401",
      "meaning": "Authentication failure with MLRun API",
      "causes": ["Token expired", "Invalid credentials", "API key not set"],
      "solutions": ["Refresh authentication token", "Set MLRUN_DBPATH with credentials", "Configure API key in environment"]
    }
  ],
  "debugging_commands": [
    "mlrun project -n <project> -w",
    "mlrun logs <run-uid>",
    "mlrun get runs --project <project>",
    "mlrun get functions --project <project>",
    "kubectl logs -n mlrun <pod-name>"
  ],
  "documentation_urls": [
    "https://docs.mlrun.org/",
    "https://github.com/mlrun/mlrun"
  ]
}
