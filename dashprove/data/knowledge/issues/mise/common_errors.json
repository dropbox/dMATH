{
  "tool": "mise",
  "category": "version_manager",
  "common_errors": [
    {
      "pattern": "No version set for.*in",
      "cause": "No version specified in .mise.toml or global config",
      "solutions": [
        "Run 'mise use <tool>@<version>'",
        "Add to .mise.toml: [tools]\\nnode = \"20\"",
        "Run 'mise use --global <tool>@<version>'"
      ]
    },
    {
      "pattern": "mise: command not found",
      "cause": "mise not installed or not activated",
      "solutions": [
        "Add to shell profile: eval \"$(mise activate bash)\"",
        "Install: curl https://mise.run | sh",
        "Restart shell after installation"
      ]
    },
    {
      "pattern": "not installed.*run.*mise install",
      "cause": "Version specified but not installed",
      "solutions": [
        "Run 'mise install'",
        "Run 'mise install <tool>@<version>'",
        "Check available versions: 'mise ls-remote <tool>'"
      ]
    },
    {
      "pattern": "Could not find a version",
      "cause": "Requested version doesn't exist",
      "solutions": [
        "List versions: 'mise ls-remote <tool>'",
        "Use fuzzy version: node@20 instead of exact",
        "Check for typos in version string"
      ]
    },
    {
      "pattern": "Error building.*from source",
      "cause": "Missing build dependencies",
      "solutions": [
        "Install build tools (gcc, make, etc.)",
        "Check mise docs for tool-specific deps",
        "Try prebuilt binary: mise settings set experimental true"
      ]
    },
    {
      "pattern": "trust.*file.*untrusted",
      "cause": ".mise.toml not trusted for directory",
      "solutions": [
        "Run 'mise trust' in the directory",
        "Run 'mise trust --all'",
        "Check trust policy: 'mise settings'"
      ]
    },
    {
      "pattern": "settings.*invalid",
      "cause": "Invalid configuration in config file",
      "solutions": [
        "Check ~/.config/mise/config.toml syntax",
        "Validate with 'mise doctor'",
        "Reset settings: remove config file"
      ]
    },
    {
      "pattern": "shim.*not found",
      "cause": "Shim not created for command",
      "solutions": [
        "Run 'mise reshim'",
        "Ensure tool version is installed",
        "Check PATH includes mise shims"
      ]
    },
    {
      "pattern": "cannot parse version",
      "cause": "Invalid version format",
      "solutions": [
        "Use format: tool@version or tool@prefix",
        "Check tool's version format",
        "Escape special characters if needed"
      ]
    },
    {
      "pattern": "env.*MISE_.*invalid",
      "cause": "Invalid environment variable",
      "solutions": [
        "Check MISE_* env vars format",
        "Review mise env documentation",
        "Unset conflicting variables"
      ]
    }
  ]
}
