{
  "tool": "whereis",
  "version": "2.38",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "no_results",
      "pattern": "no.*found|empty result|nothing returned",
      "message": "Command not found",
      "cause": "Command not in standard locations or not installed",
      "solutions": [
        {
          "approach": "Use which",
          "code": "which command  # Check PATH\ntype -a command  # All locations",
          "when": "Check PATH"
        },
        {
          "approach": "Use find",
          "code": "sudo find / -name 'command*' -type f 2>/dev/null",
          "when": "Full search"
        },
        {
          "approach": "Install command",
          "code": "# Check if installed:\napt list --installed | grep command\nbrew list | grep command",
          "when": "Not installed"
        }
      ]
    },
    {
      "id": "binary_only",
      "pattern": "binary|no source|no manpage|-b",
      "message": "Want only binary location",
      "cause": "Default shows binary, source, and man pages",
      "solutions": [
        {
          "approach": "Binary only",
          "code": "whereis -b command",
          "when": "Just executable"
        },
        {
          "approach": "Manual only",
          "code": "whereis -m command",
          "when": "Just man pages"
        },
        {
          "approach": "Source only",
          "code": "whereis -s command",
          "when": "Just source"
        }
      ]
    },
    {
      "id": "limited_search",
      "pattern": "not searching|limited|specific directories",
      "message": "whereis searches limited directories",
      "cause": "whereis only searches standard system directories",
      "solutions": [
        {
          "approach": "Add search path",
          "code": "whereis -B /custom/bin -f command",
          "when": "Custom binary dir"
        },
        {
          "approach": "Add man path",
          "code": "whereis -M /custom/man -f command",
          "when": "Custom man dir"
        },
        {
          "approach": "Use which",
          "code": "which command  # Searches all of PATH",
          "when": "Use PATH"
        }
      ]
    },
    {
      "id": "multiple_commands",
      "pattern": "multiple|several|batch",
      "message": "Check multiple commands at once",
      "cause": "Want to check multiple commands",
      "solutions": [
        {
          "approach": "Multiple args",
          "code": "whereis gcc python make",
          "when": "List commands"
        },
        {
          "approach": "Loop",
          "code": "for cmd in gcc python make; do whereis $cmd; done",
          "when": "With processing"
        }
      ]
    },
    {
      "id": "unusual_output",
      "pattern": "output format|parse|script",
      "message": "Output format for scripting",
      "cause": "Output format varies by implementation",
      "solutions": [
        {
          "approach": "Parse output",
          "code": "whereis gcc | cut -d: -f2 | tr -s ' '",
          "when": "Extract paths"
        },
        {
          "approach": "Use which",
          "code": "which gcc  # Simpler output",
          "when": "Single path needed"
        },
        {
          "approach": "Use type",
          "code": "type -p gcc  # Just path",
          "when": "Script friendly"
        }
      ]
    }
  ]
}
