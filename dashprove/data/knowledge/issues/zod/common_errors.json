{
  "tool": "zod",
  "category": "data_validation",
  "common_errors": [
    {
      "error_pattern": "ZodError.*Required",
      "description": "Required field missing",
      "causes": [
        "Field not provided in input",
        "Undefined value for required field"
      ],
      "solutions": [
        "Provide required field",
        "Use .optional() for optional fields",
        "Use .nullable() if null should be allowed"
      ]
    },
    {
      "error_pattern": "Expected.*received",
      "description": "Type mismatch in validation",
      "causes": [
        "Wrong type passed to validator",
        "String instead of number",
        "Array instead of object"
      ],
      "solutions": [
        "Provide correct type",
        "Use .coerce.number() for string to number",
        "Check input data structure"
      ]
    },
    {
      "error_pattern": "String must contain at least",
      "description": "String too short",
      "causes": [
        "Input shorter than .min()",
        "Empty string provided"
      ],
      "solutions": [
        "Provide longer string",
        "Use .default('') for empty default",
        "Adjust .min() constraint"
      ]
    },
    {
      "error_pattern": "String must contain at most",
      "description": "String too long",
      "causes": [
        "Input exceeds .max()",
        "Unexpected extra characters"
      ],
      "solutions": [
        "Truncate input string",
        "Increase .max() limit",
        "Use .trim() to remove whitespace"
      ]
    },
    {
      "error_pattern": "Invalid email",
      "description": "Email format validation failed",
      "causes": [
        "Email doesn't match format",
        "Missing @ or domain"
      ],
      "solutions": [
        "Provide valid email address",
        "Use z.string().email() for validation",
        "Custom regex with .regex() if needed"
      ]
    },
    {
      "error_pattern": "Invalid url",
      "description": "URL format invalid",
      "causes": [
        "Missing protocol",
        "Invalid URL format"
      ],
      "solutions": [
        "Include http:// or https://",
        "Use z.string().url() for validation",
        "Custom validation if needed"
      ]
    },
    {
      "error_pattern": "Invalid enum value",
      "description": "Value not in enum",
      "causes": [
        "Input not in z.enum() values",
        "Case sensitivity issue"
      ],
      "solutions": [
        "Use one of the enum values",
        "Check z.enum(['a', 'b']) values",
        "Use .toLowerCase() before validation"
      ]
    },
    {
      "error_pattern": "Unrecognized key",
      "description": "Extra field in strict object",
      "causes": [
        "Field not in schema",
        "Using .strict() mode"
      ],
      "solutions": [
        "Add field to schema",
        "Use .passthrough() to allow extra keys",
        "Use .strip() to remove unknown keys"
      ]
    },
    {
      "error_pattern": "Invalid literal value",
      "description": "Value doesn't match literal",
      "causes": [
        "Input doesn't match z.literal()",
        "Type or value mismatch"
      ],
      "solutions": [
        "Use exact literal value",
        "Check z.literal() definition",
        "Use z.enum() for multiple values"
      ]
    },
    {
      "error_pattern": "Invalid discriminator value",
      "description": "Discriminated union key mismatch",
      "causes": [
        "Discriminator field missing",
        "Value not matching any variant"
      ],
      "solutions": [
        "Include discriminator field",
        "Use correct discriminator value",
        "Check z.discriminatedUnion() variants"
      ]
    },
    {
      "error_pattern": "Array must contain at least",
      "description": "Array too short",
      "causes": [
        "Array has fewer items than .min()",
        "Empty array provided"
      ],
      "solutions": [
        "Add more items to array",
        "Use .nonempty() for non-empty arrays",
        "Adjust .min() constraint"
      ]
    },
    {
      "error_pattern": "Invalid union.*none.*succeeded",
      "description": "No union variant matched",
      "causes": [
        "Input doesn't match any z.union() variant",
        "All variants failed validation"
      ],
      "solutions": [
        "Ensure input matches at least one variant",
        "Check union variant definitions",
        "Use discriminatedUnion for better errors"
      ]
    },
    {
      "error_pattern": "refine.*function.*boolean",
      "description": "Refine function error",
      "causes": [
        ".refine() didn't return boolean",
        "Async issue with sync parse"
      ],
      "solutions": [
        "Return true/false from refine function",
        "Use .parseAsync() for async refinements",
        "Check refinement logic"
      ]
    }
  ]
}
