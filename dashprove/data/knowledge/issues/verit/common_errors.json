{
  "tool": "verit",
  "version": "2.x",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "timeout",
      "pattern": "timeout",
      "message": "Solver timed out",
      "cause": "veriT could not solve within time limit",
      "solutions": [
        {
          "approach": "Increase timeout",
          "code": "veriT --max-time=600 file.smt2",
          "when": "Problem may be solvable with more time"
        },
        {
          "approach": "Enable preprocessing",
          "code": "veriT --proof-prune file.smt2",
          "when": "Preprocessing can help"
        },
        {
          "approach": "Try different strategies",
          "code": "veriT --ccfv-breadth file.smt2",
          "when": "Default strategy ineffective"
        }
      ]
    },
    {
      "id": "unsupported",
      "pattern": "unsupported",
      "message": "Unsupported feature or logic",
      "cause": "veriT doesn't support the requested feature",
      "solutions": [
        {
          "approach": "Check supported logics",
          "code": "; veriT supports QF_UF, QF_LIA, QF_LRA, etc.",
          "when": "Using unsupported logic"
        },
        {
          "approach": "Avoid unsupported features",
          "code": "; Check veriT documentation for features",
          "when": "Using unsupported SMT-LIB feature"
        }
      ]
    },
    {
      "id": "parse_error",
      "pattern": "parse error",
      "message": "Parse error in input",
      "cause": "SMT-LIB syntax error",
      "solutions": [
        {
          "approach": "Check syntax",
          "code": "(assert (= x 5))",
          "when": "Syntax error"
        },
        {
          "approach": "Validate with other solver",
          "code": "z3 -smt2 file.smt2  ; check syntax",
          "when": "Unsure if syntax correct"
        }
      ]
    },
    {
      "id": "proof_generation",
      "pattern": "proof",
      "message": "Proof generation failed",
      "cause": "Cannot generate proof output",
      "solutions": [
        {
          "approach": "Enable proof output",
          "code": "veriT --proof=file.proof file.smt2",
          "when": "Proof output requested"
        },
        {
          "approach": "Use Alethe format",
          "code": "veriT --proof-format-and-target=alethe file.smt2",
          "when": "Need specific proof format"
        },
        {
          "approach": "Check UNSAT",
          "code": "; Proofs only for UNSAT results",
          "when": "Result was SAT"
        }
      ]
    },
    {
      "id": "quantifier_handling",
      "pattern": "quantifier",
      "message": "Quantifier instantiation issues",
      "cause": "Problems with quantifier reasoning",
      "solutions": [
        {
          "approach": "Add triggers",
          "code": "(forall ((x Int)) (! (P x) :pattern ((f x))))",
          "when": "Need to guide instantiation"
        },
        {
          "approach": "Use CCFV",
          "code": "veriT --ccfv-index file.smt2",
          "when": "Conflict-driven instantiation"
        },
        {
          "approach": "Bound instantiation",
          "code": "veriT --max-inst=10000 file.smt2",
          "when": "Too many instances"
        }
      ]
    },
    {
      "id": "type_error",
      "pattern": "type",
      "message": "Type error in formula",
      "cause": "Sort mismatch in expression",
      "solutions": [
        {
          "approach": "Check sorts",
          "code": "(declare-fun x () Int)",
          "when": "Variable has wrong sort"
        },
        {
          "approach": "Verify function application",
          "code": "; Check parameter and return sorts",
          "when": "Function applied incorrectly"
        }
      ]
    },
    {
      "id": "certificate_validation",
      "pattern": "certificate",
      "message": "Proof certificate invalid",
      "cause": "Generated proof fails validation",
      "solutions": [
        {
          "approach": "Use different proof format",
          "code": "veriT --proof-format-and-target=lfsc file.smt2",
          "when": "Alethe proof issues"
        },
        {
          "approach": "Check proof version",
          "code": "; Ensure validator matches veriT version",
          "when": "Validator incompatibility"
        }
      ]
    },
    {
      "id": "lemma_learning",
      "pattern": "lemma",
      "message": "Lemma learning issues",
      "cause": "Theory lemma generation problems",
      "solutions": [
        {
          "approach": "Adjust lemma bounds",
          "code": "veriT --max-lemmas=100000 file.smt2",
          "when": "Lemma limit reached"
        },
        {
          "approach": "Simplify theory terms",
          "code": "; Reduce complexity of arithmetic",
          "when": "Too many theory conflicts"
        }
      ]
    }
  ]
}
