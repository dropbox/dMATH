{
  "tool": "jenv",
  "category": "version_manager",
  "common_errors": [
    {
      "pattern": "jenv: command not found",
      "cause": "jenv not in PATH or not initialized",
      "solutions": [
        "Add to PATH: export PATH=\"$HOME/.jenv/bin:$PATH\"",
        "Init: eval \"$(jenv init -)\"",
        "Install: brew install jenv (macOS)"
      ]
    },
    {
      "pattern": "No JAVA_HOME set",
      "cause": "JAVA_HOME not exported by jenv",
      "solutions": [
        "Enable export: jenv enable-plugin export",
        "Restart shell after enabling plugin",
        "Verify: echo $JAVA_HOME"
      ]
    },
    {
      "pattern": "JDK.*not found",
      "cause": "JDK not registered with jenv",
      "solutions": [
        "Add JDK: jenv add /path/to/jdk",
        "List added: jenv versions",
        "macOS: jenv add /Library/Java/JavaVirtualMachines/*/Contents/Home"
      ]
    },
    {
      "pattern": "version.*not available",
      "cause": "Version alias not registered",
      "solutions": [
        "List available: jenv versions",
        "Add JDK first: jenv add /path/to/jdk",
        "Check version format (e.g., 21.0)"
      ]
    },
    {
      "pattern": "jenv:.*shims.*not in PATH",
      "cause": "jenv shims not first in PATH",
      "solutions": [
        "Ensure eval \"$(jenv init -)\" is in profile",
        "Check PATH order",
        "Verify with 'which java'"
      ]
    },
    {
      "pattern": "rehash failed",
      "cause": "Cannot create shims for installed JDK",
      "solutions": [
        "Check ~/.jenv/shims permissions",
        "Manually rehash: jenv rehash",
        "Verify JDK paths are correct"
      ]
    },
    {
      "pattern": "plugin.*not found",
      "cause": "jenv plugin not available",
      "solutions": [
        "List plugins: ls ~/.jenv/plugins/",
        "Install plugins from jenv repo",
        "Common plugins: export, maven, gradle"
      ]
    },
    {
      "pattern": "Unable to.*set.*version",
      "cause": "JDK not properly added or removed",
      "solutions": [
        "Re-add JDK: jenv add /path",
        "Check JDK exists at path",
        "Remove and re-add if corrupted"
      ]
    },
    {
      "pattern": "global.*not configured",
      "cause": "No global Java version set",
      "solutions": [
        "Set global: jenv global <version>",
        "Set local: jenv local <version>",
        "List versions: jenv versions"
      ]
    },
    {
      "pattern": "Cannot find.*Contents/Home",
      "cause": "Invalid JDK path format",
      "solutions": [
        "macOS: use .../Contents/Home path",
        "Linux: use direct JDK directory",
        "Verify path contains bin/java"
      ]
    }
  ]
}
