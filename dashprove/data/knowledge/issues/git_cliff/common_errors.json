{
  "tool": "git_cliff",
  "category": "git_conventions",
  "common_errors": [
    {
      "pattern": "config file.*not found",
      "cause": "cliff.toml not in repository",
      "solutions": [
        "Create cliff.toml: git cliff --init",
        "Specify path: --config path/to/cliff.toml",
        "Use built-in preset: --preset conventional"
      ]
    },
    {
      "pattern": "no commits found",
      "cause": "No commits matching configuration",
      "solutions": [
        "Check commit_parsers regex patterns",
        "Verify tag range is correct",
        "Use --unreleased for untagged commits"
      ]
    },
    {
      "pattern": "invalid regex.*commit_parsers",
      "cause": "Regex syntax error in config",
      "solutions": [
        "Validate regex syntax",
        "Escape special characters",
        "Test regex with sample commit messages"
      ]
    },
    {
      "pattern": "tag.*not found",
      "cause": "Specified tag doesn't exist",
      "solutions": [
        "Check tag name: git tag -l",
        "Use commit SHA instead of tag",
        "Create tag first if needed"
      ]
    },
    {
      "pattern": "template.*parse error",
      "cause": "Tera template syntax error",
      "solutions": [
        "Check template syntax (uses Tera)",
        "Validate {{ }} and {% %} blocks",
        "Review available template variables"
      ]
    },
    {
      "pattern": "cannot parse version",
      "cause": "Tag doesn't follow semver format",
      "solutions": [
        "Use --tag-pattern for custom tag format",
        "Ensure tags follow vX.Y.Z format",
        "Configure tag_pattern in cliff.toml"
      ]
    },
    {
      "pattern": "output file.*exists",
      "cause": "Would overwrite existing file",
      "solutions": [
        "Use --prepend to prepend to existing",
        "Remove existing file first",
        "Use different output path"
      ]
    },
    {
      "pattern": "git command failed",
      "cause": "Git command execution error",
      "solutions": [
        "Ensure running in git repository",
        "Check git is installed and in PATH",
        "Verify repository is not shallow"
      ]
    },
    {
      "pattern": "unknown group",
      "cause": "Commit group not defined in config",
      "solutions": [
        "Add group to commit_parsers",
        "Use filter_commits to exclude",
        "Check default_group setting"
      ]
    },
    {
      "pattern": "remote.*not configured",
      "cause": "Remote URL needed for links",
      "solutions": [
        "Add git remote: git remote add origin",
        "Set postprocessors manually in config",
        "Use --repository flag"
      ]
    }
  ]
}
