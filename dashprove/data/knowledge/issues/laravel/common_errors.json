{
  "tool": "laravel",
  "category": "web_framework",
  "errors": [
    {
      "pattern": "Class.*not found",
      "cause": "Autoloader cache stale",
      "solutions": [
        "Run composer dump-autoload",
        "Check namespace declaration",
        "Verify class file location"
      ]
    },
    {
      "pattern": "SQLSTATE.*error",
      "cause": "Database query error",
      "solutions": [
        "Check .env database config",
        "Run php artisan migrate",
        "Verify credentials"
      ]
    },
    {
      "pattern": "TokenMismatchException",
      "cause": "CSRF token validation failed",
      "solutions": [
        "Include @csrf in forms",
        "Check session configuration",
        "Verify cookie domain"
      ]
    },
    {
      "pattern": "MethodNotAllowedHttpException",
      "cause": "HTTP method not allowed",
      "solutions": [
        "Check route definition",
        "Use correct HTTP verb",
        "Review form method"
      ]
    },
    {
      "pattern": "ViewNotFound",
      "cause": "Blade template missing",
      "solutions": [
        "Check view path",
        "Verify .blade.php extension",
        "Clear view cache"
      ]
    },
    {
      "pattern": "NotFoundHttpException",
      "cause": "Route not found",
      "solutions": [
        "Check routes/web.php",
        "Run php artisan route:cache",
        "Verify controller method"
      ]
    },
    {
      "pattern": "ModelNotFoundException",
      "cause": "Eloquent model not found",
      "solutions": [
        "Use findOrFail correctly",
        "Check primary key",
        "Handle 404 in controller"
      ]
    },
    {
      "pattern": "Encryption.*failed",
      "cause": "APP_KEY not set",
      "solutions": [
        "Run php artisan key:generate",
        "Check .env APP_KEY",
        "Don't share APP_KEY"
      ]
    },
    {
      "pattern": "BindingResolutionException",
      "cause": "Dependency resolution failed",
      "solutions": [
        "Register binding in provider",
        "Check interface implementations",
        "Verify container bindings"
      ]
    },
    {
      "pattern": "Permission denied",
      "cause": "File permission issue",
      "solutions": [
        "chmod storage and bootstrap/cache",
        "Check ownership",
        "Use proper permissions"
      ]
    }
  ]
}
