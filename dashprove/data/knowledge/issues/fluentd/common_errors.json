{
  "tool": "fluentd",
  "category": "logging",
  "common_errors": [
    {
      "pattern": "config.*error",
      "meaning": "Fluentd configuration syntax error",
      "causes": ["Invalid directive", "Missing required parameter", "Incorrect indentation"],
      "solutions": ["Validate config: fluentd --dry-run", "Check directive syntax", "Review config file format"]
    },
    {
      "pattern": "plugin.*not found",
      "meaning": "Fluentd plugin not installed",
      "causes": ["Plugin not installed", "Wrong plugin name", "Gem not available"],
      "solutions": ["Install: fluent-gem install <plugin>", "Check plugin name", "Verify gem sources"]
    },
    {
      "pattern": "failed to connect",
      "meaning": "Cannot connect to output destination",
      "causes": ["Destination down", "Wrong host/port", "Auth failure"],
      "solutions": ["Check destination service", "Verify host and port", "Check credentials"]
    },
    {
      "pattern": "buffer.*overflow",
      "meaning": "Buffer queue full",
      "causes": ["Output slower than input", "Destination unavailable", "Buffer too small"],
      "solutions": ["Increase buffer size", "Check output destination", "Enable buffer overflow handling"]
    },
    {
      "pattern": "retry.*exceeded",
      "meaning": "Output retry limit reached",
      "causes": ["Destination consistently failing", "Network issue", "Auth problem"],
      "solutions": ["Check destination availability", "Increase retry limit", "Fix auth configuration"]
    },
    {
      "pattern": "memory.*usage high",
      "meaning": "Fluentd using excessive memory",
      "causes": ["Large buffer in memory", "Memory leak", "High event rate"],
      "solutions": ["Use file buffer instead of memory", "Update plugins", "Increase buffer flush interval"]
    },
    {
      "pattern": "parse.*error",
      "meaning": "Cannot parse incoming data",
      "causes": ["Wrong parser type", "Malformed data", "Encoding issue"],
      "solutions": ["Set correct parser", "Fix input data format", "Configure encoding"]
    },
    {
      "pattern": "permission denied",
      "meaning": "Fluentd lacks file/resource permissions",
      "causes": ["Wrong user", "File permissions", "SELinux"],
      "solutions": ["Run as correct user", "Fix file permissions", "Configure SELinux policy"]
    },
    {
      "pattern": "address already in use",
      "meaning": "Port already bound",
      "causes": ["Another Fluentd running", "Port conflict", "Not properly shut down"],
      "solutions": ["Stop other instance", "Use different port", "Kill orphan process"]
    },
    {
      "pattern": "chunk.*corrupted",
      "meaning": "Buffer chunk file corrupted",
      "causes": ["Disk failure", "Improper shutdown", "Storage issue"],
      "solutions": ["Delete corrupted chunks", "Check disk health", "Use queued_chunks_limit_size"]
    }
  ],
  "debugging_commands": [
    "fluentd --dry-run -c <config>",
    "fluentd -v",
    "fluent-gem list",
    "fluent-cat <tag> < test.json"
  ],
  "documentation_urls": [
    "https://docs.fluentd.org/",
    "https://github.com/fluent/fluentd"
  ]
}
