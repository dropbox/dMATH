{
  "tool": "comet",
  "category": "ml_ops",
  "common_errors": [
    {
      "pattern": "InvalidRestApiKey",
      "meaning": "Comet API key invalid or missing",
      "causes": ["API key not set", "Key expired or revoked", "Key copied incorrectly"],
      "solutions": ["Set COMET_API_KEY environment variable", "Generate new key in Comet UI", "Verify key has no extra whitespace"]
    },
    {
      "pattern": "InvalidWorkspace",
      "meaning": "Workspace doesn't exist or access denied",
      "causes": ["Workspace name typo", "Not member of workspace", "Workspace deleted"],
      "solutions": ["Verify workspace name", "Request workspace access", "Create new workspace"]
    },
    {
      "pattern": "InvalidProjectName",
      "meaning": "Project doesn't exist or invalid name",
      "causes": ["Project name contains invalid characters", "Project not created", "Case sensitivity issue"],
      "solutions": ["Use valid project name (alphanumeric, hyphens)", "Create project in UI first", "Check exact project name case"]
    },
    {
      "pattern": "ExperimentNotFound",
      "meaning": "Referenced experiment doesn't exist",
      "causes": ["Experiment key incorrect", "Experiment deleted", "Wrong project context"],
      "solutions": ["Verify experiment key in Comet UI", "Check experiment wasn't deleted", "Ensure correct workspace/project"]
    },
    {
      "pattern": "ConnectionError.*Comet",
      "meaning": "Cannot connect to Comet servers",
      "causes": ["Network issue", "Comet service down", "Proxy misconfigured"],
      "solutions": ["Check network connectivity", "Verify Comet status page", "Configure proxy settings"]
    },
    {
      "pattern": "AssetUploadFailed",
      "meaning": "Failed to upload artifact/asset",
      "causes": ["File too large", "Network timeout", "Storage quota exceeded"],
      "solutions": ["Compress or chunk large files", "Increase timeout", "Clean up old experiments"]
    },
    {
      "pattern": "MaxMetricDataPointsExceeded",
      "meaning": "Too many metric data points logged",
      "causes": ["Logging every iteration", "Long training runs", "Too many metrics"],
      "solutions": ["Log metrics less frequently", "Use step parameter to batch", "Aggregate metrics"]
    },
    {
      "pattern": "ModelNotFound",
      "meaning": "Model not found in registry",
      "causes": ["Model name incorrect", "Model not registered", "Version doesn't exist"],
      "solutions": ["Verify model name in registry", "Register model first", "Check version exists"]
    },
    {
      "pattern": "OfflineModeError",
      "meaning": "Error in offline experiment mode",
      "causes": ["Offline directory not writable", "Sync failed", "Corrupted offline data"],
      "solutions": ["Check directory permissions", "Retry sync with comet upload", "Clear offline cache"]
    },
    {
      "pattern": "ParameterValueTooLong",
      "meaning": "Logged parameter exceeds length limit",
      "causes": ["Parameter value too long", "Logging large objects as params"],
      "solutions": ["Truncate long values", "Use assets for large data", "Summarize parameter values"]
    }
  ],
  "debugging_commands": [
    "comet check",
    "comet upload <offline_dir>",
    "comet delete-offline-experiment"
  ],
  "documentation_urls": [
    "https://www.comet.com/docs/v2/",
    "https://github.com/comet-ml/comet-examples"
  ]
}
