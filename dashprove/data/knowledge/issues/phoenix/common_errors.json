{
  "tool": "phoenix",
  "category": "web_framework",
  "errors": [
    {
      "pattern": "no route found",
      "cause": "Route not defined",
      "solutions": [
        "Check router.ex",
        "Verify path helpers",
        "Review scope configuration"
      ]
    },
    {
      "pattern": "Ecto.*error",
      "cause": "Database operation failed",
      "solutions": [
        "Check repo configuration",
        "Run migrations",
        "Verify changeset"
      ]
    },
    {
      "pattern": "UndefinedFunctionError",
      "cause": "Function doesn't exist",
      "solutions": [
        "Check module imports",
        "Verify function arity",
        "Review module name"
      ]
    },
    {
      "pattern": "CompileError",
      "cause": "Elixir compilation error",
      "solutions": [
        "Fix syntax errors",
        "Check module structure",
        "Review error location"
      ]
    },
    {
      "pattern": "GenServer.*timeout",
      "cause": "Process timeout",
      "solutions": [
        "Increase timeout",
        "Check process state",
        "Review blocking operations"
      ]
    },
    {
      "pattern": "Phoenix.Socket.*error",
      "cause": "Channel error",
      "solutions": [
        "Check channel join",
        "Verify socket configuration",
        "Review authentication"
      ]
    },
    {
      "pattern": "Plug.Conn.*error",
      "cause": "Connection handling error",
      "solutions": [
        "Check plug pipeline",
        "Verify response sent",
        "Review halt() usage"
      ]
    },
    {
      "pattern": "no such migration",
      "cause": "Migration not found",
      "solutions": [
        "Check migration file exists",
        "Verify migration number",
        "Run mix ecto.create"
      ]
    },
    {
      "pattern": "CSRF token",
      "cause": "CSRF validation failed",
      "solutions": [
        "Include csrf_meta_tag",
        "Check form helper",
        "Verify plug configuration"
      ]
    },
    {
      "pattern": "LiveView.*crashed",
      "cause": "LiveView process error",
      "solutions": [
        "Check mount/3 function",
        "Review handle_event/3",
        "Verify assigns"
      ]
    }
  ]
}
