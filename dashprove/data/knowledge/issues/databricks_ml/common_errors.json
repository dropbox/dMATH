{
  "tool": "databricks_ml",
  "category": "ml_ops",
  "common_errors": [
    {
      "pattern": "PERMISSION_DENIED",
      "meaning": "Insufficient permissions for MLflow/Unity Catalog operation",
      "causes": ["Missing workspace permissions", "Unity Catalog grants missing", "Token scope insufficient"],
      "solutions": ["Grant required workspace permissions", "Add Unity Catalog privileges", "Create token with correct scopes"]
    },
    {
      "pattern": "RESOURCE_DOES_NOT_EXIST",
      "meaning": "Referenced resource not found",
      "causes": ["Experiment/model deleted", "Wrong experiment ID", "Resource in different workspace"],
      "solutions": ["Verify resource exists in UI", "Check experiment/model ID", "Ensure correct workspace context"]
    },
    {
      "pattern": "mlflow.exceptions.MlflowException.*tracking",
      "meaning": "MLflow tracking server error",
      "causes": ["Tracking URI misconfigured", "Server not accessible", "Auth token invalid"],
      "solutions": ["Set MLFLOW_TRACKING_URI correctly", "Check network access to workspace", "Refresh auth token"]
    },
    {
      "pattern": "Model.*registry.*error",
      "meaning": "Model registry operation failed",
      "causes": ["Model version conflict", "Stage transition denied", "Model name invalid"],
      "solutions": ["Check model version state", "Verify transition permissions", "Use valid model name"]
    },
    {
      "pattern": "Serving.*endpoint.*unhealthy",
      "meaning": "Model serving endpoint not healthy",
      "causes": ["Model loading failed", "Container resource exhausted", "Dependency error"],
      "solutions": ["Check endpoint logs", "Increase endpoint resources", "Verify model dependencies"]
    },
    {
      "pattern": "AutoML.*experiment.*failed",
      "meaning": "AutoML run failed",
      "causes": ["Data quality issues", "Target column invalid", "Resource constraints"],
      "solutions": ["Review data quality warnings", "Verify target column", "Increase cluster resources"]
    },
    {
      "pattern": "Feature.*store.*error",
      "meaning": "Feature store operation failed",
      "causes": ["Feature table not found", "Schema mismatch", "Unity Catalog permissions"],
      "solutions": ["Verify feature table exists", "Check schema matches", "Grant feature store permissions"]
    },
    {
      "pattern": "Unity Catalog.*model.*not found",
      "meaning": "Model not found in Unity Catalog",
      "causes": ["Model not registered to UC", "Wrong catalog/schema", "Model deleted"],
      "solutions": ["Register model to Unity Catalog", "Check catalog.schema.model path", "Verify model exists"]
    },
    {
      "pattern": "Run.*artifact.*not found",
      "meaning": "MLflow run artifact missing",
      "causes": ["Artifact not logged", "Storage deleted", "Path incorrect"],
      "solutions": ["Verify artifact was logged", "Check artifact storage", "Use correct artifact path"]
    },
    {
      "pattern": "Cluster.*terminated",
      "meaning": "Cluster terminated during ML operation",
      "causes": ["Spot instance reclaimed", "Idle timeout", "Error/crash"],
      "solutions": ["Use on-demand instances", "Increase idle timeout", "Check cluster event logs"]
    }
  ],
  "debugging_commands": [
    "databricks experiments list",
    "databricks experiments get --experiment-id <id>",
    "mlflow models list",
    "databricks serving-endpoints list"
  ],
  "documentation_urls": [
    "https://docs.databricks.com/machine-learning/",
    "https://docs.databricks.com/mlflow/"
  ]
}
