{
  "tool": "pagerduty",
  "category": "monitoring",
  "common_errors": [
    {
      "pattern": "Invalid Integration Key",
      "meaning": "Events API integration key rejected",
      "causes": ["Wrong integration key", "Key deleted", "Service deactivated"],
      "solutions": ["Get key from service integrations", "Create new integration", "Reactivate service"]
    },
    {
      "pattern": "403.*Forbidden",
      "meaning": "API authorization denied",
      "causes": ["API key lacks permission", "Wrong API key type", "Token expired"],
      "solutions": ["Use key with correct scopes", "Use REST API key not Events API key", "Regenerate token"]
    },
    {
      "pattern": "rate limit exceeded",
      "meaning": "API rate limit hit",
      "causes": ["Too many API calls", "Event storm", "Integration loop"],
      "solutions": ["Implement rate limiting", "Batch events", "Check for alert loops"]
    },
    {
      "pattern": "escalation.*not working",
      "meaning": "Escalation policy not functioning correctly",
      "causes": ["No users on-call", "Schedule gap", "Policy misconfigured"],
      "solutions": ["Verify on-call schedule coverage", "Check escalation policy targets", "Add fallback escalation"]
    },
    {
      "pattern": "notification.*not received",
      "meaning": "User not receiving notifications",
      "causes": ["Contact method not verified", "Notification rules misconfigured", "DND enabled"],
      "solutions": ["Verify contact methods", "Check notification rules", "Disable Do Not Disturb"]
    },
    {
      "pattern": "incident.*not auto-resolving",
      "meaning": "Incidents not resolving automatically",
      "causes": ["Auto-resolve disabled", "Resolve event not sent", "Event routing issue"],
      "solutions": ["Enable auto-resolve on service", "Send resolve event", "Check integration routing"]
    },
    {
      "pattern": "service.*disabled",
      "meaning": "Service not accepting events",
      "causes": ["Service manually disabled", "Maintenance window", "Service deleted"],
      "solutions": ["Re-enable service", "Wait for maintenance to end", "Recreate service"]
    },
    {
      "pattern": "dedup_key.*mismatch",
      "meaning": "Deduplication key conflict",
      "causes": ["Same dedup_key for different issues", "Dedup_key format changed", "Integration changed"],
      "solutions": ["Use unique dedup_key per distinct issue", "Keep consistent dedup_key format", "Clear existing alert"]
    },
    {
      "pattern": "incident.*acknowledged but still paging",
      "meaning": "Notifications continue after acknowledgement",
      "causes": ["Ack timeout too short", "New events triggering", "Escalation continued"],
      "solutions": ["Increase ack timeout", "Check for new trigger events", "Review escalation policy"]
    },
    {
      "pattern": "webhook.*delivery failed",
      "meaning": "Webhook extension not delivering",
      "causes": ["Endpoint unreachable", "Endpoint returning error", "SSL certificate issue"],
      "solutions": ["Verify endpoint URL", "Check endpoint returns 2xx", "Fix SSL certificate"]
    }
  ],
  "debugging_commands": [
    "pd event trigger --integration-key=<key> --description=test",
    "pd incident list",
    "pd service list",
    "pd schedule show --id=<id>"
  ],
  "documentation_urls": [
    "https://developer.pagerduty.com/docs/",
    "https://github.com/PagerDuty"
  ]
}
