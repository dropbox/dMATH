{
  "tool": "eslint",
  "version": "9.0.0",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "no_unused_vars",
      "pattern": "no-unused-vars|is defined but never used",
      "message": "Variable declared but never used",
      "cause": "Dead code or forgotten variable",
      "solutions": [
        {
          "approach": "Remove variable",
          "code": "// Delete unused declaration",
          "when": "Variable not needed"
        },
        {
          "approach": "Use underscore prefix",
          "code": "const _unusedButNeeded = value;",
          "when": "Destructuring with omitted values"
        },
        {
          "approach": "Configure rule",
          "code": "// eslint.config.js: \"no-unused-vars\": [\"error\", { \"argsIgnorePattern\": \"^_\" }]",
          "when": "Want to ignore prefixed vars"
        }
      ]
    },
    {
      "id": "no_undef",
      "pattern": "no-undef|is not defined",
      "message": "Using undefined variable",
      "cause": "Variable not declared or imported",
      "solutions": [
        {
          "approach": "Import/declare",
          "code": "import { something } from 'module';",
          "when": "Need to import"
        },
        {
          "approach": "Define global",
          "code": "/* global myGlobal */",
          "when": "Known global variable"
        },
        {
          "approach": "Configure globals",
          "code": "// eslint.config.js: globals: { myGlobal: 'readonly' }",
          "when": "Project-wide global"
        }
      ]
    },
    {
      "id": "prefer_const",
      "pattern": "prefer-const|never reassigned",
      "message": "Variable should be const",
      "cause": "Using let for variable that's never reassigned",
      "solutions": [
        {
          "approach": "Use const",
          "code": "const value = 42;  // instead of let",
          "when": "Variable won't change"
        },
        {
          "approach": "Disable rule",
          "code": "// eslint-disable-next-line prefer-const",
          "when": "Plan to reassign later"
        }
      ]
    },
    {
      "id": "eqeqeq",
      "pattern": "eqeqeq|Expected .* and instead saw",
      "message": "Use strict equality",
      "cause": "Using == instead of ===",
      "solutions": [
        {
          "approach": "Use strict equality",
          "code": "if (a === b) { ... }",
          "when": "Standard comparison"
        },
        {
          "approach": "Allow null check",
          "code": "// Rule config: \"eqeqeq\": [\"error\", \"always\", { \"null\": \"ignore\" }]",
          "when": "Want == null shorthand"
        }
      ]
    },
    {
      "id": "no_console",
      "pattern": "no-console|Unexpected console statement",
      "message": "Console statement in code",
      "cause": "Debug logging left in code",
      "solutions": [
        {
          "approach": "Remove console",
          "code": "// Delete console.log statements",
          "when": "Debug code to remove"
        },
        {
          "approach": "Use logger",
          "code": "logger.info('message');",
          "when": "Need production logging"
        },
        {
          "approach": "Disable for line",
          "code": "// eslint-disable-next-line no-console\nconsole.log('intentional');",
          "when": "Console is intentional"
        }
      ]
    },
    {
      "id": "import_order",
      "pattern": "import/order|Import should come before",
      "message": "Import order incorrect",
      "cause": "Imports not in configured order",
      "solutions": [
        {
          "approach": "Reorder imports",
          "code": "// 1. external packages\nimport React from 'react';\n// 2. internal modules\nimport { util } from './util';",
          "when": "Imports out of order"
        },
        {
          "approach": "Use auto-fix",
          "code": "eslint --fix src/",
          "when": "Let ESLint fix it"
        }
      ]
    },
    {
      "id": "config_parse_error",
      "pattern": "Parsing error|ESLint configuration|Invalid",
      "message": "Configuration or parsing error",
      "cause": "Invalid ESLint config or parser issue",
      "solutions": [
        {
          "approach": "Set parser",
          "code": "// eslint.config.js:\nimport tseslint from 'typescript-eslint';\nexport default tseslint.config(...);",
          "when": "Using TypeScript"
        },
        {
          "approach": "Check syntax",
          "code": "// Validate eslint.config.js exports",
          "when": "Config syntax error"
        },
        {
          "approach": "Set ecmaVersion",
          "code": "languageOptions: { ecmaVersion: 2024 }",
          "when": "Using new JS features"
        }
      ]
    },
    {
      "id": "no_empty",
      "pattern": "no-empty|Empty block statement",
      "message": "Empty code block",
      "cause": "Block with no statements inside",
      "solutions": [
        {
          "approach": "Add implementation",
          "code": "if (condition) {\n  doSomething();\n}",
          "when": "Forgot implementation"
        },
        {
          "approach": "Add comment",
          "code": "if (condition) {\n  // Intentionally empty\n}",
          "when": "Empty is intentional"
        },
        {
          "approach": "Allow empty catch",
          "code": "// Rule config: \"no-empty\": [\"error\", { \"allowEmptyCatch\": true }]",
          "when": "Swallowing errors intentionally"
        }
      ]
    }
  ]
}
