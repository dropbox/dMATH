{
  "tool": "orbstack",
  "category": "local_development",
  "common_errors": [
    {
      "pattern": "OrbStack.*not running",
      "cause": "OrbStack app not started",
      "solutions": [
        "Open OrbStack application",
        "Start from menu bar icon",
        "Use 'orb' command to manage"
      ]
    },
    {
      "pattern": "Cannot connect to Docker",
      "cause": "Docker context not set or OrbStack stopped",
      "solutions": [
        "Start OrbStack application",
        "Set context: docker context use orbstack",
        "Check OrbStack is running in menu bar"
      ]
    },
    {
      "pattern": "migration.*from Docker Desktop",
      "cause": "Migration from Docker Desktop incomplete",
      "solutions": [
        "Use OrbStack's migration wizard",
        "Export volumes before migration",
        "Quit Docker Desktop completely first"
      ]
    },
    {
      "pattern": "Linux machine.*not found",
      "cause": "Linux VM not created",
      "solutions": [
        "Create machine: orb create ubuntu",
        "List machines: orb list",
        "Use OrbStack GUI to create"
      ]
    },
    {
      "pattern": "file sharing.*slow",
      "cause": "Volume mount performance issue",
      "solutions": [
        "Use VirtioFS (default, fastest)",
        "Check file sync settings",
        "Reduce number of mounted files"
      ]
    },
    {
      "pattern": "kubernetes.*not ready",
      "cause": "K8s cluster not enabled or starting",
      "solutions": [
        "Enable K8s in OrbStack settings",
        "Wait for cluster to start",
        "Check kubectl context: orbstack"
      ]
    },
    {
      "pattern": "container.*domain not resolving",
      "cause": "OrbStack DNS not working",
      "solutions": [
        "Use container.orb.local domain",
        "Check OrbStack network settings",
        "Restart OrbStack"
      ]
    },
    {
      "pattern": "SSH.*connection failed",
      "cause": "SSH to Linux machine failing",
      "solutions": [
        "Use 'orb' command for shell",
        "Check machine is running",
        "Verify SSH key configuration"
      ]
    },
    {
      "pattern": "rosetta.*required",
      "cause": "x86 emulation needed on Apple Silicon",
      "solutions": [
        "Install Rosetta: softwareupdate --install-rosetta",
        "Enable Rosetta in OrbStack settings",
        "Use ARM images when possible"
      ]
    },
    {
      "pattern": "license.*expired",
      "cause": "OrbStack license issue",
      "solutions": [
        "Check license in OrbStack settings",
        "Renew or purchase license",
        "Use free tier within limits"
      ]
    }
  ]
}
