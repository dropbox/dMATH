{
  "tool": "yup",
  "category": "data_validation",
  "common_errors": [
    {
      "error_pattern": "ValidationError.*is a required field",
      "description": "Required field not provided",
      "causes": [
        "Field missing from input",
        "Null or undefined value",
        "Field name mismatch"
      ],
      "solutions": [
        "Provide required field value",
        "Use .notRequired() or .optional()",
        "Check field name matches schema"
      ]
    },
    {
      "error_pattern": "must be a.*type",
      "description": "Value type doesn't match schema",
      "causes": [
        "String passed where number expected",
        "Wrong type in input",
        "Type coercion failed"
      ],
      "solutions": [
        "Provide correct type",
        "Use .typeError() for custom message",
        "Use .transform() for type conversion"
      ]
    },
    {
      "error_pattern": "must match.*pattern",
      "description": "String doesn't match regex",
      "causes": [
        "Input doesn't match .matches()",
        "Regex pattern incorrect"
      ],
      "solutions": [
        "Provide string matching pattern",
        "Check regex syntax",
        "Use .matches(regex, { message: '...' })"
      ]
    },
    {
      "error_pattern": "must be at least.*characters",
      "description": "String too short",
      "causes": [
        "Input shorter than .min()",
        "Empty string provided"
      ],
      "solutions": [
        "Provide longer string",
        "Adjust .min() constraint",
        "Use .default('') for empty handling"
      ]
    },
    {
      "error_pattern": "must be at most.*characters",
      "description": "String too long",
      "causes": [
        "Input exceeds .max()",
        "Unexpected whitespace"
      ],
      "solutions": [
        "Shorten input string",
        "Use .trim() to remove whitespace",
        "Increase .max() limit"
      ]
    },
    {
      "error_pattern": "must be one of the following values",
      "description": "Value not in oneOf list",
      "causes": [
        "Input not in .oneOf() array",
        "Case sensitivity issue"
      ],
      "solutions": [
        "Use one of the allowed values",
        "Add value to .oneOf() list",
        "Use .lowercase().oneOf() for case-insensitive"
      ]
    },
    {
      "error_pattern": "cannot be null",
      "description": "Null value not allowed",
      "causes": [
        "Null passed to non-nullable field",
        "Schema doesn't allow null"
      ],
      "solutions": [
        "Provide non-null value",
        "Use .nullable() to allow null",
        "Use .defined() or .required() appropriately"
      ]
    },
    {
      "error_pattern": "must be a valid email",
      "description": "Invalid email format",
      "causes": [
        "Email format incorrect",
        "Missing @ or domain"
      ],
      "solutions": [
        "Provide valid email",
        "Use custom .matches() for special cases",
        "Check input before validation"
      ]
    },
    {
      "error_pattern": "must be a valid URL",
      "description": "Invalid URL format",
      "causes": [
        "Missing protocol (http://)",
        "Invalid URL characters"
      ],
      "solutions": [
        "Include protocol in URL",
        "Use .url() with schemes option",
        "Validate URL format"
      ]
    },
    {
      "error_pattern": "circular reference",
      "description": "Schema has circular dependency",
      "causes": [
        "Schema references itself directly",
        "Nested schemas reference each other"
      ],
      "solutions": [
        "Use yup.lazy() for recursive schemas",
        "Break circular dependencies",
        "Define schema inside .shape()"
      ]
    },
    {
      "error_pattern": "this must be defined",
      "description": "Undefined value not allowed",
      "causes": [
        "Field is undefined",
        ".defined() constraint violated"
      ],
      "solutions": [
        "Provide defined value",
        "Use .default() for default value",
        "Remove .defined() if undefined OK"
      ]
    },
    {
      "error_pattern": "test.*is not a function",
      "description": "Custom test incorrectly configured",
      "causes": [
        ".test() not given function",
        "Wrong test method signature"
      ],
      "solutions": [
        "Pass function as second or third arg to .test()",
        "Use correct signature: .test('name', 'message', fn)",
        "Return boolean from test function"
      ]
    }
  ]
}
