{
  "tool": "npm_build",
  "category": "build_system",
  "errors": [
    {
      "pattern": "npm ERR! ENOENT",
      "cause": "File or directory not found",
      "solutions": [
        "Run npm install first",
        "Check file path exists",
        "Verify package.json is present"
      ]
    },
    {
      "pattern": "npm ERR! EACCES",
      "cause": "Permission denied",
      "solutions": [
        "Fix npm permissions",
        "Use nvm instead of global install",
        "Don't run npm with sudo"
      ]
    },
    {
      "pattern": "npm ERR! peer dep missing",
      "cause": "Peer dependency not satisfied",
      "solutions": [
        "Install missing peer dependency",
        "Use --legacy-peer-deps",
        "Update package versions"
      ]
    },
    {
      "pattern": "npm ERR! code ERESOLVE",
      "cause": "Dependency resolution conflict",
      "solutions": [
        "Use --force to override",
        "Delete node_modules and reinstall",
        "Update conflicting packages"
      ]
    },
    {
      "pattern": "Cannot find module",
      "cause": "Module not installed or missing",
      "solutions": [
        "Run npm install",
        "Check import path",
        "Verify module is in dependencies"
      ]
    },
    {
      "pattern": "npm WARN.*deprecated",
      "cause": "Using deprecated package",
      "solutions": [
        "Update to newer version",
        "Find alternative package",
        "Check package changelog"
      ]
    },
    {
      "pattern": "npm ERR! code EINTEGRITY",
      "cause": "Package integrity check failed",
      "solutions": [
        "Clear npm cache",
        "Delete package-lock.json",
        "Run npm install again"
      ]
    },
    {
      "pattern": "npm ERR! network",
      "cause": "Network connectivity issue",
      "solutions": [
        "Check internet connection",
        "Configure proxy settings",
        "Try different registry"
      ]
    },
    {
      "pattern": "npm ERR! 404",
      "cause": "Package not found in registry",
      "solutions": [
        "Check package name spelling",
        "Verify package exists on npm",
        "Check registry configuration"
      ]
    },
    {
      "pattern": "npm ERR! engine",
      "cause": "Node.js version incompatible",
      "solutions": [
        "Update Node.js version",
        "Use nvm to switch versions",
        "Check engines field in package.json"
      ]
    }
  ]
}
