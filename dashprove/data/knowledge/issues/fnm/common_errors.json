{
  "tool": "fnm",
  "category": "version_manager",
  "common_errors": [
    {
      "pattern": "Can't find an installed Node version",
      "cause": "No Node version installed or none matches",
      "solutions": [
        "Run 'fnm install <version>'",
        "Run 'fnm install' to install from .nvmrc/.node-version",
        "List installed: 'fnm list'"
      ]
    },
    {
      "pattern": "fnm: command not found",
      "cause": "fnm not initialized in shell",
      "solutions": [
        "Add to shell profile: eval \"$(fnm env)\"",
        "Use --use-on-cd for auto-switching",
        "Restart terminal after setup"
      ]
    },
    {
      "pattern": "Can't find version in dotfiles",
      "cause": "No .nvmrc or .node-version file found",
      "solutions": [
        "Create .node-version with version number",
        "Create .nvmrc with version number",
        "Specify version: 'fnm use 20'"
      ]
    },
    {
      "pattern": "Node.js.*does not exist",
      "cause": "Specified version not available",
      "solutions": [
        "List available: 'fnm list-remote'",
        "Use LTS alias: 'fnm install --lts'",
        "Check version format (v prefix optional)"
      ]
    },
    {
      "pattern": "Can't create symlink",
      "cause": "Permission or filesystem issue",
      "solutions": [
        "Check FNM_DIR permissions",
        "Run with appropriate permissions",
        "Verify filesystem supports symlinks"
      ]
    },
    {
      "pattern": "error: not a valid semver",
      "cause": "Invalid version format",
      "solutions": [
        "Use valid semver: 20.0.0 or 20",
        "Use alias: lts, latest",
        "Remove invalid characters from version"
      ]
    },
    {
      "pattern": "multishell is not enabled",
      "cause": "Multishell feature not activated",
      "solutions": [
        "Add --multi to fnm env: eval \"$(fnm env --multi)\"",
        "Set FNM_MULTISHELL_PATH",
        "Restart shell after enabling"
      ]
    },
    {
      "pattern": "download.*failed",
      "cause": "Network or mirror issue",
      "solutions": [
        "Check internet connection",
        "Set custom mirror: FNM_NODE_DIST_MIRROR",
        "Retry after network recovery"
      ]
    },
    {
      "pattern": "corepack.*not found",
      "cause": "Corepack not enabled for Node version",
      "solutions": [
        "Run 'fnm exec corepack enable'",
        "Enable corepack: 'corepack enable'",
        "Install corepack globally"
      ]
    },
    {
      "pattern": "shell.*not supported",
      "cause": "Shell not recognized by fnm",
      "solutions": [
        "Manually configure PATH for shell",
        "Use --shell flag with fnm env",
        "Check supported shells: bash, zsh, fish, powershell"
      ]
    }
  ]
}
