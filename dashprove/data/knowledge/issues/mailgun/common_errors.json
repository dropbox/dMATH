{
  "tool": "mailgun",
  "category": "email_services",
  "errors": [
    {
      "pattern": "401.*Unauthorized",
      "meaning": "Invalid API key",
      "causes": ["Wrong API key", "Using sending key for non-sending endpoint", "Key deleted or expired"],
      "solutions": ["Verify MAILGUN_API_KEY", "Use correct key type (sending vs. management)", "Generate new key in dashboard"]
    },
    {
      "pattern": "Domain not found",
      "meaning": "Domain not configured in Mailgun",
      "causes": ["Domain not added to account", "Typo in domain name", "Domain deleted"],
      "solutions": ["Add domain in Mailgun dashboard", "Verify domain name spelling", "Check domain status in dashboard"]
    },
    {
      "pattern": "Domain.*not verified",
      "meaning": "Domain DNS records not verified",
      "causes": ["DNS records not added", "DNS propagation pending", "Incorrect DNS values"],
      "solutions": ["Add TXT and MX records to DNS", "Wait for DNS propagation (up to 48h)", "Verify DNS values match Mailgun dashboard exactly"]
    },
    {
      "pattern": "Free accounts are for test purposes only",
      "meaning": "Sandbox domain restrictions",
      "causes": ["Using sandbox domain in production", "Recipient not authorized"],
      "solutions": ["Verify custom domain", "Add authorized recipients for sandbox", "Upgrade to paid plan"]
    },
    {
      "pattern": "bounced.*hard",
      "meaning": "Hard bounce - permanent delivery failure",
      "causes": ["Email address doesn't exist", "Domain doesn't accept email", "Mailbox permanently disabled"],
      "solutions": ["Remove address from list", "Implement double opt-in", "Validate emails at collection time"]
    },
    {
      "pattern": "complained",
      "meaning": "Recipient marked as spam",
      "causes": ["Unwanted emails", "Missing unsubscribe link", "Poor list hygiene"],
      "solutions": ["Add prominent unsubscribe link", "Honor opt-out requests immediately", "Clean list regularly"]
    },
    {
      "pattern": "temporary.*failure|soft bounce",
      "meaning": "Temporary delivery failure",
      "causes": ["Recipient mailbox full", "Server temporarily unavailable", "Greylisting"],
      "solutions": ["Mailgun will retry automatically", "Monitor for persistent soft bounces", "Convert to hard bounce after multiple failures"]
    },
    {
      "pattern": "rate limit",
      "meaning": "API rate limit exceeded",
      "causes": ["Too many API requests", "Burst sending limit hit", "Plan limit reached"],
      "solutions": ["Implement rate limiting in code", "Use batch sending endpoint", "Upgrade plan for higher limits"]
    }
  ]
}
