{
  "tool": "django",
  "category": "web_framework",
  "errors": [
    {
      "pattern": "ImproperlyConfigured",
      "cause": "Django settings issue",
      "solutions": [
        "Set DJANGO_SETTINGS_MODULE",
        "Check settings.py syntax",
        "Verify installed apps"
      ]
    },
    {
      "pattern": "OperationalError.*no such table",
      "cause": "Database table doesn't exist",
      "solutions": [
        "Run python manage.py migrate",
        "Check migration files",
        "Verify database connection"
      ]
    },
    {
      "pattern": "TemplateDoesNotExist",
      "cause": "Template file not found",
      "solutions": [
        "Check TEMPLATES dirs setting",
        "Verify template path",
        "Check app is in INSTALLED_APPS"
      ]
    },
    {
      "pattern": "RelatedObjectDoesNotExist",
      "cause": "Related model instance missing",
      "solutions": [
        "Check foreign key exists",
        "Use hasattr() before access",
        "Handle None case"
      ]
    },
    {
      "pattern": "CSRF.*Failed",
      "cause": "CSRF validation failed",
      "solutions": [
        "Include {% csrf_token %}",
        "Check CSRF middleware",
        "Verify cookie settings"
      ]
    },
    {
      "pattern": "IntegrityError",
      "cause": "Database constraint violation",
      "solutions": [
        "Check unique constraints",
        "Verify foreign keys exist",
        "Review NOT NULL fields"
      ]
    },
    {
      "pattern": "ProgrammingError",
      "cause": "SQL or migration error",
      "solutions": [
        "Run makemigrations",
        "Check raw SQL syntax",
        "Reset migrations if stuck"
      ]
    },
    {
      "pattern": "ModuleNotFoundError",
      "cause": "Missing Python package",
      "solutions": [
        "Install with pip",
        "Check virtual environment",
        "Verify PYTHONPATH"
      ]
    },
    {
      "pattern": "DisallowedHost",
      "cause": "Host not in ALLOWED_HOSTS",
      "solutions": [
        "Add host to ALLOWED_HOSTS",
        "Use '*' in development",
        "Check request Host header"
      ]
    },
    {
      "pattern": "TypeError.*argument",
      "cause": "Wrong function arguments",
      "solutions": [
        "Check function signature",
        "Verify positional vs keyword",
        "Review Django version changes"
      ]
    }
  ]
}
