{
  "tool": "axum",
  "category": "web_framework",
  "errors": [
    {
      "pattern": "missing.*FromRef",
      "cause": "State extraction failed",
      "solutions": [
        "Derive FromRef for state",
        "Check State<T> type",
        "Use with_state() correctly"
      ]
    },
    {
      "pattern": "Handler.*not implemented",
      "cause": "Handler trait not satisfied",
      "solutions": [
        "Check handler signature",
        "Verify extractor order",
        "Review async fn return"
      ]
    },
    {
      "pattern": "Json.*rejection",
      "cause": "JSON extraction failed",
      "solutions": [
        "Check Content-Type header",
        "Verify JSON format",
        "Review struct definition"
      ]
    },
    {
      "pattern": "ExtensionRejection",
      "cause": "Extension not found",
      "solutions": [
        "Add extension via layer",
        "Check Extension<T> type",
        "Verify middleware order"
      ]
    },
    {
      "pattern": "Router.*conflict",
      "cause": "Overlapping routes",
      "solutions": [
        "Check route patterns",
        "Review nesting",
        "Use method routing"
      ]
    },
    {
      "pattern": "Layer.*error",
      "cause": "Tower layer error",
      "solutions": [
        "Check layer implementation",
        "Verify Service trait bounds",
        "Review tower compatibility"
      ]
    },
    {
      "pattern": "address in use",
      "cause": "Port already bound",
      "solutions": [
        "Use different port",
        "Kill existing process",
        "Check socket options"
      ]
    },
    {
      "pattern": "MatchedPath.*none",
      "cause": "No route matched",
      "solutions": [
        "Add fallback handler",
        "Check route definitions",
        "Review request path"
      ]
    }
  ]
}
