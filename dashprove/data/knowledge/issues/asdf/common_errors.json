{
  "tool": "asdf",
  "category": "version_manager",
  "common_errors": [
    {
      "pattern": "No version is set for command",
      "cause": "No version specified in .tool-versions or globally",
      "solutions": [
        "Add version to .tool-versions file",
        "Run 'asdf global <tool> <version>'",
        "Run 'asdf local <tool> <version>' for directory"
      ]
    },
    {
      "pattern": "Plugin not installed for command",
      "cause": "asdf plugin not added",
      "solutions": [
        "Run 'asdf plugin add <name>'",
        "List available: 'asdf plugin list all'",
        "Check plugin URL if custom"
      ]
    },
    {
      "pattern": "version.*not installed",
      "cause": "Specified version not installed locally",
      "solutions": [
        "Run 'asdf install <tool> <version>'",
        "Run 'asdf install' to install all from .tool-versions",
        "List available: 'asdf list all <tool>'"
      ]
    },
    {
      "pattern": "asdf: command not found",
      "cause": "asdf not in PATH or not sourced",
      "solutions": [
        "Add to shell profile: . $HOME/.asdf/asdf.sh",
        "Source completions: . $HOME/.asdf/completions/asdf.bash",
        "Restart shell or source profile"
      ]
    },
    {
      "pattern": "Build failed.*configure: error",
      "cause": "Missing build dependencies for tool",
      "solutions": [
        "Install build essentials: gcc, make, etc.",
        "Check plugin README for dependencies",
        "Install OS-specific dev libraries"
      ]
    },
    {
      "pattern": "ASDF_DATA_DIR.*not found",
      "cause": "Custom data directory misconfigured",
      "solutions": [
        "Check ASDF_DATA_DIR env var",
        "Ensure directory exists and is writable",
        "Reset to default: unset ASDF_DATA_DIR"
      ]
    },
    {
      "pattern": "Unknown command",
      "cause": "Invalid asdf subcommand",
      "solutions": [
        "Check spelling of command",
        "Run 'asdf help' for available commands",
        "Ensure asdf is up to date"
      ]
    },
    {
      "pattern": "Checksum mismatch",
      "cause": "Downloaded file integrity check failed",
      "solutions": [
        "Retry the installation",
        "Check network connection",
        "Verify plugin is official/trusted"
      ]
    },
    {
      "pattern": "legacy version file.*not enabled",
      "cause": "Legacy file support (e.g., .nvmrc) not enabled",
      "solutions": [
        "Add legacy_version_file = yes to ~/.asdfrc",
        "Enable per-plugin legacy support",
        "Use .tool-versions instead"
      ]
    },
    {
      "pattern": "cannot resolve.*to an executable",
      "cause": "Shim cannot find installed binary",
      "solutions": [
        "Run 'asdf reshim <tool>'",
        "Reinstall the version",
        "Check tool is properly built"
      ]
    }
  ]
}
