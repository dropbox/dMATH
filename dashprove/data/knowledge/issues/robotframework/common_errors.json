{
  "tool": "robotframework",
  "version": "7.0",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "keyword_not_found",
      "pattern": "No keyword with name '.*' found|Keyword '.*' not found",
      "message": "Keyword not found",
      "cause": "Keyword not defined or library not imported",
      "solutions": [
        {
          "approach": "Import library",
          "code": "*** Settings ***\nLibrary    SeleniumLibrary\nLibrary    Collections",
          "when": "Library not imported"
        },
        {
          "approach": "Check keyword name",
          "code": "# Keywords are case-insensitive but space-sensitive\nClick Button  vs  ClickButton",
          "when": "Name mismatch"
        },
        {
          "approach": "Use library prefix",
          "code": "SeleniumLibrary.Click Element    ${locator}",
          "when": "Ambiguous keyword"
        }
      ]
    },
    {
      "id": "variable_not_found",
      "pattern": "Variable '.*' not found|Resolving variable '.*' failed",
      "message": "Variable not defined",
      "cause": "Variable not set or scope issue",
      "solutions": [
        {
          "approach": "Define in Variables section",
          "code": "*** Variables ***\n${URL}    https://example.com\n@{USERS}    alice    bob",
          "when": "Static variable needed"
        },
        {
          "approach": "Set in keyword",
          "code": "${result}=    Get Text    ${locator}\nSet Suite Variable    ${RESULT}    ${result}",
          "when": "Dynamic variable"
        },
        {
          "approach": "Pass as argument",
          "code": "My Keyword    ${arg1}    ${arg2}",
          "when": "Variable from test case"
        }
      ]
    },
    {
      "id": "element_not_found",
      "pattern": "Element .* not found|ElementNotFound|NoSuchElementException",
      "message": "Web element not found",
      "cause": "Locator doesn't match any element",
      "solutions": [
        {
          "approach": "Wait for element",
          "code": "Wait Until Element Is Visible    ${locator}    timeout=10s",
          "when": "Element loads dynamically"
        },
        {
          "approach": "Use better locator",
          "code": "# Prefer id > name > xpath\n${elem}=    Get WebElement    id=submit-btn",
          "when": "Locator unreliable"
        },
        {
          "approach": "Handle iframe",
          "code": "Select Frame    id=myframe\nClick Element    ${locator}\nUnselect Frame",
          "when": "Element in iframe"
        }
      ]
    },
    {
      "id": "timeout_error",
      "pattern": "Timeout .* exceeded|TimeoutException|timed out",
      "message": "Operation timed out",
      "cause": "Action didn't complete within timeout",
      "solutions": [
        {
          "approach": "Increase timeout",
          "code": "Set Selenium Timeout    30 seconds\nWait Until Page Contains    text    timeout=60s",
          "when": "Operation legitimately slow"
        },
        {
          "approach": "Set implicit wait",
          "code": "Set Selenium Implicit Wait    10 seconds",
          "when": "Elements load slowly"
        },
        {
          "approach": "Check page load",
          "code": "Wait Until Page Contains Element    id=loaded-indicator",
          "when": "Page not fully loaded"
        }
      ]
    },
    {
      "id": "import_error",
      "pattern": "Importing library '.*' failed|ModuleNotFoundError",
      "message": "Library import failed",
      "cause": "Library not installed or path wrong",
      "solutions": [
        {
          "approach": "Install library",
          "code": "pip install robotframework-seleniumlibrary\npip install robotframework-requests",
          "when": "Library not installed"
        },
        {
          "approach": "Check Python path",
          "code": "# Add to PYTHONPATH or use absolute import\nLibrary    /path/to/MyLibrary.py",
          "when": "Custom library"
        },
        {
          "approach": "Check dependencies",
          "code": "pip install selenium>=4.0.0",
          "when": "Missing dependency"
        }
      ]
    },
    {
      "id": "syntax_error",
      "pattern": "Parsing '.*' failed|Invalid syntax",
      "message": "Robot syntax error",
      "cause": "Invalid Robot Framework syntax",
      "solutions": [
        {
          "approach": "Check indentation",
          "code": "*** Test Cases ***\nMy Test\n    [Documentation]    Two spaces before keywords\n    Log    Hello",
          "when": "Wrong indentation"
        },
        {
          "approach": "Check separators",
          "code": "# Use 2+ spaces or tab between cells\nClick Element    ${locator}",
          "when": "Missing separator"
        },
        {
          "approach": "Escape special chars",
          "code": "Log    \\${not_a_variable}\nLog    Price: \\$100",
          "when": "Need literal $ or @"
        }
      ]
    },
    {
      "id": "resource_not_found",
      "pattern": "Resource file '.*' does not exist",
      "message": "Resource file not found",
      "cause": "Resource path incorrect",
      "solutions": [
        {
          "approach": "Use relative path",
          "code": "*** Settings ***\nResource    ../resources/common.robot",
          "when": "Path relative to test"
        },
        {
          "approach": "Use variables",
          "code": "Resource    ${CURDIR}/common.robot",
          "when": "Path relative to file"
        },
        {
          "approach": "Check file exists",
          "code": "# Verify file exists at specified path",
          "when": "File missing"
        }
      ]
    },
    {
      "id": "return_value_error",
      "pattern": "Return value|RETURN|Cannot convert",
      "message": "Return value handling error",
      "cause": "Incorrect return value usage",
      "solutions": [
        {
          "approach": "Capture return value",
          "code": "${result}=    My Keyword\nLog    Result: ${result}",
          "when": "Need keyword result"
        },
        {
          "approach": "Return from keyword",
          "code": "My Keyword\n    ${value}=    Do Something\n    RETURN    ${value}",
          "when": "Creating keyword that returns"
        },
        {
          "approach": "Multiple return values",
          "code": "${a}    ${b}=    Get Two Values\nLog    ${a} and ${b}",
          "when": "Multiple returns"
        }
      ]
    }
  ]
}
