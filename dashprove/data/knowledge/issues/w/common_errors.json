{
  "tool": "w",
  "version": "procps",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "cannot_open_utmp",
      "pattern": "cannot open|utmp|no utmp",
      "message": "Cannot read utmp",
      "cause": "utmp file missing or unreadable",
      "solutions": [
        {
          "approach": "Check utmp",
          "code": "ls -la /var/run/utmp",
          "when": "Verify file"
        },
        {
          "approach": "Alternative",
          "code": "who\nps aux | grep -E 'pts|tty'",
          "when": "Other tools"
        }
      ]
    },
    {
      "id": "empty_output",
      "pattern": "no output|no users",
      "message": "No users shown",
      "cause": "No active login sessions",
      "solutions": [
        {
          "approach": "Check processes",
          "code": "ps aux | grep bash\nps aux | grep ssh",
          "when": "Find sessions"
        },
        {
          "approach": "Container/Docker",
          "code": "# In containers, utmp may not be updated\nps aux",
          "when": "Containerized"
        }
      ]
    },
    {
      "id": "high_load",
      "pattern": "load average",
      "message": "Understanding load",
      "cause": "Load average interpretation",
      "solutions": [
        {
          "approach": "Interpret load",
          "code": "# load average: 1min 5min 15min\n# Values > CPU count = overloaded\nnproc  # Get CPU count",
          "when": "Performance check"
        },
        {
          "approach": "Just users",
          "code": "w -h  # No header, just users",
          "when": "Skip system info"
        }
      ]
    }
  ]
}
