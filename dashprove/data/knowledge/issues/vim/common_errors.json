{
  "tool": "vim",
  "version": "9.1",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "e212_cant_open_write",
      "pattern": "E212.*Can't open file for writing",
      "message": "E212: Can't open file for writing",
      "cause": "No write permission or directory doesn't exist",
      "solutions": [
        {
          "approach": "Write with sudo",
          "code": ":w !sudo tee %",
          "when": "Need root permissions"
        },
        {
          "approach": "Create directory",
          "code": ":!mkdir -p %:h",
          "when": "Parent directory missing"
        },
        {
          "approach": "Write to different path",
          "code": ":w /tmp/backup.txt",
          "when": "Save to writable location"
        }
      ]
    },
    {
      "id": "e325_swap_file",
      "pattern": "E325.*ATTENTION.*swap file",
      "message": "E325: Swap file already exists",
      "cause": "Previous Vim session didn't close properly",
      "solutions": [
        {
          "approach": "Recover file",
          "code": "Press 'r' to recover, then :e! to reload",
          "when": "Previous session had unsaved changes"
        },
        {
          "approach": "Delete swap",
          "code": "Press 'd' to delete swap file",
          "when": "Previous changes not needed"
        },
        {
          "approach": "Delete swap manually",
          "code": "rm .filename.swp",
          "when": "Open file in different way"
        }
      ]
    },
    {
      "id": "e486_pattern_not_found",
      "pattern": "E486.*Pattern not found",
      "message": "E486: Pattern not found",
      "cause": "Search pattern doesn't match any text",
      "solutions": [
        {
          "approach": "Case insensitive search",
          "code": "/pattern\\c  or  :set ignorecase",
          "when": "Case mismatch"
        },
        {
          "approach": "Escape special chars",
          "code": "/pattern\\.txt  (escape dots, etc.)",
          "when": "Pattern has regex metacharacters"
        },
        {
          "approach": "Use very nomagic",
          "code": "/\\Vpattern.txt",
          "when": "Search literal string"
        }
      ]
    },
    {
      "id": "e505_readonly",
      "pattern": "E505.*is read-only",
      "message": "E505: File is read-only",
      "cause": "File opened in readonly mode or has readonly attribute",
      "solutions": [
        {
          "approach": "Force write",
          "code": ":w!",
          "when": "Have write permissions"
        },
        {
          "approach": "Remove readonly mode",
          "code": ":set noro",
          "when": "Opened with -R flag"
        },
        {
          "approach": "Change file permissions",
          "code": ":!chmod u+w %",
          "when": "File has readonly attribute"
        }
      ]
    },
    {
      "id": "e37_no_write",
      "pattern": "E37.*No write since last change",
      "message": "E37: No write since last change",
      "cause": "Trying to quit without saving changes",
      "solutions": [
        {
          "approach": "Save and quit",
          "code": ":wq  or  ZZ",
          "when": "Want to save changes"
        },
        {
          "approach": "Force quit",
          "code": ":q!  or  ZQ",
          "when": "Discard changes"
        },
        {
          "approach": "Write then quit",
          "code": ":x",
          "when": "Save only if changed"
        }
      ]
    },
    {
      "id": "e21_cannot_modify",
      "pattern": "E21.*Cannot make changes.*modifiable is off",
      "message": "E21: Cannot make changes, 'modifiable' is off",
      "cause": "Buffer is set to non-modifiable (help files, readonly buffers)",
      "solutions": [
        {
          "approach": "Enable modification",
          "code": ":set modifiable",
          "when": "Need to edit buffer"
        },
        {
          "approach": "Force edit",
          "code": ":set ma | normal! ... | set noma",
          "when": "Temporary modification"
        },
        {
          "approach": "Copy to new buffer",
          "code": ":%y | enew | p",
          "when": "Edit copy instead"
        }
      ]
    },
    {
      "id": "e492_not_editor_command",
      "pattern": "E492.*Not an editor command",
      "message": "E492: Not an editor command",
      "cause": "Typo in command or plugin not loaded",
      "solutions": [
        {
          "approach": "Check command spelling",
          "code": ":help index  (list all commands)",
          "when": "Possible typo"
        },
        {
          "approach": "Check plugin loaded",
          "code": ":scriptnames",
          "when": "Command from plugin"
        },
        {
          "approach": "Source vimrc",
          "code": ":source ~/.vimrc",
          "when": "Config not loaded"
        }
      ]
    }
  ]
}
