{
  "tool": "circleci",
  "category": "ci_cd",
  "common_errors": [
    {
      "pattern": "Config file.*not valid|configuration error",
      "meaning": "Invalid .circleci/config.yml",
      "causes": ["YAML syntax error", "Invalid key", "Missing required field"],
      "solutions": ["Validate locally: circleci config validate", "Check YAML indentation", "Use circleci config process to debug"]
    },
    {
      "pattern": "Could not find executor.*image",
      "meaning": "Docker image not found",
      "causes": ["Image doesn't exist", "Private registry auth", "Typo in image name"],
      "solutions": ["Verify image on Docker Hub", "Add auth for private images", "Check image tag exists"]
    },
    {
      "pattern": "OOM|memory|killed.*signal 9",
      "meaning": "Out of memory",
      "causes": ["Resource class too small", "Memory leak", "Too many parallel processes"],
      "solutions": ["Use larger resource_class", "Reduce parallelism", "Optimize memory usage in tests"]
    },
    {
      "pattern": "no space left on device|disk.*full",
      "meaning": "Disk space exhausted",
      "causes": ["Build artifacts too large", "Cache too big", "Log files filling disk"],
      "solutions": ["Clean up between steps", "Use persist_to_workspace selectively", "Optimize Docker layer caching"]
    },
    {
      "pattern": "Timed out|job timed out|no output",
      "meaning": "Job exceeded time limit",
      "causes": ["Default 10 min no-output timeout", "Process hanging", "Test too slow"],
      "solutions": ["Add no_output_timeout: 30m to step", "Check for deadlocks", "Split into smaller jobs"]
    },
    {
      "pattern": "permission denied|EACCES",
      "meaning": "File permission error",
      "causes": ["Wrong file permissions", "Different user context", "Volume mount issues"],
      "solutions": ["Use chmod in setup step", "Check file ownership", "Set correct permissions in Dockerfile"]
    },
    {
      "pattern": "checkout.*failed|could not clone",
      "meaning": "Git checkout failed",
      "causes": ["Deploy key missing", "SSH key not added", "Private repo access"],
      "solutions": ["Add SSH key in project settings", "Use checkout step with ssh fingerprint", "Check deploy key permissions"]
    },
    {
      "pattern": "cache.*not found|unable to restore",
      "meaning": "Cache restoration failed",
      "causes": ["Cache key mismatch", "Cache expired", "First build"],
      "solutions": ["Check cache key pattern", "Use checksum in key", "Cache not available on first run is normal"]
    },
    {
      "pattern": "context.*not found|permission denied.*context",
      "meaning": "Context access issue",
      "causes": ["Context doesn't exist", "No permission", "Restricted context"],
      "solutions": ["Create context in org settings", "Add project to context permissions", "Check security group access"]
    },
    {
      "pattern": "orb.*not found|version.*does not exist",
      "meaning": "Orb not found or invalid",
      "causes": ["Orb name typo", "Version doesn't exist", "Private orb access"],
      "solutions": ["Check orb registry", "Pin to valid version", "Enable third-party orbs in org settings"]
    }
  ],
  "debugging_commands": [
    "circleci config validate",
    "circleci config process .circleci/config.yml",
    "circleci local execute --job <job-name>",
    "circleci orb validate orb.yml"
  ],
  "documentation_urls": [
    "https://circleci.com/docs/",
    "https://github.com/CircleCI-Public"
  ]
}
