{
  "tool": "faker",
  "category": "test_data_generation",
  "common_errors": [
    {
      "error_pattern": "Faker.*has no attribute",
      "description": "Attempting to use provider method that doesn't exist",
      "causes": [
        "Typo in provider method name",
        "Provider not registered",
        "Method deprecated or renamed in version"
      ],
      "solutions": [
        "Check available providers: dir(fake)",
        "Use correct method: fake.name(), fake.email()",
        "Register custom provider: fake.add_provider(MyProvider)",
        "Check Faker documentation for method names"
      ]
    },
    {
      "error_pattern": "UnsupportedLocale",
      "description": "Requested locale not available",
      "causes": [
        "Invalid locale code",
        "Locale not installed",
        "Locale code misspelled"
      ],
      "solutions": [
        "Use valid locale: Faker('en_US'), Faker('de_DE')",
        "Check available locales in documentation",
        "Fallback to generic locale: Faker('en')"
      ]
    },
    {
      "error_pattern": "AttributeError.*unique.*exhausted",
      "description": "No more unique values available for field",
      "causes": [
        "Called unique too many times",
        "Small value space (e.g., boolean, small enum)",
        "Seed causing same values"
      ],
      "solutions": [
        "Reset unique state: fake.unique.clear()",
        "Increase value space in custom provider",
        "Use fake.unique with limited iterations",
        "Consider if uniqueness is truly required"
      ]
    },
    {
      "error_pattern": "RecursionError.*seed",
      "description": "Recursion during seeded generation",
      "causes": [
        "Provider recursively calls itself",
        "Circular dependency in custom providers"
      ],
      "solutions": [
        "Review custom provider implementation",
        "Break circular dependencies",
        "Use base Faker methods in custom providers"
      ]
    },
    {
      "error_pattern": "TypeError.*seed.*argument",
      "description": "Invalid seed argument",
      "causes": [
        "Seed not an integer",
        "Seed passed incorrectly"
      ],
      "solutions": [
        "Use integer seed: Faker.seed(12345)",
        "Seed instance: fake.seed_instance(12345)",
        "Set seed before generating data"
      ]
    },
    {
      "error_pattern": "ValueError.*weights",
      "description": "Invalid weights for random_element",
      "causes": [
        "Weights don't match element count",
        "Negative or invalid weight values"
      ],
      "solutions": [
        "Match weight count to elements",
        "Use positive numeric weights",
        "Weights: fake.random_element(elements=OrderedDict([('a', 0.5), ('b', 0.5)]))"
      ]
    },
    {
      "error_pattern": "ImportError.*faker",
      "description": "Faker package not installed correctly",
      "causes": [
        "Package not installed",
        "Virtual environment issue",
        "Conflicting package named faker"
      ],
      "solutions": [
        "Install: pip install Faker",
        "Check venv: which python",
        "Upgrade: pip install --upgrade Faker"
      ]
    },
    {
      "error_pattern": "JSON.*serializable",
      "description": "Faker value cannot be JSON serialized",
      "causes": [
        "Datetime object returned",
        "Custom object from provider",
        "Binary data generated"
      ],
      "solutions": [
        "Convert datetime: str(fake.date_object())",
        "Use string methods: fake.iso8601()",
        "Implement custom JSON encoder"
      ]
    },
    {
      "error_pattern": "DuplicateKeyError.*unique",
      "description": "Generated duplicate when unique required",
      "causes": [
        "Database unique constraint",
        "Faker unique exhausted",
        "Same seed across test runs"
      ],
      "solutions": [
        "Use fake.unique.first_name()",
        "Clear between tests: fake.unique.clear()",
        "Use different seed per test"
      ]
    },
    {
      "error_pattern": "locale.*None",
      "description": "Locale not properly set",
      "causes": [
        "Locale set to None",
        "Empty locale string"
      ],
      "solutions": [
        "Set valid locale: Faker('en_US')",
        "Use default: Faker() defaults to en_US"
      ]
    }
  ]
}
