{
  "tool": "tsserver",
  "version": "5.3+",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "tsconfig_not_found",
      "pattern": "tsconfig.json.*not found|Cannot find.*tsconfig|no project",
      "message": "TypeScript configuration not found",
      "cause": "No tsconfig.json in project",
      "solutions": [
        {
          "approach": "Create tsconfig.json",
          "code": "npx tsc --init",
          "when": "New project"
        },
        {
          "approach": "Open correct folder",
          "code": "// Open folder containing tsconfig.json",
          "when": "Wrong directory"
        },
        {
          "approach": "Specify config file",
          "code": "// Use tsconfig setting in editor",
          "when": "Non-standard location"
        }
      ]
    },
    {
      "id": "module_not_found",
      "pattern": "Cannot find module|Module not found|could not find.*declaration",
      "message": "Module cannot be found",
      "cause": "Module not installed or types missing",
      "solutions": [
        {
          "approach": "Install package",
          "code": "npm install packagename",
          "when": "Package not installed"
        },
        {
          "approach": "Install types",
          "code": "npm install -D @types/packagename",
          "when": "Types missing"
        },
        {
          "approach": "Configure module resolution",
          "code": "{ \"compilerOptions\": { \"moduleResolution\": \"bundler\" } }",
          "when": "Resolution strategy wrong"
        },
        {
          "approach": "Add to paths",
          "code": "{ \"compilerOptions\": { \"paths\": { \"@/*\": [\"./src/*\"] } } }",
          "when": "Using path aliases"
        }
      ]
    },
    {
      "id": "type_error",
      "pattern": "Type.*is not assignable|Property.*does not exist|Argument.*not assignable",
      "message": "TypeScript type error",
      "cause": "Type mismatch in code",
      "solutions": [
        {
          "approach": "Fix type annotation",
          "code": "const x: CorrectType = value;",
          "when": "Wrong type annotation"
        },
        {
          "approach": "Use type assertion",
          "code": "const x = value as ExpectedType;",
          "when": "Type is correct but TS can't infer"
        },
        {
          "approach": "Add type guard",
          "code": "if ('property' in obj) { obj.property }",
          "when": "Need narrowing"
        },
        {
          "approach": "Update types",
          "code": "npm update @types/packagename",
          "when": "Types outdated"
        }
      ]
    },
    {
      "id": "server_crash",
      "pattern": "tsserver.*crash|OOM|out of memory|heap",
      "message": "TypeScript server crashed",
      "cause": "Memory exhaustion or bug",
      "solutions": [
        {
          "approach": "Increase memory",
          "code": "// Set NODE_OPTIONS=\"--max-old-space-size=8192\"",
          "when": "Memory limit"
        },
        {
          "approach": "Restart server",
          "code": "// Restart TS server from editor",
          "when": "Corrupted state"
        },
        {
          "approach": "Exclude large directories",
          "code": "{ \"exclude\": [\"node_modules\", \"dist\", \"**/generated\"] }",
          "when": "Too many files"
        },
        {
          "approach": "Update TypeScript",
          "code": "npm install -D typescript@latest",
          "when": "Known bug"
        }
      ]
    },
    {
      "id": "slow_analysis",
      "pattern": "analyzing|slow|checking|Loading",
      "message": "Analysis very slow",
      "cause": "Large project or complex types",
      "solutions": [
        {
          "approach": "Enable incremental",
          "code": "{ \"compilerOptions\": { \"incremental\": true } }",
          "when": "Speed up rebuilds"
        },
        {
          "approach": "Use skipLibCheck",
          "code": "{ \"compilerOptions\": { \"skipLibCheck\": true } }",
          "when": "Skip .d.ts checking"
        },
        {
          "approach": "Exclude test files",
          "code": "{ \"exclude\": [\"**/*.test.ts\", \"**/*.spec.ts\"] }",
          "when": "Separate test config"
        },
        {
          "approach": "Use project references",
          "code": "{ \"references\": [{ \"path\": \"./packages/core\" }] }",
          "when": "Monorepo"
        }
      ]
    },
    {
      "id": "jsx_error",
      "pattern": "JSX.*error|Cannot use JSX|react-jsx",
      "message": "JSX configuration error",
      "cause": "JSX not configured correctly",
      "solutions": [
        {
          "approach": "Set jsx option",
          "code": "{ \"compilerOptions\": { \"jsx\": \"react-jsx\" } }",
          "when": "React 17+"
        },
        {
          "approach": "Use preserve for bundler",
          "code": "{ \"compilerOptions\": { \"jsx\": \"preserve\" } }",
          "when": "Using bundler to transform"
        },
        {
          "approach": "Install React types",
          "code": "npm install -D @types/react @types/react-dom",
          "when": "Types missing"
        }
      ]
    },
    {
      "id": "declaration_emit_error",
      "pattern": "declaration.*emit|Cannot write file|d.ts.*error",
      "message": "Declaration file emit error",
      "cause": "Can't generate .d.ts files",
      "solutions": [
        {
          "approach": "Set declaration output",
          "code": "{ \"compilerOptions\": { \"declarationDir\": \"./types\" } }",
          "when": "Output directory issue"
        },
        {
          "approach": "Enable declaration",
          "code": "{ \"compilerOptions\": { \"declaration\": true } }",
          "when": "Declarations not enabled"
        },
        {
          "approach": "Fix inline types",
          "code": "// Export types that are used in public API",
          "when": "Inlined type error"
        }
      ]
    },
    {
      "id": "strict_mode_error",
      "pattern": "strict.*error|strictNullChecks|implicitAny",
      "message": "Strict mode violations",
      "cause": "Code doesn't satisfy strict checks",
      "solutions": [
        {
          "approach": "Add null checks",
          "code": "if (value !== null && value !== undefined) { ... }",
          "when": "strictNullChecks errors"
        },
        {
          "approach": "Add type annotations",
          "code": "function foo(x: string): number { ... }",
          "when": "noImplicitAny errors"
        },
        {
          "approach": "Disable specific strict option",
          "code": "{ \"compilerOptions\": { \"strictNullChecks\": false } }",
          "when": "Migrating gradually"
        }
      ]
    },
    {
      "id": "version_mismatch",
      "pattern": "version.*mismatch|Expected version|incompatible",
      "message": "TypeScript version mismatch",
      "cause": "Global vs project TypeScript version differs",
      "solutions": [
        {
          "approach": "Use workspace version",
          "code": "// Configure editor to use workspace TypeScript",
          "when": "Wrong version selected"
        },
        {
          "approach": "Install specific version",
          "code": "npm install -D typescript@5.3",
          "when": "Need specific version"
        },
        {
          "approach": "Check node_modules",
          "code": "npx tsc --version",
          "when": "Verify version"
        }
      ]
    },
    {
      "id": "plugin_error",
      "pattern": "plugin.*error|typescript-plugin|Cannot load plugin",
      "message": "TypeScript plugin error",
      "cause": "Plugin not installed or configured",
      "solutions": [
        {
          "approach": "Install plugin",
          "code": "npm install -D typescript-plugin-css-modules",
          "when": "Plugin not installed"
        },
        {
          "approach": "Configure plugins",
          "code": "{ \"compilerOptions\": { \"plugins\": [{ \"name\": \"pluginname\" }] } }",
          "when": "Plugin not configured"
        },
        {
          "approach": "Check plugin compatibility",
          "code": "// Ensure plugin version matches TS version",
          "when": "Version mismatch"
        }
      ]
    }
  ]
}
