{
  "tool": "sdkman",
  "category": "version_manager",
  "common_errors": [
    {
      "pattern": "sdk: command not found",
      "cause": "SDKMAN not initialized in shell",
      "solutions": [
        "Add to profile: source \"$HOME/.sdkman/bin/sdkman-init.sh\"",
        "Run installer: curl -s https://get.sdkman.io | bash",
        "Restart shell after installation"
      ]
    },
    {
      "pattern": "Stop!.*is not a valid candidate",
      "cause": "Candidate not available in SDKMAN",
      "solutions": [
        "List candidates: sdk list",
        "Check spelling of candidate name",
        "Use correct candidate (e.g., java, gradle, maven)"
      ]
    },
    {
      "pattern": "Stop!.*not available",
      "cause": "Version not available for candidate",
      "solutions": [
        "List versions: sdk list <candidate>",
        "Check for vendor prefix (e.g., 21.0.1-tem)",
        "Update SDKMAN: sdk selfupdate"
      ]
    },
    {
      "pattern": "Stop!.*is not installed",
      "cause": "Version not installed locally",
      "solutions": [
        "Install: sdk install <candidate> <version>",
        "Use installed version: sdk use <candidate> <version>",
        "List installed: sdk list <candidate> | grep installed"
      ]
    },
    {
      "pattern": "SDKMAN_DIR.*not set",
      "cause": "SDKMAN_DIR environment not configured",
      "solutions": [
        "Set SDKMAN_DIR: export SDKMAN_DIR=\"$HOME/.sdkman\"",
        "Source init script properly",
        "Reinstall SDKMAN"
      ]
    },
    {
      "pattern": "Cannot install.*offline mode",
      "cause": "SDKMAN in offline mode without cached versions",
      "solutions": [
        "Disable offline: sdk offline disable",
        "Check internet connection",
        "Download manually and place in archives"
      ]
    },
    {
      "pattern": "checksum.*verification failed",
      "cause": "Downloaded archive corrupted",
      "solutions": [
        "Retry installation",
        "Clear SDKMAN cache: rm ~/.sdkman/tmp/*",
        "Check network stability"
      ]
    },
    {
      "pattern": "JAVA_HOME.*not set",
      "cause": "No Java version set as current",
      "solutions": [
        "Set version: sdk default java <version>",
        "Use version: sdk use java <version>",
        "Install Java: sdk install java"
      ]
    },
    {
      "pattern": "sdk update failed",
      "cause": "Cannot fetch SDKMAN updates",
      "solutions": [
        "Check internet connection",
        "Try: sdk selfupdate force",
        "Check api.sdkman.io is accessible"
      ]
    },
    {
      "pattern": "env.*file not found",
      "cause": ".sdkmanrc not found in project",
      "solutions": [
        "Create .sdkmanrc: sdk env init",
        "Export current versions: sdk env",
        "Check sdkman_auto_env in config"
      ]
    }
  ]
}
