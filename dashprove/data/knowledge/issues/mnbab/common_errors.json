{
  "tool": "mnbab",
  "version": "latest",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "timeout",
      "pattern": "timeout",
      "message": "Verification timed out",
      "cause": "Branch and bound exploration took too long",
      "solutions": [
        {
          "approach": "Increase timeout",
          "code": "timeout = 3600  # 1 hour",
          "when": "May succeed with more time"
        },
        {
          "approach": "Reduce epsilon",
          "code": "eps = eps / 2",
          "when": "Perturbation too large"
        },
        {
          "approach": "Use fewer branches",
          "code": "# Limit branching depth",
          "when": "Deep branching"
        }
      ]
    },
    {
      "id": "memory_error",
      "pattern": "memory",
      "message": "Out of memory",
      "cause": "Too many subproblems in BaB",
      "solutions": [
        {
          "approach": "Limit subproblems",
          "code": "# Set maximum subproblem count",
          "when": "Exponential branching"
        },
        {
          "approach": "Use GPU memory efficiently",
          "code": "# Batch subproblems carefully",
          "when": "GPU memory limited"
        },
        {
          "approach": "Prune aggressively",
          "code": "# Increase pruning threshold",
          "when": "Too many open branches"
        }
      ]
    },
    {
      "id": "model_error",
      "pattern": "model",
      "message": "Model loading failed",
      "cause": "Incompatible model format",
      "solutions": [
        {
          "approach": "Check ONNX opset",
          "code": "# Use supported ONNX operations",
          "when": "ONNX compatibility"
        },
        {
          "approach": "Verify layer support",
          "code": "# Check supported activation functions",
          "when": "Unsupported activation"
        }
      ]
    },
    {
      "id": "branching_error",
      "pattern": "branching",
      "message": "Branching strategy failed",
      "cause": "Cannot select neuron for branching",
      "solutions": [
        {
          "approach": "Use different strategy",
          "code": "# Try fsb, babsr, or kfsb branching",
          "when": "Default strategy failing"
        },
        {
          "approach": "Check unstable neurons",
          "code": "# Need unstable neurons for branching",
          "when": "All neurons stable"
        }
      ]
    },
    {
      "id": "bound_error",
      "pattern": "bound",
      "message": "Bound computation failed",
      "cause": "Cannot compute intermediate bounds",
      "solutions": [
        {
          "approach": "Use simpler bounds",
          "code": "# Try IBP initialization",
          "when": "Complex bounds failing"
        },
        {
          "approach": "Check numerical stability",
          "code": "# Verify no NaN/inf in bounds",
          "when": "Numerical issues"
        }
      ]
    },
    {
      "id": "verified_unsafe",
      "pattern": "UNSAFE",
      "message": "Property violated",
      "cause": "Found counterexample",
      "solutions": [
        {
          "approach": "Examine counterexample",
          "code": "# Check adversarial input",
          "when": "Understanding attack"
        },
        {
          "approach": "Verify counterexample",
          "code": "# Confirm on original model",
          "when": "Validating result"
        },
        {
          "approach": "Train more robust model",
          "code": "# Use certified training",
          "when": "Improving robustness"
        }
      ]
    },
    {
      "id": "unknown",
      "pattern": "UNKNOWN",
      "message": "Verification inconclusive",
      "cause": "BaB could not prove or disprove",
      "solutions": [
        {
          "approach": "Increase resources",
          "code": "# More time/memory",
          "when": "Resource limited"
        },
        {
          "approach": "Use tighter bounds",
          "code": "# Enable multi-neuron constraints",
          "when": "Bounds too loose"
        }
      ]
    },
    {
      "id": "specification_error",
      "pattern": "specification",
      "message": "Invalid property specification",
      "cause": "Property cannot be verified",
      "solutions": [
        {
          "approach": "Check spec format",
          "code": "# Verify VNN-LIB syntax",
          "when": "Syntax error"
        },
        {
          "approach": "Check dimensions",
          "code": "# Output spec must match network",
          "when": "Dimension mismatch"
        }
      ]
    }
  ]
}
