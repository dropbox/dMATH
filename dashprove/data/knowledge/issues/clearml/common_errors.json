{
  "tool": "clearml",
  "category": "ml_ops",
  "common_errors": [
    {
      "pattern": "clearml.backend_api.session.SendError",
      "meaning": "Cannot communicate with ClearML server",
      "causes": ["Server URL wrong", "Network connectivity issue", "Server not running"],
      "solutions": ["Verify CLEARML_API_HOST is correct", "Check network access to server", "Ensure ClearML server is up"]
    },
    {
      "pattern": "Credentials.*invalid",
      "meaning": "ClearML API credentials rejected",
      "causes": ["Access key expired", "Secret key wrong", "Credentials not set"],
      "solutions": ["Regenerate credentials in ClearML UI", "Run clearml-init to reconfigure", "Check ~/.clearml.conf"]
    },
    {
      "pattern": "Task.*not found",
      "meaning": "Referenced task doesn't exist",
      "causes": ["Task ID incorrect", "Task deleted", "Wrong project scope"],
      "solutions": ["Verify task ID in ClearML UI", "Check task wasn't deleted", "Ensure correct project context"]
    },
    {
      "pattern": "Failed to upload artifact",
      "meaning": "Artifact upload to storage failed",
      "causes": ["Storage credentials invalid", "Bucket doesn't exist", "File too large"],
      "solutions": ["Update storage credentials", "Create required bucket", "Configure multipart upload"]
    },
    {
      "pattern": "Agent.*failed to pull",
      "meaning": "ClearML Agent cannot fetch required resources",
      "causes": ["Git credentials missing", "Docker image not found", "pip package unavailable"],
      "solutions": ["Configure Git credentials for agent", "Push Docker image to registry", "Check package exists on PyPI"]
    },
    {
      "pattern": "Queue.*empty.*no tasks",
      "meaning": "Agent has no tasks to execute",
      "causes": ["No tasks enqueued", "Queue name mismatch", "Tasks all completed"],
      "solutions": ["Enqueue tasks to the queue", "Verify agent is listening to correct queue", "Check task status in UI"]
    },
    {
      "pattern": "Model.*storage.*error",
      "meaning": "Cannot store/retrieve model files",
      "causes": ["Output URI misconfigured", "Permissions denied", "Storage full"],
      "solutions": ["Configure output_uri in task.connect()", "Grant storage write permissions", "Clean up old models"]
    },
    {
      "pattern": "Experiment.*already exists",
      "meaning": "Task name collision in project",
      "causes": ["Same name reused", "Retry without new task", "Clone failed"],
      "solutions": ["Use Task.init(reuse_last_task_id=False)", "Change task name", "Use Task.clone() for iterations"]
    },
    {
      "pattern": "Pipeline.*step.*failed",
      "meaning": "Pipeline step execution error",
      "causes": ["Step task raised exception", "Dependency step failed", "Resource allocation error"],
      "solutions": ["Check step task logs", "Review upstream step status", "Adjust step resource requirements"]
    },
    {
      "pattern": "Dataset.*version.*not found",
      "meaning": "Requested dataset version doesn't exist",
      "causes": ["Version ID wrong", "Dataset deleted", "Version not published"],
      "solutions": ["List available versions in UI", "Check dataset wasn't deleted", "Publish dataset version first"]
    }
  ],
  "debugging_commands": [
    "clearml-agent daemon --status",
    "clearml-agent list",
    "clearml-data list",
    "clearml-task list --project <name>"
  ],
  "documentation_urls": [
    "https://clear.ml/docs/",
    "https://github.com/allegroai/clearml"
  ]
}
