{
  "tool": "react_native",
  "category": "mobile_framework",
  "common_errors": [
    {
      "id": "rn_metro_bundler",
      "pattern": "metro bundler|bundle error|unable to resolve|transformer",
      "severity": "error",
      "causes": [
        "Metro cache corrupted",
        "Module not found",
        "Wrong transformer config"
      ],
      "solutions": [
        "Clear cache: npx react-native start --reset-cache",
        "Delete node_modules and reinstall",
        "Check metro.config.js"
      ]
    },
    {
      "id": "rn_native_modules",
      "pattern": "native module|NativeModules|linking error",
      "severity": "error",
      "causes": [
        "Native module not linked",
        "Autolinking failed",
        "Pod install needed"
      ],
      "solutions": [
        "Run: cd ios && pod install",
        "RN 0.60+: autolinking, no manual link",
        "Check react-native.config.js"
      ]
    },
    {
      "id": "rn_android_build",
      "pattern": "android build|gradle error|sdk version",
      "severity": "error",
      "causes": [
        "SDK version mismatch",
        "Gradle version issues",
        "Dependencies conflict"
      ],
      "solutions": [
        "Set compileSdkVersion/targetSdkVersion in build.gradle",
        "Check gradle-wrapper.properties version",
        "Clean: cd android && ./gradlew clean"
      ]
    },
    {
      "id": "rn_ios_build",
      "pattern": "ios build|xcode error|pod install|swift version",
      "severity": "error",
      "causes": [
        "Pods out of sync",
        "Xcode version issues",
        "Swift compatibility"
      ],
      "solutions": [
        "pod deintegrate && pod install",
        "Clean DerivedData in Xcode",
        "Check iOS deployment target"
      ]
    },
    {
      "id": "rn_flatlist_performance",
      "pattern": "FlatList slow|list performance|virtualization",
      "severity": "warning",
      "causes": [
        "Missing key extractor",
        "Not using getItemLayout",
        "Too many rerenders"
      ],
      "solutions": [
        "Add keyExtractor prop",
        "Use getItemLayout for fixed-size items",
        "Use React.memo for list items"
      ]
    },
    {
      "id": "rn_navigation",
      "pattern": "navigation error|screen not found|navigator",
      "severity": "error",
      "causes": [
        "Navigation not configured",
        "Missing NavigationContainer",
        "Screen not registered"
      ],
      "solutions": [
        "Wrap app in NavigationContainer",
        "Register all screens in navigator",
        "Use correct navigation method for navigator type"
      ]
    },
    {
      "id": "rn_invariant_violation",
      "pattern": "Invariant Violation|requireNativeComponent|not registered",
      "severity": "error",
      "causes": [
        "Native component missing",
        "Version mismatch",
        "Build not synced"
      ],
      "solutions": [
        "Rebuild native code: npx react-native run-ios/android",
        "Clear watchman: watchman watch-del-all",
        "Reinstall pods/rebuild gradle"
      ]
    },
    {
      "id": "rn_new_architecture",
      "pattern": "new architecture|fabric|turbo modules|bridgeless",
      "severity": "info",
      "causes": [
        "Migrating to New Architecture",
        "Fabric/TurboModules errors",
        "Library compatibility"
      ],
      "solutions": [
        "Enable in gradle.properties: newArchEnabled=true",
        "Check library compatibility",
        "Use codegen for native modules"
      ]
    }
  ]
}
