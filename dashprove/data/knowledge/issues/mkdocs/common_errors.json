{
  "tool": "mkdocs",
  "version": "1.5+",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "config_not_found",
      "pattern": "mkdocs.yml.*not found|configuration file.*missing|no configuration",
      "message": "Configuration file not found",
      "cause": "mkdocs.yml missing or in wrong directory",
      "solutions": [
        {
          "approach": "Create config file",
          "code": "mkdocs new my-project",
          "when": "Starting new project"
        },
        {
          "approach": "Specify config path",
          "code": "mkdocs build -f path/to/mkdocs.yml",
          "when": "Non-standard location"
        },
        {
          "approach": "Check working directory",
          "code": "ls mkdocs.yml",
          "when": "Wrong directory"
        }
      ]
    },
    {
      "id": "doc_not_found",
      "pattern": "file.*not found|document.*missing|Page.*not found",
      "message": "Markdown file referenced in nav not found",
      "cause": "File path in mkdocs.yml doesn't exist",
      "solutions": [
        {
          "approach": "Fix file path",
          "code": "nav:\n  - Home: index.md  # Must exist in docs/",
          "when": "Wrong path in nav"
        },
        {
          "approach": "Create missing file",
          "code": "touch docs/missing-page.md",
          "when": "File should exist"
        },
        {
          "approach": "Remove from nav",
          "code": "# Remove non-existent page from nav section",
          "when": "Page no longer needed"
        }
      ]
    },
    {
      "id": "theme_not_found",
      "pattern": "theme.*not found|unknown theme|Theme.*does not exist",
      "message": "MkDocs theme not installed",
      "cause": "Theme package not installed",
      "solutions": [
        {
          "approach": "Install Material theme",
          "code": "pip install mkdocs-material",
          "when": "Using Material theme"
        },
        {
          "approach": "Use built-in theme",
          "code": "theme:\n  name: mkdocs  # or 'readthedocs'",
          "when": "Don't need custom theme"
        },
        {
          "approach": "Install required theme",
          "code": "pip install mkdocs-theme-name",
          "when": "Other third-party theme"
        }
      ]
    },
    {
      "id": "plugin_error",
      "pattern": "plugin.*not found|failed to load plugin|PluginError",
      "message": "MkDocs plugin error",
      "cause": "Plugin not installed or configuration error",
      "solutions": [
        {
          "approach": "Install plugin",
          "code": "pip install mkdocs-minify-plugin",
          "when": "Plugin not installed"
        },
        {
          "approach": "Check plugin name",
          "code": "plugins:\n  - search  # Not 'mkdocs-search'",
          "when": "Wrong plugin name"
        },
        {
          "approach": "Remove plugin",
          "code": "# Remove from plugins list if not needed",
          "when": "Plugin optional"
        }
      ]
    },
    {
      "id": "yaml_syntax_error",
      "pattern": "YAML.*error|yaml.*syntax|parsing error.*yml",
      "message": "YAML syntax error in config",
      "cause": "Invalid YAML in mkdocs.yml",
      "solutions": [
        {
          "approach": "Fix indentation",
          "code": "# Use 2 spaces, no tabs\nnav:\n  - Home: index.md",
          "when": "Indentation wrong"
        },
        {
          "approach": "Quote special strings",
          "code": "site_name: 'My Site: With Colon'",
          "when": "Special characters"
        },
        {
          "approach": "Validate YAML",
          "code": "python -c \"import yaml; yaml.safe_load(open('mkdocs.yml'))\"",
          "when": "Check syntax"
        }
      ]
    },
    {
      "id": "extension_error",
      "pattern": "extension.*error|markdown extension.*not found|pymdown.*error",
      "message": "Markdown extension error",
      "cause": "Markdown extension not installed",
      "solutions": [
        {
          "approach": "Install pymdownx",
          "code": "pip install pymdown-extensions",
          "when": "Using PyMdown extensions"
        },
        {
          "approach": "Configure extensions",
          "code": "markdown_extensions:\n  - pymdownx.highlight\n  - pymdownx.superfences",
          "when": "Extensions not enabled"
        },
        {
          "approach": "Check Python-Markdown",
          "code": "pip install markdown",
          "when": "Base markdown package"
        }
      ]
    },
    {
      "id": "build_error",
      "pattern": "build.*failed|could not build|Error building docs",
      "message": "Documentation build failed",
      "cause": "Various build errors",
      "solutions": [
        {
          "approach": "Run with verbose",
          "code": "mkdocs build -v",
          "when": "Need more details"
        },
        {
          "approach": "Check strict mode",
          "code": "mkdocs build --strict",
          "when": "Find warnings"
        },
        {
          "approach": "Clean and rebuild",
          "code": "rm -rf site && mkdocs build",
          "when": "Stale build artifacts"
        }
      ]
    },
    {
      "id": "serve_port_in_use",
      "pattern": "port.*in use|address already in use|cannot bind",
      "message": "Development server port unavailable",
      "cause": "Port 8000 already in use",
      "solutions": [
        {
          "approach": "Use different port",
          "code": "mkdocs serve -a localhost:8080",
          "when": "Port 8000 busy"
        },
        {
          "approach": "Kill existing process",
          "code": "lsof -ti:8000 | xargs kill",
          "when": "Previous server running"
        }
      ]
    },
    {
      "id": "link_broken",
      "pattern": "broken link|link.*not found|invalid link",
      "message": "Internal link broken",
      "cause": "Markdown link target doesn't exist",
      "solutions": [
        {
          "approach": "Fix relative path",
          "code": "[Link](../other-page.md)",
          "when": "Wrong relative path"
        },
        {
          "approach": "Use absolute path",
          "code": "[Link](/folder/page.md)",
          "when": "Deep nesting"
        },
        {
          "approach": "Check file exists",
          "code": "ls docs/path/to/file.md",
          "when": "Target may not exist"
        }
      ]
    },
    {
      "id": "encoding_error",
      "pattern": "encoding.*error|UnicodeDecodeError|codec can't decode",
      "message": "File encoding error",
      "cause": "Markdown file not UTF-8",
      "solutions": [
        {
          "approach": "Convert to UTF-8",
          "code": "iconv -f ISO-8859-1 -t UTF-8 file.md > file_utf8.md",
          "when": "Wrong encoding"
        },
        {
          "approach": "Set docs dir encoding",
          "code": "# Ensure all .md files are saved as UTF-8",
          "when": "Multiple files"
        },
        {
          "approach": "Remove BOM",
          "code": "sed -i '1s/^\\xEF\\xBB\\xBF//' file.md",
          "when": "UTF-8 with BOM"
        }
      ]
    }
  ]
}
