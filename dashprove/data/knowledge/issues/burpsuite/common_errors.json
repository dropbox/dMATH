{
  "tool": "burpsuite",
  "category": "dast",
  "errors": [
    {
      "pattern": "License.*invalid",
      "cause": "Burp Suite license issue",
      "solutions": [
        "Enter valid license key",
        "Use Community Edition for free features",
        "Contact PortSwigger support"
      ]
    },
    {
      "pattern": "Proxy.*port in use",
      "cause": "Proxy port already occupied",
      "solutions": [
        "Change proxy port in settings",
        "Stop other application using port",
        "Default port is 8080"
      ]
    },
    {
      "pattern": "Certificate not trusted",
      "cause": "Burp CA not installed in browser",
      "solutions": [
        "Export Burp CA certificate",
        "Install CA in browser/system",
        "Follow PortSwigger CA setup guide"
      ]
    },
    {
      "pattern": "Connection timed out",
      "cause": "Target not responding",
      "solutions": [
        "Check target is accessible",
        "Verify proxy configuration",
        "Increase timeout settings"
      ]
    },
    {
      "pattern": "Scanner.*stalled",
      "cause": "Active scan stopped progressing",
      "solutions": [
        "Check scan queue status",
        "Reduce scan threads",
        "Restart scan with new scope"
      ]
    },
    {
      "pattern": "Out of memory",
      "cause": "JVM heap exhausted",
      "solutions": [
        "Increase memory in burpsuite.sh/bat",
        "Clear project data",
        "Reduce scan scope"
      ]
    },
    {
      "pattern": "Extension.*failed",
      "cause": "BApp extension error",
      "solutions": [
        "Check extension compatibility",
        "Update extension version",
        "Review extension logs"
      ]
    },
    {
      "pattern": "Issue.*found",
      "cause": "Vulnerability detected in scan",
      "solutions": [
        "Review issue details",
        "Verify finding accuracy",
        "Report to development team"
      ]
    }
  ]
}
