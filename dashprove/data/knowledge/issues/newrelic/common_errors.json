{
  "tool": "newrelic",
  "category": "monitoring",
  "common_errors": [
    {
      "pattern": "license_key.*invalid",
      "meaning": "New Relic license key rejected",
      "causes": ["Wrong license key", "Key from different account", "Key expired"],
      "solutions": ["Get correct key from New Relic UI", "Check account/organization", "Generate new key"]
    },
    {
      "pattern": "Agent.*not reporting",
      "meaning": "Agent not sending data to New Relic",
      "causes": ["Agent not running", "Network blocked", "Configuration error"],
      "solutions": ["Restart agent", "Check firewall to collector.newrelic.com", "Verify newrelic.yml"]
    },
    {
      "pattern": "transaction.*not appearing",
      "meaning": "Transactions not showing in APM",
      "causes": ["Agent not instrumented", "Wrong framework version", "Sampling"],
      "solutions": ["Verify agent attached to app", "Check framework compatibility", "Adjust trace sampling"]
    },
    {
      "pattern": "custom metric.*limit",
      "meaning": "Custom metrics quota exceeded",
      "causes": ["Too many unique metrics", "High cardinality attributes", "Metric explosion"],
      "solutions": ["Reduce metric cardinality", "Aggregate metrics", "Review custom instrumentation"]
    },
    {
      "pattern": "distributed tracing.*broken",
      "meaning": "Distributed traces not connecting",
      "causes": ["Missing header propagation", "Agent version mismatch", "Sampling mismatch"],
      "solutions": ["Ensure trace headers propagated", "Update all agents to same major version", "Configure matching sampling"]
    },
    {
      "pattern": "log forwarding.*failed",
      "meaning": "Log in context not working",
      "causes": ["Log forwarding disabled", "API key issue", "Rate limited"],
      "solutions": ["Enable log forwarding in config", "Check Ingest license key", "Review log quotas"]
    },
    {
      "pattern": "Infrastructure.*agent.*offline",
      "meaning": "Infrastructure agent not reporting",
      "causes": ["Agent crashed", "License key wrong", "Hostname issue"],
      "solutions": ["Restart infra agent", "Verify license_key in newrelic-infra.yml", "Check hostname resolution"]
    },
    {
      "pattern": "Synthetics.*monitor.*failing",
      "meaning": "Synthetic monitor failing checks",
      "causes": ["Endpoint down", "Script error", "Timeout"],
      "solutions": ["Check target endpoint", "Debug script in editor", "Increase timeout"]
    },
    {
      "pattern": "NRQL.*syntax error",
      "meaning": "NRQL query syntax invalid",
      "causes": ["Wrong function name", "Missing clause", "Unquoted string"],
      "solutions": ["Check NRQL syntax docs", "Quote string literals", "Verify event/metric names"]
    },
    {
      "pattern": "alert.*not triggering",
      "meaning": "Alert condition not firing as expected",
      "causes": ["Threshold not met", "Wrong aggregation window", "No data"],
      "solutions": ["Verify condition threshold", "Check aggregation window duration", "Confirm data exists for query"]
    }
  ],
  "debugging_commands": [
    "newrelic diagnostics validate",
    "sudo systemctl status newrelic-infra",
    "cat /var/log/newrelic/newrelic-daemon.log",
    "cat /var/log/newrelic/php_agent.log"
  ],
  "documentation_urls": [
    "https://docs.newrelic.com/",
    "https://github.com/newrelic"
  ]
}
