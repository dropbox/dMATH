{
  "tool": "who",
  "version": "coreutils",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "no_utmp",
      "pattern": "cannot open|/var/run/utmp|no utmp",
      "message": "Cannot read utmp",
      "cause": "utmp file missing or unreadable",
      "solutions": [
        {
          "approach": "Check utmp",
          "code": "ls -la /var/run/utmp\ntouch /var/run/utmp",
          "when": "File missing"
        },
        {
          "approach": "Alternative",
          "code": "w\nusers\nlast",
          "when": "Other tools"
        }
      ]
    },
    {
      "id": "empty_output",
      "pattern": "no output|empty",
      "message": "No users shown",
      "cause": "No login sessions recorded",
      "solutions": [
        {
          "approach": "Check if PTY",
          "code": "tty  # Shows terminal device\nwho am i  # Your session specifically",
          "when": "SSH/pseudo-terminal"
        },
        {
          "approach": "Use w instead",
          "code": "w\nps aux | grep -E 'pts|tty'",
          "when": "More detailed"
        }
      ]
    },
    {
      "id": "invalid_option",
      "pattern": "invalid option|unrecognized option",
      "message": "Invalid option",
      "cause": "Option not supported",
      "solutions": [
        {
          "approach": "Common options",
          "code": "who              # All logged in\nwho -b           # Last boot time\nwho -r           # Current runlevel\nwho am i         # Your session\nwho -H           # With headers",
          "when": "Reference"
        }
      ]
    }
  ]
}
