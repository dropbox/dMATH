{
  "tool": "testcafe",
  "category": "browser_automation",
  "common_errors": [
    {
      "pattern": "Cannot find the browser.*not installed",
      "meaning": "Browser not found on system",
      "causes": ["Browser not installed", "Wrong browser path", "Browser name typo"],
      "solutions": ["Install browser or use testcafe chrome:headless", "Specify path: testcafe path:/path/to/chrome", "Check available: testcafe --list-browsers"]
    },
    {
      "pattern": "Unable to establish.*connection|browser disconnected",
      "meaning": "Lost connection to browser",
      "causes": ["Browser crashed", "Network timeout", "Resource exhaustion"],
      "solutions": ["Increase timeout: testcafe --browser-init-timeout", "Check system resources", "Try headless mode: chrome:headless"]
    },
    {
      "pattern": "Selector.*timeout exceeded|Element.*not found",
      "meaning": "Element selector timeout",
      "causes": ["Element not rendered", "Wrong selector", "Element in iframe"],
      "solutions": ["Use .with({ timeout: 10000 })", "Verify selector in DevTools", "Use Selector().withText() for flexibility"]
    },
    {
      "pattern": "action.*element is not visible",
      "meaning": "Element not visible for interaction",
      "causes": ["Element hidden by CSS", "Off-screen", "Covered by overlay"],
      "solutions": ["Wait for visibility: await t.expect(elem.visible).ok()", "Scroll: await t.scroll(elem, 'center')", "Close modal/overlay first"]
    },
    {
      "pattern": "AssertionError.*expected.*to be",
      "meaning": "Test assertion failed",
      "causes": ["Actual value differs from expected", "Timing issue", "Test logic error"],
      "solutions": ["Check actual value in error", "Add wait before assertion", "Debug with console.log or t.debug()"]
    },
    {
      "pattern": "native automation.*not supported|browser automation failed",
      "meaning": "TestCafe automation unsupported",
      "causes": ["Browser doesn't support", "OS/driver issue", "Headless limitation"],
      "solutions": ["Try different browser", "Update TestCafe", "Check OS compatibility"]
    },
    {
      "pattern": "Role.*cannot.*login|authentication failed",
      "meaning": "Role authentication issue",
      "causes": ["Login flow changed", "Cookies not preserved", "Wrong credentials"],
      "solutions": ["Verify login manually", "Check Role preserveUrl setting", "Add logging to debug auth"]
    },
    {
      "pattern": "fixture.*syntax error|test file.*error",
      "meaning": "Test file has syntax error",
      "causes": ["JavaScript syntax error", "Missing import", "Wrong async usage"],
      "solutions": ["Check file syntax", "Ensure exports correct", "Use async/await properly with t."]
    },
    {
      "pattern": "Request failed.*CORS|network request failed",
      "meaning": "API/network request blocked",
      "causes": ["CORS policy", "Network error", "SSL certificate issue"],
      "solutions": ["Use RequestMock for stubbing", "Configure proxy settings", "Add --ssl-certificate-path for self-signed"]
    },
    {
      "pattern": "cannot read property.*of undefined|TypeError",
      "meaning": "JavaScript runtime error in test",
      "causes": ["Null/undefined access", "Element not found", "Wrong variable"],
      "solutions": ["Add null checks", "Verify selectors return elements", "Debug with t.debug()"]
    }
  ],
  "debugging_commands": [
    "testcafe chrome test.js --debug-mode",
    "testcafe --list-browsers",
    "testcafe chrome test.js --screenshots path=./errors,takeOnFails=true",
    "testcafe chrome test.js --reporter spec,xunit:report.xml"
  ],
  "documentation_urls": [
    "https://testcafe.io/documentation/",
    "https://github.com/DevExpress/testcafe"
  ]
}
