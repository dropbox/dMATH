{
  "tool": "gcloud",
  "category": "cloud_cli",
  "common_errors": [
    {
      "pattern": "ERROR.*PERMISSION_DENIED",
      "meaning": "IAM permission denied for operation",
      "causes": ["Missing IAM role", "Service account lacks permission", "Organization policy blocks"],
      "solutions": ["Grant required IAM role", "Check service account permissions", "Review organization policies"]
    },
    {
      "pattern": "ERROR.*Insufficient Permission",
      "meaning": "User/service account lacks required permission",
      "causes": ["Role not granted", "Permission not in role", "Resource-level IAM denies"],
      "solutions": ["Grant appropriate role", "Use role with required permission", "Check resource IAM policy"]
    },
    {
      "pattern": "Your current project is not set",
      "meaning": "No default project configured",
      "causes": ["gcloud init not run", "Project not set", "Config corrupted"],
      "solutions": ["Run gcloud config set project PROJECT_ID", "Run gcloud init", "Check ~/.config/gcloud/"]
    },
    {
      "pattern": "UNAUTHENTICATED",
      "meaning": "Authentication credentials invalid or missing",
      "causes": ["Not logged in", "Token expired", "Service account key invalid"],
      "solutions": ["Run gcloud auth login", "Refresh: gcloud auth application-default login", "Verify service account key"]
    },
    {
      "pattern": "API.*has not been.*enabled",
      "meaning": "Required GCP API not enabled",
      "causes": ["API disabled", "New project", "API not available in region"],
      "solutions": ["Enable API: gcloud services enable API_NAME", "Wait for propagation", "Check API availability"]
    },
    {
      "pattern": "RESOURCE_EXHAUSTED",
      "meaning": "Quota or resource limit exceeded",
      "causes": ["Quota limit hit", "Concurrent operation limit", "Rate limiting"],
      "solutions": ["Request quota increase", "Wait and retry", "Batch operations"]
    },
    {
      "pattern": "NOT_FOUND",
      "meaning": "Resource doesn't exist",
      "causes": ["Resource deleted", "Wrong project", "Typo in resource name"],
      "solutions": ["Verify resource exists", "Check project context", "Verify resource name"]
    },
    {
      "pattern": "ALREADY_EXISTS",
      "meaning": "Resource with that name already exists",
      "causes": ["Duplicate creation attempt", "Name collision", "Resource not fully deleted"],
      "solutions": ["Use different name", "Check existing resources", "Wait for deletion to complete"]
    },
    {
      "pattern": "Error 403.*forbidden",
      "meaning": "Access to resource forbidden",
      "causes": ["Wrong account", "VPC Service Controls blocks", "Resource in different org"],
      "solutions": ["Verify correct account/project", "Check VPC Service Controls", "Verify resource access"]
    },
    {
      "pattern": "INVALID_ARGUMENT",
      "meaning": "Invalid argument provided to API",
      "causes": ["Wrong parameter format", "Invalid value", "Incompatible settings"],
      "solutions": ["Check parameter format", "Review API documentation", "Verify value constraints"]
    }
  ],
  "debugging_commands": [
    "gcloud auth list",
    "gcloud config list",
    "gcloud projects list",
    "gcloud --verbosity=debug <command>"
  ],
  "documentation_urls": [
    "https://cloud.google.com/sdk/docs/",
    "https://github.com/google-cloud-sdk-unofficial/google-cloud-sdk"
  ]
}
