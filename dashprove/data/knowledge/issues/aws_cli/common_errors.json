{
  "tool": "aws_cli",
  "category": "cloud_cli",
  "common_errors": [
    {
      "pattern": "Unable to locate credentials",
      "meaning": "AWS credentials not configured",
      "causes": ["No credentials file", "AWS_PROFILE not set", "IAM role not attached"],
      "solutions": ["Run aws configure", "Set AWS_ACCESS_KEY_ID/AWS_SECRET_ACCESS_KEY", "Attach IAM role to instance"]
    },
    {
      "pattern": "ExpiredToken",
      "meaning": "AWS session/security token expired",
      "causes": ["STS token expired", "MFA session timeout", "Temporary credentials expired"],
      "solutions": ["Refresh credentials: aws sts get-session-token", "Re-authenticate with MFA", "Get new temporary credentials"]
    },
    {
      "pattern": "AccessDenied",
      "meaning": "IAM policy denies the action",
      "causes": ["Missing IAM permission", "Resource policy denies", "SCP blocks action"],
      "solutions": ["Add required IAM permission", "Check resource-based policy", "Review organization SCPs"]
    },
    {
      "pattern": "InvalidAccessKeyId",
      "meaning": "Access key doesn't exist or is inactive",
      "causes": ["Key deleted", "Key deactivated", "Wrong key ID"],
      "solutions": ["Create new access key", "Activate key in IAM console", "Verify key ID is correct"]
    },
    {
      "pattern": "SignatureDoesNotMatch",
      "meaning": "Request signature verification failed",
      "causes": ["Secret key wrong", "Clock skew", "Request modified in transit"],
      "solutions": ["Verify secret access key", "Sync system clock: sudo ntpdate pool.ntp.org", "Check proxy not modifying requests"]
    },
    {
      "pattern": "UnauthorizedAccess",
      "meaning": "Not authorized for this AWS account",
      "causes": ["Wrong AWS account", "Cross-account access denied", "Resource in different account"],
      "solutions": ["Verify correct AWS account", "Set up cross-account role", "Check resource ARN account ID"]
    },
    {
      "pattern": "ValidationError",
      "meaning": "API request validation failed",
      "causes": ["Invalid parameter value", "Missing required parameter", "Parameter format wrong"],
      "solutions": ["Check parameter against API docs", "Provide all required parameters", "Fix parameter format"]
    },
    {
      "pattern": "ResourceNotFoundException",
      "meaning": "Requested resource doesn't exist",
      "causes": ["Resource deleted", "Wrong region", "Typo in resource name/ARN"],
      "solutions": ["Verify resource exists", "Check --region flag", "Verify resource name/ARN"]
    },
    {
      "pattern": "ThrottlingException",
      "meaning": "API rate limit exceeded",
      "causes": ["Too many requests", "Account throttled", "Service limit hit"],
      "solutions": ["Implement exponential backoff", "Request limit increase", "Batch operations"]
    },
    {
      "pattern": "Could not connect to the endpoint URL",
      "meaning": "Cannot reach AWS service endpoint",
      "causes": ["Network issue", "Wrong region", "VPC endpoint missing"],
      "solutions": ["Check network connectivity", "Verify region setting", "Create VPC endpoint"]
    }
  ],
  "debugging_commands": [
    "aws sts get-caller-identity",
    "aws configure list",
    "aws iam get-user",
    "aws --debug <command>"
  ],
  "documentation_urls": [
    "https://docs.aws.amazon.com/cli/latest/userguide/",
    "https://github.com/aws/aws-cli"
  ]
}
