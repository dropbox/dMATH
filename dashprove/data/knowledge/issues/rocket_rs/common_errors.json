{
  "tool": "rocket_rs",
  "category": "web_framework",
  "errors": [
    {
      "pattern": "Rocket failed to launch",
      "cause": "Server startup failed",
      "solutions": [
        "Check port availability",
        "Verify configuration",
        "Review ignition errors"
      ]
    },
    {
      "pattern": "No matching routes",
      "cause": "Route not found",
      "solutions": [
        "Check #[get/post] attributes",
        "Verify mount path",
        "Review route ranking"
      ]
    },
    {
      "pattern": "Guard.*failed",
      "cause": "Request guard failed",
      "solutions": [
        "Check guard implementation",
        "Verify FromRequest trait",
        "Review guard dependencies"
      ]
    },
    {
      "pattern": "Responder.*error",
      "cause": "Response generation failed",
      "solutions": [
        "Check Responder impl",
        "Verify return type",
        "Review content type"
      ]
    },
    {
      "pattern": "Config.*error",
      "cause": "Configuration error",
      "solutions": [
        "Check Rocket.toml",
        "Verify environment variables",
        "Review figment providers"
      ]
    },
    {
      "pattern": "Fairings.*failed",
      "cause": "Fairing execution error",
      "solutions": [
        "Check fairing implementation",
        "Verify attach order",
        "Review fairing callbacks"
      ]
    },
    {
      "pattern": "Form.*error",
      "cause": "Form parsing failed",
      "solutions": [
        "Check Form<T> struct",
        "Verify field attributes",
        "Review Content-Type"
      ]
    },
    {
      "pattern": "State.*not managed",
      "cause": "State not registered",
      "solutions": [
        "Use .manage() in build",
        "Check State<T> extraction",
        "Verify type matches"
      ]
    }
  ]
}
