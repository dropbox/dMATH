{
  "tool": "neovim",
  "version": "0.10.0",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "lsp_not_attached",
      "pattern": "No LSP client attached|lsp.*not.*attached",
      "message": "No LSP client attached to buffer",
      "cause": "Language server not started or configured",
      "solutions": [
        {
          "approach": "Check LSP status",
          "code": ":LspInfo",
          "when": "Verify server running"
        },
        {
          "approach": "Install server",
          "code": ":Mason | :MasonInstall lua-language-server",
          "when": "Server not installed"
        },
        {
          "approach": "Configure LSP",
          "code": "require('lspconfig').lua_ls.setup{}",
          "when": "Server not configured"
        }
      ]
    },
    {
      "id": "treesitter_missing_parser",
      "pattern": "No parser.*treesitter|parser.*not available",
      "message": "Tree-sitter parser not available",
      "cause": "Parser for language not installed",
      "solutions": [
        {
          "approach": "Install parser",
          "code": ":TSInstall python  (or language name)",
          "when": "Parser not installed"
        },
        {
          "approach": "Update parsers",
          "code": ":TSUpdate",
          "when": "Parser outdated"
        },
        {
          "approach": "Check installed",
          "code": ":TSInstallInfo",
          "when": "Verify installation"
        }
      ]
    },
    {
      "id": "plugin_not_found",
      "pattern": "module.*not found|No.*plugin.*found",
      "message": "Plugin/module not found",
      "cause": "Plugin not installed or path incorrect",
      "solutions": [
        {
          "approach": "Sync plugins (lazy.nvim)",
          "code": ":Lazy sync",
          "when": "Using lazy.nvim"
        },
        {
          "approach": "Install plugins (packer)",
          "code": ":PackerSync",
          "when": "Using packer.nvim"
        },
        {
          "approach": "Check runtimepath",
          "code": ":set rtp?",
          "when": "Verify plugin path"
        }
      ]
    },
    {
      "id": "e5108_lua_error",
      "pattern": "E5108.*Error executing lua",
      "message": "E5108: Error executing Lua",
      "cause": "Syntax error or runtime error in Lua config",
      "solutions": [
        {
          "approach": "Check Lua syntax",
          "code": "luacheck init.lua",
          "when": "Syntax error in config"
        },
        {
          "approach": "Debug with print",
          "code": "print(vim.inspect(variable))",
          "when": "Debug variable values"
        },
        {
          "approach": "Use pcall",
          "code": "local ok, err = pcall(require, 'module')\nif not ok then print(err) end",
          "when": "Catch and handle errors"
        }
      ]
    },
    {
      "id": "clipboard_not_working",
      "pattern": "clipboard.*not.*configured|E353.*no.*register",
      "message": "System clipboard not working",
      "cause": "Clipboard provider not installed",
      "solutions": [
        {
          "approach": "Check clipboard health",
          "code": ":checkhealth provider.clipboard",
          "when": "Diagnose clipboard issues"
        },
        {
          "approach": "Install clipboard provider (Linux)",
          "code": "sudo apt install xclip  # or wl-clipboard for Wayland",
          "when": "Missing clipboard tool"
        },
        {
          "approach": "Configure clipboard",
          "code": "vim.opt.clipboard = 'unnamedplus'",
          "when": "Not using system clipboard"
        }
      ]
    },
    {
      "id": "deprecated_api",
      "pattern": "deprecated|vim\\.lsp\\..*deprecated",
      "message": "Deprecated API usage",
      "cause": "Config using deprecated Neovim API",
      "solutions": [
        {
          "approach": "Update plugin",
          "code": ":Lazy update  or  :PackerUpdate",
          "when": "Plugin has deprecated calls"
        },
        {
          "approach": "Check breaking changes",
          "code": ":help deprecated",
          "when": "Find replacement APIs"
        },
        {
          "approach": "Update config",
          "code": "Replace vim.lsp.buf_* with vim.lsp.*",
          "when": "Personal config uses deprecated"
        }
      ]
    },
    {
      "id": "checkhealth_warnings",
      "pattern": "WARNING.*checkhealth|health#.*warning",
      "message": "Checkhealth warnings",
      "cause": "Missing optional dependencies or misconfigurations",
      "solutions": [
        {
          "approach": "Run checkhealth",
          "code": ":checkhealth",
          "when": "Diagnose all issues"
        },
        {
          "approach": "Check specific provider",
          "code": ":checkhealth nvim-treesitter",
          "when": "Check specific plugin"
        },
        {
          "approach": "Install missing deps",
          "code": "Follow suggestions in checkhealth output",
          "when": "Missing dependencies"
        }
      ]
    }
  ]
}
