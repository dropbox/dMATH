{
  "tool": "moonrepo",
  "category": "monorepo",
  "common_errors": [
    {
      "pattern": "error.*Project.*does not exist",
      "cause": "Project not found or moon.yml missing",
      "solutions": [
        "Ensure project has moon.yml file",
        "Check .moon/workspace.yml projects glob pattern",
        "Run 'moon project-graph' to see detected projects"
      ]
    },
    {
      "pattern": "error.*Task.*does not exist in project",
      "cause": "Task not defined in project's moon.yml",
      "solutions": [
        "Add task to project's moon.yml tasks section",
        "Check for typos in task name",
        "Inherit from .moon/tasks/*.yml if using task inheritance"
      ]
    },
    {
      "pattern": "error.*Dependency cycle detected",
      "cause": "Circular task or project dependencies",
      "solutions": [
        "Run 'moon project-graph' to visualize",
        "Remove deps that create cycle",
        "Use implicit: false for optional dependencies"
      ]
    },
    {
      "pattern": "error.*Hash.*does not match",
      "cause": "Cached output doesn't match expected hash",
      "solutions": [
        "Clear cache with 'moon clean'",
        "Check inputs in task configuration",
        "Verify outputs are deterministic"
      ]
    },
    {
      "pattern": "error.*Toolchain.*failed to install",
      "cause": "Toolchain download or installation failed",
      "solutions": [
        "Check internet connectivity",
        "Verify toolchain version in .moon/toolchain.yml",
        "Clear toolchain cache: rm -rf ~/.moon/tools"
      ]
    },
    {
      "pattern": "error.*Invalid workspace configuration",
      "cause": "Syntax error in .moon/workspace.yml",
      "solutions": [
        "Validate YAML syntax",
        "Check against moon workspace schema",
        "Ensure required fields are present"
      ]
    },
    {
      "pattern": "error.*Cannot determine VCS root",
      "cause": "Not in a git repository or VCS misconfigured",
      "solutions": [
        "Initialize git repository: git init",
        "Check vcs.manager in workspace.yml",
        "Ensure .moon/ is in repository root"
      ]
    },
    {
      "pattern": "error.*Token.*not supported",
      "cause": "Invalid token syntax in task command",
      "solutions": [
        "Use @in(index) for input files",
        "Use @out(index) for output files",
        "Check moon documentation for valid tokens"
      ]
    },
    {
      "pattern": "error.*Platform.*not configured",
      "cause": "Required platform not enabled in toolchain.yml",
      "solutions": [
        "Add platform configuration to .moon/toolchain.yml",
        "Enable node, rust, or other required platform",
        "Run 'moon setup' after configuration"
      ]
    },
    {
      "pattern": "error.*Runner.*process exited.*code 1",
      "cause": "Task command failed",
      "solutions": [
        "Check command output for actual error",
        "Run task with 'moon run --log debug'",
        "Verify command works outside moon"
      ]
    }
  ]
}
