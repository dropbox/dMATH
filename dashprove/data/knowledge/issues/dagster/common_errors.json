{
  "tool": "dagster",
  "category": "data_orchestration",
  "description": "Dagster - data orchestration platform for data assets and pipelines",
  "common_errors": [
    {
      "id": "repository_load_error",
      "pattern": "repository load error",
      "regex": "(?i)repository.*load|failed to load.*definitions|DagsterInvariantViolationError|code location",
      "cause": "Error loading Dagster definitions/repository",
      "solution": "Check for Python syntax errors, verify imports, run 'dagster dev' locally to debug",
      "severity": "high",
      "category": "definitions"
    },
    {
      "id": "asset_materialization_failed",
      "pattern": "asset materialization failed",
      "regex": "(?i)asset.*failed|materialization.*error|AssetMaterializationError|asset.*did not yield",
      "cause": "Asset materialization failed during execution",
      "solution": "Check asset function logs, verify upstream assets, review I/O manager configuration",
      "severity": "medium",
      "category": "assets"
    },
    {
      "id": "io_manager_error",
      "pattern": "I/O manager error",
      "regex": "(?i)io.*manager.*error|IOManager|failed to load.*asset|storage.*error",
      "cause": "I/O manager failed to load or store asset",
      "solution": "Check I/O manager configuration, verify storage credentials, review asset output type",
      "severity": "high",
      "category": "io_managers"
    },
    {
      "id": "resource_not_found",
      "pattern": "resource not found",
      "regex": "(?i)resource.*not found|resource.*not configured|ResourceDefinition",
      "cause": "Required resource is not configured",
      "solution": "Add resource to Definitions, check resource key matches usage in assets/ops",
      "severity": "medium",
      "category": "resources"
    },
    {
      "id": "daemon_not_running",
      "pattern": "daemon not running",
      "regex": "(?i)daemon.*not running|dagster.*daemon|sensor.*error|scheduler.*error",
      "cause": "Dagster daemon is not running (required for schedules/sensors)",
      "solution": "Start daemon with 'dagster-daemon run', check daemon logs for errors",
      "severity": "high",
      "category": "daemon"
    },
    {
      "id": "partition_error",
      "pattern": "partition error",
      "regex": "(?i)partition.*error|invalid.*partition|partition.*does not exist|PartitionsDefinition",
      "cause": "Error with asset/job partitions",
      "solution": "Verify partition key format, check partitions definition, ensure partition exists",
      "severity": "medium",
      "category": "partitions"
    },
    {
      "id": "sensor_error",
      "pattern": "sensor error",
      "regex": "(?i)sensor.*error|sensor.*failed|RunRequest.*error|SensorEvaluationError",
      "cause": "Sensor evaluation failed",
      "solution": "Check sensor function for errors, verify cursor handling, review sensor logs",
      "severity": "medium",
      "category": "sensors"
    },
    {
      "id": "type_check_failed",
      "pattern": "type check failed",
      "regex": "(?i)type check.*failed|TypeCheck|dagster.*type|unexpected.*type",
      "cause": "Dagster type check assertion failed",
      "solution": "Verify output matches expected Dagster type, check type annotations",
      "severity": "medium",
      "category": "types"
    },
    {
      "id": "database_error",
      "pattern": "database error",
      "regex": "(?i)database.*error|postgres.*error|sqlite.*error|instance.*error|event.*log",
      "cause": "Dagster instance database error",
      "solution": "Check dagster.yaml database configuration, verify database connectivity",
      "severity": "critical",
      "category": "instance"
    },
    {
      "id": "dependency_error",
      "pattern": "dependency error",
      "regex": "(?i)dependency.*error|upstream.*failed|dependency.*violated|asset.*dependency",
      "cause": "Asset dependency constraint violated",
      "solution": "Materialize upstream assets first, check asset dependencies graph",
      "severity": "medium",
      "category": "dependencies"
    }
  ],
  "related_tools": ["dbt", "airflow", "prefect", "spark"],
  "documentation_url": "https://docs.dagster.io/"
}
