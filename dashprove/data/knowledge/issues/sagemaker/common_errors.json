{
  "tool": "sagemaker",
  "category": "ml_ops",
  "common_errors": [
    {
      "pattern": "AccessDeniedException",
      "meaning": "IAM permissions insufficient for SageMaker operation",
      "causes": ["Missing IAM policy", "Role trust relationship wrong", "Resource-based policy denies"],
      "solutions": ["Attach SageMaker execution policy", "Update role trust relationship", "Check S3 bucket policy"]
    },
    {
      "pattern": "ResourceLimitExceeded",
      "meaning": "AWS service limit exceeded",
      "causes": ["Instance limit reached", "Endpoint limit hit", "Concurrent training limit"],
      "solutions": ["Request limit increase via AWS Support", "Delete unused resources", "Use different instance type"]
    },
    {
      "pattern": "UnexpectedStatusException.*Failed",
      "meaning": "Training job failed unexpectedly",
      "causes": ["Algorithm error", "Infrastructure failure", "Data access issue"],
      "solutions": ["Check CloudWatch logs", "Verify S3 data access", "Review algorithm parameters"]
    },
    {
      "pattern": "ClientError.*ValidationException",
      "meaning": "Invalid API request parameters",
      "causes": ["Missing required parameter", "Invalid value format", "Incompatible settings"],
      "solutions": ["Check API documentation", "Verify parameter types", "Review request payload"]
    },
    {
      "pattern": "ModelError.*endpoint",
      "meaning": "Model serving error on endpoint",
      "causes": ["Model loading failed", "Inference code error", "Container crash"],
      "solutions": ["Check endpoint logs", "Test model locally", "Verify container health"]
    },
    {
      "pattern": "AlgorithmError",
      "meaning": "Built-in algorithm execution failed",
      "causes": ["Data format mismatch", "Hyperparameter invalid", "Input channel missing"],
      "solutions": ["Verify data matches algorithm requirements", "Check hyperparameter ranges", "Provide all required channels"]
    },
    {
      "pattern": "S3.*Access Denied",
      "meaning": "Cannot access S3 data for training",
      "causes": ["S3 bucket policy denies", "Wrong region", "KMS key access denied"],
      "solutions": ["Update bucket policy for SageMaker", "Use same region as bucket", "Grant KMS decrypt permission"]
    },
    {
      "pattern": "CapacityError",
      "meaning": "No capacity for requested instance type",
      "causes": ["Instance type unavailable", "Spot capacity depleted", "AZ constraints"],
      "solutions": ["Try different instance type", "Use on-demand instead of spot", "Try different availability zone"]
    },
    {
      "pattern": "InternalServerError",
      "meaning": "SageMaker service error",
      "causes": ["Transient service issue", "Infrastructure problem", "Service degradation"],
      "solutions": ["Retry with exponential backoff", "Check AWS status page", "Contact AWS support"]
    },
    {
      "pattern": "Pipeline.*execution.*failed",
      "meaning": "SageMaker Pipeline execution failed",
      "causes": ["Step execution error", "Condition evaluation failed", "Parameter resolution error"],
      "solutions": ["Check step execution details", "Review pipeline definition", "Verify parameter values"]
    }
  ],
  "debugging_commands": [
    "aws sagemaker describe-training-job --training-job-name <name>",
    "aws sagemaker describe-endpoint --endpoint-name <name>",
    "aws logs get-log-events --log-group-name /aws/sagemaker/TrainingJobs",
    "aws sagemaker list-training-jobs --status-equals Failed"
  ],
  "documentation_urls": [
    "https://docs.aws.amazon.com/sagemaker/",
    "https://github.com/aws/sagemaker-python-sdk"
  ]
}
