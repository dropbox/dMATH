{
  "tool": "vscode",
  "version": "1.95.0",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "extension_host_crash",
      "pattern": "Extension host terminated unexpectedly|extension host.*crash",
      "message": "Extension host terminated unexpectedly",
      "cause": "Extension consuming too much memory or causing runtime error",
      "solutions": [
        {
          "approach": "Disable extensions",
          "code": "code --disable-extensions",
          "when": "Start VS Code without extensions to isolate issue"
        },
        {
          "approach": "Check extension logs",
          "code": "View > Output > Extension Host",
          "when": "Need to identify problematic extension"
        },
        {
          "approach": "Increase memory limit",
          "code": "\"extensions.experimental.affinity\": { \"extension-id\": 1 }",
          "when": "Extension needs more resources"
        }
      ]
    },
    {
      "id": "intellisense_not_working",
      "pattern": "IntelliSense.*not working|no suggestions|autocomplete.*broken",
      "message": "IntelliSense not working",
      "cause": "Language server not started or project not configured",
      "solutions": [
        {
          "approach": "Reload window",
          "code": "Ctrl+Shift+P > Developer: Reload Window",
          "when": "Language server crashed"
        },
        {
          "approach": "Check language server status",
          "code": "View > Output > [Language] Language Server",
          "when": "Need to see server errors"
        },
        {
          "approach": "Verify workspace config",
          "code": "Ensure tsconfig.json/jsconfig.json/pyrightconfig.json exists",
          "when": "Project configuration missing"
        }
      ]
    },
    {
      "id": "settings_sync_conflict",
      "pattern": "Settings Sync.*conflict|merge conflict.*settings",
      "message": "Settings sync conflict",
      "cause": "Local and remote settings have diverged",
      "solutions": [
        {
          "approach": "Accept remote",
          "code": "Settings Sync: Show Synced Data > Replace Local",
          "when": "Remote settings are correct"
        },
        {
          "approach": "Accept local",
          "code": "Settings Sync: Show Synced Data > Replace Remote",
          "when": "Local settings are correct"
        },
        {
          "approach": "Manual merge",
          "code": "Settings Sync: Show Synced Data > Merge",
          "when": "Need to combine both"
        }
      ]
    },
    {
      "id": "workspace_not_trusted",
      "pattern": "Workspace Trust|restricted mode|not trusted",
      "message": "Workspace is not trusted",
      "cause": "VS Code running in restricted mode for untrusted folders",
      "solutions": [
        {
          "approach": "Trust workspace",
          "code": "Click 'Trust' in the banner or Ctrl+Shift+P > Workspaces: Manage Workspace Trust",
          "when": "You trust the folder contents"
        },
        {
          "approach": "Configure trusted folders",
          "code": "\"security.workspace.trust.untrustedFiles\": \"open\"",
          "when": "Want to always open files"
        },
        {
          "approach": "Disable trust feature",
          "code": "\"security.workspace.trust.enabled\": false",
          "when": "Want to disable entirely (not recommended)"
        }
      ]
    },
    {
      "id": "git_not_found",
      "pattern": "Git.*not found|git is not installed|unable to find git",
      "message": "Git not found in PATH",
      "cause": "Git not installed or not in system PATH",
      "solutions": [
        {
          "approach": "Install Git",
          "code": "brew install git  # macOS\nwinget install Git.Git  # Windows",
          "when": "Git not installed"
        },
        {
          "approach": "Configure Git path",
          "code": "\"git.path\": \"/usr/local/bin/git\"",
          "when": "Git installed but not in PATH"
        },
        {
          "approach": "Restart VS Code",
          "code": "Close and reopen VS Code after installing Git",
          "when": "Git just installed"
        }
      ]
    },
    {
      "id": "eslint_config_error",
      "pattern": "ESLint.*configuration|failed to load config|eslint.*error",
      "message": "ESLint configuration error",
      "cause": "Invalid or missing ESLint configuration",
      "solutions": [
        {
          "approach": "Initialize ESLint",
          "code": "npm init @eslint/config",
          "when": "No ESLint config exists"
        },
        {
          "approach": "Check config syntax",
          "code": "npx eslint --print-config file.js",
          "when": "Config may be invalid"
        },
        {
          "approach": "Install dependencies",
          "code": "npm install --save-dev eslint @eslint/js",
          "when": "ESLint packages missing"
        }
      ]
    },
    {
      "id": "file_watcher_limit",
      "pattern": "ENOSPC.*file watchers|too many open files|inotify",
      "message": "File watcher limit reached",
      "cause": "System limit on file watchers exceeded",
      "solutions": [
        {
          "approach": "Increase limit (Linux)",
          "code": "echo fs.inotify.max_user_watches=524288 | sudo tee -a /etc/sysctl.conf && sudo sysctl -p",
          "when": "On Linux systems"
        },
        {
          "approach": "Exclude folders",
          "code": "\"files.watcherExclude\": { \"**/node_modules/**\": true }",
          "when": "Too many files being watched"
        },
        {
          "approach": "Use simple watcher",
          "code": "\"files.legacyWatcher\": \"on\"",
          "when": "Advanced watcher has issues"
        }
      ]
    }
  ]
}
