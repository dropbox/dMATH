{
  "tool": "unzip",
  "version": "6.0",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "cannot_find_file",
      "pattern": "cannot find|No such file|does not exist",
      "message": "Archive file not found",
      "cause": "Wrong path or filename",
      "solutions": [
        {
          "approach": "Check path",
          "code": "ls -la archive.zip\nfile archive.zip",
          "when": "Verify exists"
        },
        {
          "approach": "Tab completion",
          "code": "unzip arch<TAB>  # Tab complete filename",
          "when": "Find file"
        }
      ]
    },
    {
      "id": "wrong_password",
      "pattern": "incorrect password|password|skipping.*encrypted|bad CRC",
      "message": "Wrong password",
      "cause": "Archive encrypted with different password",
      "solutions": [
        {
          "approach": "Provide password",
          "code": "unzip -P 'password' archive.zip",
          "when": "With password"
        },
        {
          "approach": "Interactive",
          "code": "unzip archive.zip  # Prompts for password",
          "when": "Enter manually"
        },
        {
          "approach": "Test",
          "code": "unzip -t archive.zip  # Test without extracting",
          "when": "Verify"
        }
      ]
    },
    {
      "id": "overwrite_prompt",
      "pattern": "overwrite|replace|already exists",
      "message": "Files already exist",
      "cause": "Destination files exist",
      "solutions": [
        {
          "approach": "Overwrite all",
          "code": "unzip -o archive.zip",
          "when": "Replace all"
        },
        {
          "approach": "Never overwrite",
          "code": "unzip -n archive.zip",
          "when": "Keep existing"
        },
        {
          "approach": "Only newer",
          "code": "unzip -u archive.zip  # Update: extract only newer",
          "when": "Update only"
        },
        {
          "approach": "To directory",
          "code": "unzip archive.zip -d /path/to/output/",
          "when": "Different dest"
        }
      ]
    },
    {
      "id": "corrupted_archive",
      "pattern": "bad CRC|CRC error|corrupted|truncated",
      "message": "Archive is corrupted",
      "cause": "Download error or file damage",
      "solutions": [
        {
          "approach": "Test integrity",
          "code": "unzip -t archive.zip",
          "when": "Check corruption"
        },
        {
          "approach": "Re-download",
          "code": "# Download again with verification\ncurl -O URL\nmd5sum archive.zip",
          "when": "Get fresh copy"
        },
        {
          "approach": "Partial extract",
          "code": "unzip -F archive.zip  # Fix minor corruption\nunzip -FF archive.zip -d output/  # More aggressive",
          "when": "Salvage data"
        }
      ]
    },
    {
      "id": "unsupported_compression",
      "pattern": "unsupported compression|unknown compression|method",
      "message": "Unknown compression method",
      "cause": "Archive uses newer compression method",
      "solutions": [
        {
          "approach": "Update unzip",
          "code": "# Ubuntu/Debian\nsudo apt update && sudo apt upgrade unzip",
          "when": "Upgrade tool"
        },
        {
          "approach": "Use 7z",
          "code": "7z x archive.zip  # Supports more formats",
          "when": "Alternative"
        },
        {
          "approach": "Check format",
          "code": "file archive.zip\nzipinfo archive.zip",
          "when": "Identify"
        }
      ]
    },
    {
      "id": "filename_encoding",
      "pattern": "filename|encoding|UTF-8|unicode|garbled",
      "message": "Filenames display incorrectly",
      "cause": "Character encoding mismatch",
      "solutions": [
        {
          "approach": "Specify encoding",
          "code": "unzip -O CP932 archive.zip  # Japanese\nunzip -O GBK archive.zip  # Chinese",
          "when": "Known encoding"
        },
        {
          "approach": "Convert names",
          "code": "convmv -f CP932 -t UTF-8 --notest extracted/*",
          "when": "After extract"
        },
        {
          "approach": "Use 7z",
          "code": "LANG=ja_JP.SJIS 7z x archive.zip",
          "when": "Better handling"
        }
      ]
    }
  ]
}
