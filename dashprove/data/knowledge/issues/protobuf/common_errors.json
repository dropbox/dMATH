{
  "tool": "protobuf",
  "category": "serialization",
  "common_errors": [
    {
      "error_pattern": "Message type .* not found",
      "description": "Protobuf compiler cannot find referenced message type",
      "causes": [
        "Missing import statement",
        "Typo in message name",
        "Wrong package prefix",
        "Proto file not in include path"
      ],
      "solutions": [
        "Add import statement: import \"path/to/file.proto\"",
        "Verify message name spelling and case",
        "Use fully qualified name: package.MessageName",
        "Add proto path: protoc -I=./protos"
      ]
    },
    {
      "error_pattern": "Field number \\d+ has already been used",
      "description": "Duplicate field number in message definition",
      "causes": [
        "Copy-paste error",
        "Concurrent editing conflict",
        "Reusing reserved field number"
      ],
      "solutions": [
        "Assign unique field number to each field",
        "Use reserved statement for deprecated fields",
        "Review field numbers sequentially"
      ]
    },
    {
      "error_pattern": "Expected.*but found",
      "description": "Syntax error in proto file",
      "causes": [
        "Missing semicolon",
        "Unclosed brace or bracket",
        "Invalid keyword usage",
        "Wrong proto syntax version"
      ],
      "solutions": [
        "Check for missing semicolons after statements",
        "Verify matching braces {}",
        "Add syntax = \"proto3\" at file start",
        "Use proto linter: buf lint"
      ]
    },
    {
      "error_pattern": "Non-repeated field.*cannot be packed",
      "description": "Invalid packed option on non-repeated field",
      "causes": [
        "packed option on singular field",
        "packed on non-numeric type"
      ],
      "solutions": [
        "Only use packed on repeated numeric fields",
        "Remove packed option from singular fields",
        "Proto3 uses packed by default for repeated numerics"
      ]
    },
    {
      "error_pattern": "Import .* was not found",
      "description": "Cannot locate imported proto file",
      "causes": [
        "File doesn't exist at specified path",
        "Missing -I/--proto_path flag",
        "Relative path resolution issue"
      ],
      "solutions": [
        "Verify import path is correct",
        "Add proto_path: protoc -I=./protos -I=/usr/include",
        "Use absolute path or correct relative path",
        "Check GOPATH/src for Go projects"
      ]
    },
    {
      "error_pattern": "Unresolved reference",
      "description": "IDE or compiler cannot resolve proto reference",
      "causes": [
        "Missing proto file in project",
        "IDE proto plugin not configured",
        "Circular import dependency"
      ],
      "solutions": [
        "Add all proto files to project",
        "Configure IDE proto plugin paths",
        "Break circular imports into separate files"
      ]
    },
    {
      "error_pattern": "reserved.*tag.*conflict",
      "description": "Using a reserved field number or name",
      "causes": [
        "New field uses reserved number",
        "New field uses reserved name",
        "Forgot about previously reserved fields"
      ],
      "solutions": [
        "Use different field number",
        "Check reserved declarations",
        "Document why numbers were reserved"
      ]
    },
    {
      "error_pattern": "Enum value .* is already used",
      "description": "Duplicate enum value number",
      "causes": [
        "Same numeric value for different enum entries",
        "Allow_alias not set for intentional aliases"
      ],
      "solutions": [
        "Use unique numbers for enum values",
        "Add option allow_alias = true; if aliasing intended",
        "Start enum with _UNSPECIFIED = 0 in proto3"
      ]
    },
    {
      "error_pattern": "cannot convert.*to.*type",
      "description": "Type mismatch during proto parsing/serialization",
      "causes": [
        "Field type changed incompatibly",
        "Wrong message type for deserialization",
        "Binary format corruption"
      ],
      "solutions": [
        "Never change field types incompatibly",
        "Create new field for type changes",
        "Verify correct message type for deserialization",
        "Check for data corruption"
      ]
    },
    {
      "error_pattern": "ParseFromString.*failed",
      "description": "Failed to parse binary proto data",
      "causes": [
        "Corrupted or incomplete binary data",
        "Wrong message type",
        "Version mismatch in schema"
      ],
      "solutions": [
        "Validate data before parsing",
        "Verify message type matches",
        "Use ParsePartialFromString for partial data",
        "Check for transmission errors"
      ]
    },
    {
      "error_pattern": "Extension number \\d+ is out of range",
      "description": "Extension field number outside allowed range",
      "causes": [
        "Extension number not in extensions range",
        "Using reserved extension numbers"
      ],
      "solutions": [
        "Define extensions range in message",
        "Use number within declared range",
        "extensions 100 to 199 in base message"
      ]
    },
    {
      "error_pattern": "google.protobuf.*not found",
      "description": "Cannot find well-known types",
      "causes": [
        "Missing Google proto includes",
        "Wrong import path",
        "Proto compiler doesn't include well-known types"
      ],
      "solutions": [
        "Import with: import \"google/protobuf/timestamp.proto\"",
        "Add Google proto path: protoc -I=/usr/include",
        "Install protobuf includes package"
      ]
    }
  ]
}
