{
  "tool": "goenv",
  "category": "version_manager",
  "common_errors": [
    {
      "pattern": "goenv: command not found",
      "cause": "goenv not initialized in shell",
      "solutions": [
        "Add to profile: export GOENV_ROOT=\"$HOME/.goenv\"",
        "Add to PATH: export PATH=\"$GOENV_ROOT/bin:$PATH\"",
        "Init: eval \"$(goenv init -)\""
      ]
    },
    {
      "pattern": "goenv: version.*not installed",
      "cause": "Go version not installed via goenv",
      "solutions": [
        "Install: 'goenv install <version>'",
        "List available: 'goenv install -l'",
        "Update definitions: git pull in goenv repo"
      ]
    },
    {
      "pattern": "goenv: GOPATH.*not set",
      "cause": "GOPATH environment variable not configured",
      "solutions": [
        "Set GOPATH: export GOPATH=\"$HOME/go\"",
        "Enable goenv-managed GOPATH: export GOENV_GOPATH_PREFIX=\"$HOME/go\"",
        "Add GOPATH/bin to PATH"
      ]
    },
    {
      "pattern": "goenv:.*no version file found",
      "cause": "No .go-version in directory tree",
      "solutions": [
        "Create .go-version: goenv local <version>",
        "Set global: goenv global <version>",
        "Set GOENV_VERSION env var"
      ]
    },
    {
      "pattern": "download.*checksum mismatch",
      "cause": "Downloaded Go archive corrupted",
      "solutions": [
        "Retry the installation",
        "Clear goenv cache",
        "Check network stability"
      ]
    },
    {
      "pattern": "goenv:.*shims.*not in PATH",
      "cause": "goenv shims not first in PATH",
      "solutions": [
        "Ensure goenv init is evaluated in shell",
        "Check PATH order: shims should come first",
        "Verify with 'which go'"
      ]
    },
    {
      "pattern": "definition not found",
      "cause": "Go version not in available definitions",
      "solutions": [
        "Update goenv: git pull in ~/.goenv",
        "Check for typos in version",
        "List available: goenv install -l"
      ]
    },
    {
      "pattern": "GOROOT.*mismatch",
      "cause": "GOROOT set incorrectly or conflicting",
      "solutions": [
        "Unset GOROOT: unset GOROOT",
        "Let goenv manage GOROOT",
        "Check for conflicting Go installations"
      ]
    },
    {
      "pattern": "go: cannot find GOROOT",
      "cause": "Go installation incomplete or corrupted",
      "solutions": [
        "Reinstall version: goenv uninstall && goenv install",
        "Check ~/.goenv/versions/<version>",
        "Verify goenv init sets GOROOT"
      ]
    },
    {
      "pattern": "permission denied.*goenv",
      "cause": "Permission issues with goenv directories",
      "solutions": [
        "Fix permissions: chmod -R u+w ~/.goenv",
        "Don't use sudo with goenv",
        "Check GOENV_ROOT ownership"
      ]
    }
  ]
}
