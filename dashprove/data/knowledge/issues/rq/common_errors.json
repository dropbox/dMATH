{
  "tool": "rq",
  "category": "task_queue",
  "description": "Python Redis Queue",
  "errors": [
    {
      "pattern": "ConnectionError.*Connection refused",
      "meaning": "Cannot connect to Redis",
      "causes": ["Redis not running", "Wrong connection params", "Network issue"],
      "solutions": ["Start Redis: redis-server", "Check Redis URL/host/port", "Verify network access"]
    },
    {
      "pattern": "ModuleNotFoundError.*No module named",
      "meaning": "Worker can't import job function",
      "causes": ["Function not in Python path", "Worker in different environment", "Import error in module"],
      "solutions": ["Start worker from project root", "Install dependencies in worker env", "Check import statements"]
    },
    {
      "pattern": "Job.*moved to failed queue",
      "meaning": "Job execution failed",
      "causes": ["Exception in job function", "Dependency failure", "Timeout exceeded"],
      "solutions": ["Check failed queue: rq info", "Review job exception", "Fix job code"]
    },
    {
      "pattern": "DequeueTimeout|WorkerLostError",
      "meaning": "Worker died or timed out",
      "causes": ["Job exceeded timeout", "Worker process killed", "System resource issue"],
      "solutions": ["Increase job timeout", "Check system resources", "Monitor worker health"]
    },
    {
      "pattern": "Cannot pickle|serialization error",
      "meaning": "Job argument not serializable",
      "causes": ["Non-picklable argument", "Lambda or closure passed", "Complex object"],
      "solutions": ["Pass only basic types", "Use job ID to look up data", "Serialize manually"]
    },
    {
      "pattern": "Queue.*not found|No such queue",
      "meaning": "Queue doesn't exist",
      "causes": ["Queue name typo", "Queue not created", "Different Redis database"],
      "solutions": ["Check queue name spelling", "Verify Redis database number", "List queues: rq info"]
    },
    {
      "pattern": "Job.*is not callable",
      "meaning": "Invalid job function",
      "causes": ["String path to function wrong", "Function doesn't exist", "Typo in function name"],
      "solutions": ["Use direct function reference", "Check function exists", "Verify module path"]
    },
    {
      "pattern": "InvalidJobDependency",
      "meaning": "Job dependency issue",
      "causes": ["Dependent job doesn't exist", "Circular dependency", "Dependency already failed"],
      "solutions": ["Verify dependency jobs exist", "Check for circular deps", "Handle dependency failures"]
    }
  ]
}
