{
  "tool": "sonarqube",
  "category": "sast",
  "errors": [
    {
      "pattern": "Not authorized",
      "cause": "Invalid or missing authentication token",
      "solutions": [
        "Generate token in SonarQube UI",
        "Set SONAR_TOKEN environment variable",
        "Use -Dsonar.login parameter",
        "Check token permissions"
      ]
    },
    {
      "pattern": "Project not found",
      "cause": "Project key does not exist in SonarQube",
      "solutions": [
        "Create project in SonarQube UI first",
        "Verify sonar.projectKey value",
        "Check user has access to project"
      ]
    },
    {
      "pattern": "Unable to connect",
      "cause": "Cannot reach SonarQube server",
      "solutions": [
        "Verify sonar.host.url is correct",
        "Check network connectivity",
        "Ensure SonarQube service is running"
      ]
    },
    {
      "pattern": "Quality Gate.*FAILED",
      "cause": "Code quality metrics below threshold",
      "solutions": [
        "Review failing conditions in SonarQube",
        "Fix code issues or adjust thresholds",
        "Check coverage requirements"
      ]
    },
    {
      "pattern": "No files.*analyzed",
      "cause": "Scanner found no source files",
      "solutions": [
        "Check sonar.sources path",
        "Verify language plugin is installed",
        "Review file inclusion patterns"
      ]
    },
    {
      "pattern": "Out of memory",
      "cause": "Scanner exceeded heap space",
      "solutions": [
        "Increase SONAR_SCANNER_OPTS heap size",
        "Analyze smaller codebases",
        "Exclude generated/vendor code"
      ]
    },
    {
      "pattern": "Coverage report.*not found",
      "cause": "Coverage file path is incorrect",
      "solutions": [
        "Generate coverage report first",
        "Set correct sonar.coverage.jacoco.xmlReportPaths",
        "Check file exists at specified path"
      ]
    },
    {
      "pattern": "Duplicate project key",
      "cause": "Project key already in use",
      "solutions": [
        "Use unique project key",
        "Delete existing project first",
        "Append branch/version to key"
      ]
    },
    {
      "pattern": "Plugin.*incompatible",
      "cause": "Plugin version mismatch with SonarQube",
      "solutions": [
        "Update plugin to compatible version",
        "Check SonarQube compatibility matrix",
        "Upgrade/downgrade SonarQube"
      ]
    },
    {
      "pattern": "SCM provider.*failed",
      "cause": "Git/SVN integration error",
      "solutions": [
        "Run from within repository root",
        "Disable SCM with sonar.scm.disabled=true",
        "Check git is accessible"
      ]
    }
  ]
}
