{
  "tool": "mysql",
  "category": "database",
  "common_errors": [
    {
      "pattern": "Access denied for user",
      "meaning": "Authentication failed",
      "causes": ["Wrong password", "User doesn't exist", "Host not allowed"],
      "solutions": ["Verify password", "Create user: CREATE USER", "Grant access: GRANT ALL ON db.* TO user@host"]
    },
    {
      "pattern": "Can't connect to MySQL server",
      "meaning": "Cannot establish connection to MySQL",
      "causes": ["Server not running", "Wrong host/port", "Firewall blocking"],
      "solutions": ["Start MySQL: systemctl start mysql", "Check host and port", "Open port 3306 in firewall"]
    },
    {
      "pattern": "Table.*doesn't exist",
      "meaning": "Referenced table not found",
      "causes": ["Table not created", "Wrong database", "Case sensitivity issue"],
      "solutions": ["Create the table", "Use correct database: USE dbname", "Check table name case"]
    },
    {
      "pattern": "Duplicate entry.*for key",
      "meaning": "Primary key or unique constraint violation",
      "causes": ["Inserting duplicate value", "Concurrent inserts", "Auto-increment collision"],
      "solutions": ["Use INSERT IGNORE or ON DUPLICATE KEY UPDATE", "Check unique constraints", "Reset auto-increment if needed"]
    },
    {
      "pattern": "Lock wait timeout exceeded",
      "meaning": "Transaction waiting too long for row lock",
      "causes": ["Long-running transaction holding lock", "Deadlock", "Large batch operation"],
      "solutions": ["Increase innodb_lock_wait_timeout", "Kill blocking transaction", "Break into smaller batches"]
    },
    {
      "pattern": "Deadlock found",
      "meaning": "Circular lock dependency detected",
      "causes": ["Concurrent transactions with conflicting locks", "Inconsistent lock order", "Gap locks"],
      "solutions": ["Retry transaction", "Access tables in consistent order", "Use lower isolation level"]
    },
    {
      "pattern": "Too many connections",
      "meaning": "Connection pool exhausted",
      "causes": ["max_connections limit hit", "Connection leaks", "Too many clients"],
      "solutions": ["Increase max_connections", "Close unused connections", "Use connection pooling"]
    },
    {
      "pattern": "The table.*is full",
      "meaning": "Table cannot grow further",
      "causes": ["Disk full", "Table size limit reached", "Temp table limit"],
      "solutions": ["Free disk space", "Increase table size limit", "Increase tmp_table_size"]
    },
    {
      "pattern": "Foreign key constraint fails",
      "meaning": "FK reference violation",
      "causes": ["Parent row doesn't exist", "Deleting parent with children", "Wrong reference column"],
      "solutions": ["Insert parent row first", "Use ON DELETE CASCADE", "Verify FK column types match"]
    },
    {
      "pattern": "Incorrect.*value.*for column",
      "meaning": "Data type mismatch or invalid value",
      "causes": ["Wrong data type", "Value out of range", "NULL in NOT NULL column"],
      "solutions": ["Use correct data type", "Check value constraints", "Provide non-NULL value"]
    }
  ],
  "debugging_commands": [
    "SHOW PROCESSLIST;",
    "SHOW ENGINE INNODB STATUS;",
    "SHOW VARIABLES LIKE '%connections%';",
    "EXPLAIN SELECT ...;",
    "SHOW FULL TABLES;"
  ],
  "documentation_urls": [
    "https://dev.mysql.com/doc/",
    "https://github.com/mysql/mysql-server"
  ]
}
