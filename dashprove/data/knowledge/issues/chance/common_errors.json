{
  "tool": "chance",
  "category": "test_data_generation",
  "common_errors": [
    {
      "error_pattern": "Chance is not defined",
      "description": "Chance library not loaded",
      "causes": [
        "Script not included",
        "Module not imported",
        "CDN load failed"
      ],
      "solutions": [
        "Import: const Chance = require('chance')",
        "ES6: import Chance from 'chance'",
        "CDN: <script src=\"https://chancejs.com/chance.min.js\">",
        "npm install chance"
      ]
    },
    {
      "error_pattern": "chance\\..* is not a function",
      "description": "Method doesn't exist on Chance instance",
      "causes": [
        "Typo in method name",
        "Using Chance class instead of instance",
        "Method not available in version"
      ],
      "solutions": [
        "Create instance: const chance = new Chance()",
        "Check method name: chance.name(), chance.email()",
        "Review documentation for available methods"
      ]
    },
    {
      "error_pattern": "seed.*expects.*number",
      "description": "Invalid seed provided to Chance",
      "causes": [
        "Non-numeric seed value",
        "String seed not auto-converted"
      ],
      "solutions": [
        "Use numeric seed: new Chance(12345)",
        "Convert string: new Chance(parseInt(seedStr))",
        "Use string seed (will be converted): new Chance('my-seed')"
      ]
    },
    {
      "error_pattern": "options\\..*is required",
      "description": "Missing required option for method",
      "causes": [
        "Method requires options object",
        "Missing required property"
      ],
      "solutions": [
        "Provide options: chance.string({ length: 5 })",
        "Check documentation for required options"
      ]
    },
    {
      "error_pattern": "min.*greater.*max",
      "description": "Invalid min/max range",
      "causes": [
        "min value greater than max",
        "Range inverted"
      ],
      "solutions": [
        "Fix range: chance.integer({ min: 1, max: 100 })",
        "Ensure min < max in options"
      ]
    },
    {
      "error_pattern": "Cannot read property.*undefined",
      "description": "Accessing undefined result",
      "causes": [
        "Method returned undefined",
        "Chaining on null result",
        "Pool empty in pick"
      ],
      "solutions": [
        "Check result before use",
        "Ensure array not empty for pick",
        "Verify method returns expected type"
      ]
    },
    {
      "error_pattern": "unique.*exhausted",
      "description": "No more unique values in finite set",
      "causes": [
        "Called unique too many times",
        "Pool too small",
        "Constrained value space"
      ],
      "solutions": [
        "Reset unique tracker",
        "Increase pool size with options",
        "Use unique with comparator: chance.unique(fn, n)"
      ]
    },
    {
      "error_pattern": "Invalid.*locale",
      "description": "Locale not supported",
      "causes": [
        "Using unsupported locale code",
        "Mixin not loaded for locale"
      ],
      "solutions": [
        "Use default locale",
        "Create custom locale mixin",
        "Check available locales in documentation"
      ]
    },
    {
      "error_pattern": "mixin.*not a function",
      "description": "Invalid mixin registration",
      "causes": [
        "Mixin is not a function",
        "Mixin already registered",
        "Wrong mixin format"
      ],
      "solutions": [
        "Provide function: chance.mixin({ custom: function() {} })",
        "Mixin should be object with method functions"
      ]
    },
    {
      "error_pattern": "pool.*undefined",
      "description": "Pool for weighted selection invalid",
      "causes": [
        "Weights don't match pool",
        "Empty pool array"
      ],
      "solutions": [
        "Provide valid pool: chance.weighted(['a','b'], [1,2])",
        "Ensure pool and weights same length"
      ]
    }
  ]
}
