{
  "tool": "neptune",
  "category": "ml_ops",
  "common_errors": [
    {
      "pattern": "NeptuneApiTokenNotProvided",
      "meaning": "Neptune API token not configured",
      "causes": ["NEPTUNE_API_TOKEN not set", "Token not in config file", "Environment variable missing"],
      "solutions": ["Set NEPTUNE_API_TOKEN environment variable", "Add token via neptune init", "Get token from Neptune UI"]
    },
    {
      "pattern": "NeptuneProjectNotFound",
      "meaning": "Specified project doesn't exist",
      "causes": ["Project name typo", "Project not created", "Workspace mismatch"],
      "solutions": ["Verify project name: workspace/project", "Create project in Neptune UI", "Check workspace access"]
    },
    {
      "pattern": "NeptuneRunNotFound",
      "meaning": "Referenced run doesn't exist",
      "causes": ["Run ID incorrect", "Run deleted", "Run in different project"],
      "solutions": ["Verify run ID in Neptune UI", "Check run wasn't archived/deleted", "Ensure correct project context"]
    },
    {
      "pattern": "NeptuneConnectionLostException",
      "meaning": "Lost connection to Neptune servers",
      "causes": ["Network interruption", "Server maintenance", "Firewall blocking"],
      "solutions": ["Check network connectivity", "Retry after brief wait", "Verify no firewall blocks"]
    },
    {
      "pattern": "NeptuneMissingApiTokenException",
      "meaning": "API authentication failed",
      "causes": ["Token invalid or expired", "Token lacks permissions", "Wrong token type"],
      "solutions": ["Generate new API token", "Verify token has project access", "Use API token not OAuth"]
    },
    {
      "pattern": "NeptuneLimitExceeded",
      "meaning": "Resource quota exceeded",
      "causes": ["Storage limit reached", "Run limit exceeded", "Concurrent sync limit"],
      "solutions": ["Upgrade plan or delete old data", "Archive completed runs", "Reduce parallel operations"]
    },
    {
      "pattern": "NeptuneFieldCountLimitExceeded",
      "meaning": "Too many fields in a single run",
      "causes": ["Logging too many metrics", "Dynamic field names", "Uncontrolled logging loops"],
      "solutions": ["Reduce number of logged fields", "Use static field names", "Aggregate metrics before logging"]
    },
    {
      "pattern": "NeptuneUnprocessableEntityException",
      "meaning": "Invalid data format sent to API",
      "causes": ["Field name invalid", "Value type unsupported", "Nested structure too deep"],
      "solutions": ["Check field naming conventions", "Convert to supported type", "Flatten nested structures"]
    },
    {
      "pattern": "NeptuneOfflineModeSyncError",
      "meaning": "Cannot sync offline run data",
      "causes": ["Offline data corrupted", "Run already exists", "Version mismatch"],
      "solutions": ["Check offline sync directory", "Use new run ID for sync", "Update neptune client"]
    },
    {
      "pattern": "NeptuneModelNotFound",
      "meaning": "Referenced model doesn't exist",
      "causes": ["Model ID incorrect", "Model deleted", "Model in different project"],
      "solutions": ["Verify model ID", "Check model registry in UI", "Ensure correct project"]
    }
  ],
  "debugging_commands": [
    "neptune sync --list",
    "neptune status",
    "neptune clear"
  ],
  "documentation_urls": [
    "https://docs.neptune.ai/",
    "https://github.com/neptune-ai/neptune-client"
  ]
}
