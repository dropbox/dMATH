{
  "tool": "dvc",
  "category": "ml_ops",
  "common_errors": [
    {
      "pattern": "ERROR.*failed to pull",
      "meaning": "Cannot fetch data from remote storage",
      "causes": ["Remote not configured", "Credentials invalid", "File not in remote"],
      "solutions": ["Configure remote: dvc remote add", "Update storage credentials", "Push data first: dvc push"]
    },
    {
      "pattern": "ERROR.*output.*already tracked by SCM",
      "meaning": "File tracked by both Git and DVC",
      "causes": ["Added to Git before DVC", "Forgot to add to .gitignore", "DVC file conflict"],
      "solutions": ["Remove from Git: git rm --cached", "Run dvc add again", "Check .gitignore includes .dvc files"]
    },
    {
      "pattern": "ERROR.*checkout.*failed",
      "meaning": "Cannot restore files from DVC cache",
      "causes": ["Cache missing files", "Hash mismatch", "File locked by another process"],
      "solutions": ["Pull from remote: dvc pull", "Clear cache: dvc gc", "Close applications using files"]
    },
    {
      "pattern": "ERROR.*stage.*changed",
      "meaning": "Pipeline stage dependencies changed unexpectedly",
      "causes": ["Input file modified", "Code dependency changed", "Parameter changed"],
      "solutions": ["Run dvc repro to update", "Check dvc.lock for changes", "Commit or stash changes first"]
    },
    {
      "pattern": "ERROR.*cyclic dependency",
      "meaning": "Pipeline has circular dependencies",
      "causes": ["Output used as input in same stage", "Multi-stage circular reference"],
      "solutions": ["Review dvc.yaml for cycles", "Break cycle by splitting stages", "Remove conflicting dependency"]
    },
    {
      "pattern": "ERROR.*remote.*access denied",
      "meaning": "Storage permission error",
      "causes": ["Bucket permissions insufficient", "IAM role missing", "Credentials expired"],
      "solutions": ["Update bucket/storage permissions", "Attach required IAM policy", "Refresh credentials"]
    },
    {
      "pattern": "ERROR.*cache.*hash mismatch",
      "meaning": "Cached file corrupted or tampered",
      "causes": ["File modified after caching", "Cache corruption", "Hash collision (rare)"],
      "solutions": ["Remove from cache and re-add", "Run dvc gc to clean cache", "Pull fresh from remote"]
    },
    {
      "pattern": "ERROR.*not a DVC repository",
      "meaning": "DVC not initialized in directory",
      "causes": ["Not in project root", "DVC never initialized", ".dvc directory missing"],
      "solutions": ["Navigate to project root", "Run dvc init", "Clone repository properly"]
    },
    {
      "pattern": "ERROR.*run.*failed",
      "meaning": "Pipeline stage execution failed",
      "causes": ["Command returned non-zero", "Missing dependency", "Environment issue"],
      "solutions": ["Check stage command and logs", "Verify all deps exist", "Test command manually first"]
    },
    {
      "pattern": "ERROR.*merge conflict.*dvc.lock",
      "meaning": "DVC lock file has Git merge conflict",
      "causes": ["Parallel changes to pipeline", "Different experiments merged"],
      "solutions": ["Resolve dvc.lock manually", "Run dvc repro after merge", "Use dvc exp tools for experiments"]
    }
  ],
  "debugging_commands": [
    "dvc status",
    "dvc diff",
    "dvc dag",
    "dvc remote list",
    "dvc cache dir"
  ],
  "documentation_urls": [
    "https://dvc.org/doc",
    "https://github.com/iterative/dvc"
  ]
}
