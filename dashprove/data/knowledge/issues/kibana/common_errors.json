{
  "tool": "kibana",
  "category": "logging",
  "common_errors": [
    {
      "pattern": "Unable to connect to Elasticsearch",
      "meaning": "Kibana cannot reach Elasticsearch",
      "causes": ["ES not running", "Wrong URL", "SSL issue", "Auth failure"],
      "solutions": ["Start Elasticsearch", "Check elasticsearch.hosts in kibana.yml", "Configure SSL correctly", "Verify credentials"]
    },
    {
      "pattern": "Kibana server is not ready yet",
      "meaning": "Kibana still initializing",
      "causes": ["ES not ready", "Index migration", "Plugin initialization"],
      "solutions": ["Wait for ES to be green", "Check Kibana logs", "Wait for initialization to complete"]
    },
    {
      "pattern": "index.*not found",
      "meaning": "Referenced index doesn't exist",
      "causes": ["Index deleted", "Wrong index pattern", "Index not created yet"],
      "solutions": ["Create index", "Update index pattern", "Wait for data ingestion"]
    },
    {
      "pattern": "authentication.*error",
      "meaning": "Kibana authentication failed",
      "causes": ["Wrong credentials", "Security plugin issue", "Session expired"],
      "solutions": ["Verify username/password", "Check security configuration", "Clear cookies and retry"]
    },
    {
      "pattern": "saved object.*not found",
      "meaning": "Dashboard/visualization/search not found",
      "causes": ["Object deleted", "Wrong space", "Export/import issue"],
      "solutions": ["Check saved objects list", "Switch to correct space", "Re-import objects"]
    },
    {
      "pattern": "field.*not indexed",
      "meaning": "Field not available for aggregation",
      "causes": ["Field not indexed", "Wrong field type", "Index pattern needs refresh"],
      "solutions": ["Update mapping to index field", "Check field type in mapping", "Refresh index pattern"]
    },
    {
      "pattern": "circuit_breaker.*triggered",
      "meaning": "Query exceeded memory limit",
      "causes": ["Query too large", "Too many buckets", "Large time range"],
      "solutions": ["Reduce query scope", "Limit aggregation buckets", "Narrow time range"]
    },
    {
      "pattern": "version.*incompatible",
      "meaning": "Kibana/Elasticsearch version mismatch",
      "causes": ["Different major versions", "Upgrade incomplete"],
      "solutions": ["Use matching versions", "Complete upgrade process"]
    },
    {
      "pattern": "space.*quota exceeded",
      "meaning": "Space storage limit reached",
      "causes": ["Too many saved objects", "Space quota configured", "Large dashboards"],
      "solutions": ["Delete unused objects", "Increase space quota", "Archive old dashboards"]
    },
    {
      "pattern": "plugin.*failed",
      "meaning": "Kibana plugin error",
      "causes": ["Plugin incompatible", "Plugin not installed correctly", "Dependency missing"],
      "solutions": ["Update plugin to compatible version", "Reinstall plugin", "Check plugin dependencies"]
    }
  ],
  "debugging_commands": [
    "curl -X GET 'http://localhost:5601/api/status'",
    "curl -X GET 'http://localhost:5601/api/saved_objects/_find?type=index-pattern'",
    "kibana --logging.verbose=true"
  ],
  "documentation_urls": [
    "https://www.elastic.co/guide/en/kibana/current/",
    "https://github.com/elastic/kibana"
  ]
}
