{
  "tool": "opsgenie",
  "category": "monitoring",
  "common_errors": [
    {
      "pattern": "API key.*not valid",
      "meaning": "Opsgenie API key rejected",
      "causes": ["Wrong API key", "Key disabled", "Key lacks permission"],
      "solutions": ["Get correct key from Opsgenie settings", "Enable the API key", "Use key with correct access rights"]
    },
    {
      "pattern": "integration.*not found",
      "meaning": "Integration doesn't exist",
      "causes": ["Integration deleted", "Wrong integration ID", "Team context mismatch"],
      "solutions": ["Create new integration", "Verify integration ID", "Check team ownership"]
    },
    {
      "pattern": "responder.*not found",
      "meaning": "Alert responder doesn't exist",
      "causes": ["User deactivated", "Team deleted", "Schedule removed"],
      "solutions": ["Verify user exists", "Check team still exists", "Confirm schedule exists"]
    },
    {
      "pattern": "rate limit.*exceeded",
      "meaning": "API rate limit hit",
      "causes": ["Too many requests", "Alert storm", "Script loop"],
      "solutions": ["Implement backoff", "Aggregate alerts", "Check for loops"]
    },
    {
      "pattern": "alert.*not created",
      "meaning": "Alert creation failed",
      "causes": ["Missing required field", "Invalid payload", "Integration disabled"],
      "solutions": ["Check message field is present", "Validate JSON payload", "Enable integration"]
    },
    {
      "pattern": "notification.*not sent",
      "meaning": "Notification didn't reach user",
      "causes": ["Contact not configured", "Notification rules filter", "Quiet hours"],
      "solutions": ["Add user contact methods", "Check notification rules", "Review quiet hours settings"]
    },
    {
      "pattern": "escalation.*not triggered",
      "meaning": "Escalation not proceeding as expected",
      "causes": ["Alert acknowledged", "Escalation policy misconfigured", "Delay not elapsed"],
      "solutions": ["Check alert status", "Review escalation policy steps", "Wait for delay period"]
    },
    {
      "pattern": "team.*permission denied",
      "meaning": "Cannot access team resources",
      "causes": ["Not team member", "Role lacks permission", "Team routing issue"],
      "solutions": ["Add user to team", "Assign role with permission", "Check routing rules"]
    },
    {
      "pattern": "webhook.*failed",
      "meaning": "Outgoing webhook delivery failed",
      "causes": ["URL unreachable", "Authentication error", "Response not 2xx"],
      "solutions": ["Verify webhook URL", "Check auth credentials", "Ensure endpoint returns success"]
    },
    {
      "pattern": "heartbeat.*expired",
      "meaning": "Heartbeat monitoring alert triggered",
      "causes": ["Heartbeat not sent in interval", "System down", "Network issue"],
      "solutions": ["Check heartbeat sender", "Verify system health", "Check network connectivity"]
    }
  ],
  "debugging_commands": [
    "curl -X GET 'https://api.opsgenie.com/v2/alerts' -H 'Authorization: GenieKey <key>'",
    "curl -X GET 'https://api.opsgenie.com/v2/integrations/<id>' -H 'Authorization: GenieKey <key>'"
  ],
  "documentation_urls": [
    "https://support.atlassian.com/opsgenie/",
    "https://docs.opsgenie.com/"
  ]
}
