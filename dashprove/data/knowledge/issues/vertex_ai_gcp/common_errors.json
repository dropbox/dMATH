{
  "tool": "vertex_ai_gcp",
  "category": "gcp_ai",
  "description": "Google Vertex AI - unified ML platform for building and deploying models",
  "common_errors": [
    {
      "id": "permission_denied",
      "pattern": "PermissionDenied",
      "regex": "(?i)permission.*denied|403|not.*authorized|aiplatform.*permission",
      "cause": "IAM permissions missing for Vertex AI operation",
      "solution": "Grant roles/aiplatform.user or aiplatform.admin, check service account permissions",
      "severity": "high",
      "category": "permissions"
    },
    {
      "id": "quota_exceeded",
      "pattern": "Quota exceeded",
      "regex": "(?i)quota.*exceeded|resource.*exhausted|rate.*limit|training.*quota",
      "cause": "Vertex AI quota exceeded",
      "solution": "Request quota increase, use different region, reduce concurrent jobs",
      "severity": "medium",
      "category": "limits"
    },
    {
      "id": "training_failed",
      "pattern": "Training failed",
      "regex": "(?i)training.*failed|custom.*job.*failed|hyperparameter.*failed|worker.*error",
      "cause": "Model training job failed",
      "solution": "Check training logs, verify container image, review resource configuration (CPU/GPU/memory)",
      "severity": "high",
      "category": "training"
    },
    {
      "id": "deployment_failed",
      "pattern": "Deployment failed",
      "regex": "(?i)deployment.*failed|endpoint.*error|model.*deploy.*failed|predict.*route",
      "cause": "Model deployment to endpoint failed",
      "solution": "Check model artifacts, verify serving container, review machine type availability",
      "severity": "high",
      "category": "deployment"
    },
    {
      "id": "prediction_error",
      "pattern": "Prediction error",
      "regex": "(?i)prediction.*error|inference.*error|input.*format|batch.*prediction",
      "cause": "Model prediction request failed",
      "solution": "Verify input format matches model signature, check endpoint health, review request payload",
      "severity": "medium",
      "category": "inference"
    },
    {
      "id": "model_not_found",
      "pattern": "Model not found",
      "regex": "(?i)model.*not.*found|artifact.*not.*found|gcs.*model|model.*uri",
      "cause": "Model artifacts not found at specified location",
      "solution": "Verify GCS path to model artifacts, check bucket permissions, ensure model was uploaded",
      "severity": "high",
      "category": "models"
    },
    {
      "id": "gpu_error",
      "pattern": "GPU",
      "regex": "(?i)gpu.*error|accelerator.*error|cuda.*error|no.*gpu.*available",
      "cause": "GPU not available or CUDA error",
      "solution": "Request GPU quota in region, verify GPU availability, check CUDA version compatibility",
      "severity": "high",
      "category": "compute"
    },
    {
      "id": "pipeline_error",
      "pattern": "Pipeline",
      "regex": "(?i)pipeline.*failed|component.*error|artifact.*error|kubeflow",
      "cause": "Vertex AI Pipeline execution failed",
      "solution": "Review component logs, check input/output artifacts, verify component dependencies",
      "severity": "high",
      "category": "pipelines"
    },
    {
      "id": "feature_store",
      "pattern": "Feature Store",
      "regex": "(?i)feature.*store|featurestore.*error|entity.*type|feature.*serving",
      "cause": "Feature Store operation failed",
      "solution": "Check feature store configuration, verify entity type schema, review serving configuration",
      "severity": "medium",
      "category": "features"
    },
    {
      "id": "autoscaling_error",
      "pattern": "Autoscaling",
      "regex": "(?i)autoscaling|scale.*error|replica.*error|min.*max.*nodes",
      "cause": "Endpoint autoscaling configuration issue",
      "solution": "Review min/max replicas, check traffic patterns, verify machine type quotas",
      "severity": "medium",
      "category": "scaling"
    }
  ],
  "related_tools": ["gcloud", "bigquery", "cloud_storage", "tensorflow"],
  "documentation_url": "https://cloud.google.com/vertex-ai/docs"
}
