{
  "tool": "objection",
  "category": "database_orm",
  "description": "Objection.js - SQL ORM built on Knex",
  "errors": [
    {
      "pattern": "Model.*not bound to a knex instance",
      "meaning": "Model missing knex binding",
      "causes": ["knex not bound to Model class", "Import order issue", "Missing initialization"],
      "solutions": ["Call Model.knex(knexInstance)", "Ensure binding in app startup", "Check import order"]
    },
    {
      "pattern": "relation.*is not defined",
      "meaning": "Relation not found in relationMappings",
      "causes": ["Relation name typo", "Missing relationMappings", "Circular reference not handled"],
      "solutions": ["Check relationMappings() method", "Verify relation name spelling", "Use require() for circular refs"]
    },
    {
      "pattern": "ValidationError.*required",
      "meaning": "JSON Schema validation failed",
      "causes": ["Required property missing", "Type mismatch", "Schema constraint violated"],
      "solutions": ["Provide all required fields", "Match types to jsonSchema", "Review schema constraints"]
    },
    {
      "pattern": "Cannot read.*modelClass",
      "meaning": "Model class resolution failed",
      "causes": ["Circular dependency in relations", "Wrong model path", "Model not exported"],
      "solutions": ["Use string or function for modelClass", "Check model file exports", "Break circular dependencies"]
    },
    {
      "pattern": "unknown column.*in.*clause",
      "meaning": "Column not in table",
      "causes": ["Column name mismatch", "Migration not run", "Wrong table schema"],
      "solutions": ["Check column names in database", "Run pending migrations", "Verify model matches schema"]
    },
    {
      "pattern": "insertGraph.*violates foreign key",
      "meaning": "Foreign key constraint in graph insert",
      "causes": ["Related record doesn't exist", "Insert order incorrect", "Missing cascade option"],
      "solutions": ["Use #id references for new related items", "Insert parent first", "Check foreign key exists"]
    },
    {
      "pattern": "eager.*circular",
      "meaning": "Circular eager loading detected",
      "causes": ["Self-referencing relation loaded infinitely", "Bidirectional eager load", "Missing recursion limit"],
      "solutions": ["Use withGraphFetched with specific depth", "Break circular with modifiers", "Limit recursion in query"]
    },
    {
      "pattern": "SQLITE_CONSTRAINT.*FOREIGN KEY",
      "meaning": "SQLite foreign key violation",
      "causes": ["Deleting parent with children", "Insert with invalid foreign key", "Foreign keys not enabled"],
      "solutions": ["Enable foreign keys: PRAGMA foreign_keys = ON", "Delete children first", "Use onDelete cascade in migration"]
    }
  ]
}
