{
  "tool": "latex",
  "version": "TeX Live 2024",
  "last_updated": "2025-12-23",
  "errors": [
    {
      "id": "undefined_control_sequence",
      "pattern": "Undefined control sequence|\\\\\\w+.*undefined",
      "message": "Unknown command used",
      "cause": "Command not defined or package not loaded",
      "solutions": [
        {
          "approach": "Load required package",
          "code": "\\usepackage{amsmath}  % for \\text, \\mathbb, etc.\n\\usepackage{graphicx}  % for \\includegraphics",
          "when": "Missing package"
        },
        {
          "approach": "Check spelling",
          "code": "\\textbf not \\testbf",
          "when": "Typo in command"
        },
        {
          "approach": "Define command",
          "code": "\\newcommand{\\mycommand}[1]{...}",
          "when": "Custom command needed"
        }
      ]
    },
    {
      "id": "missing_begin",
      "pattern": "Missing \\$|Missing \\\\begin|Extra \\}|Runaway argument",
      "message": "Unmatched delimiters",
      "cause": "Opening delimiter without matching closing",
      "solutions": [
        {
          "approach": "Match math delimiters",
          "code": "$x^2$ not $x^2  % missing closing $",
          "when": "Inline math"
        },
        {
          "approach": "Balance environments",
          "code": "\\begin{equation}\n  ...\n\\end{equation}",
          "when": "Environment mismatch"
        },
        {
          "approach": "Escape special chars",
          "code": "\\$ \\% \\& \\# \\_  % escape special characters",
          "when": "Literal special char"
        }
      ]
    },
    {
      "id": "file_not_found",
      "pattern": "File.*not found|Cannot find.*\\.sty|Package.*not installed",
      "message": "Required file or package missing",
      "cause": "Package not installed or path incorrect",
      "solutions": [
        {
          "approach": "Install package",
          "code": "tlmgr install <package-name>",
          "when": "Package not installed"
        },
        {
          "approach": "Update TeX Live",
          "code": "tlmgr update --all",
          "when": "Outdated installation"
        },
        {
          "approach": "Check file path",
          "code": "\\input{./sections/chapter1}  % use relative path",
          "when": "File in subdirectory"
        }
      ]
    },
    {
      "id": "overfull_hbox",
      "pattern": "Overfull \\\\hbox|badness \\d+|underfull \\\\hbox",
      "message": "Line too long or short",
      "cause": "Text doesn't fit in line width properly",
      "solutions": [
        {
          "approach": "Allow hyphenation",
          "code": "\\hyphenation{spe-cial-word}",
          "when": "Word not hyphenating"
        },
        {
          "approach": "Use sloppypar",
          "code": "\\begin{sloppypar}\n  Long text...\n\\end{sloppypar}",
          "when": "Allow looser spacing"
        },
        {
          "approach": "Break manually",
          "code": "\\linebreak or \\\\",
          "when": "Force line break"
        }
      ]
    },
    {
      "id": "font_error",
      "pattern": "Font.*not found|cannot load font|missing glyphs",
      "message": "Font not available",
      "cause": "Font not installed or wrong engine",
      "solutions": [
        {
          "approach": "Use XeLaTeX for system fonts",
          "code": "xelatex file.tex  % then:\n\\usepackage{fontspec}\n\\setmainfont{Arial}",
          "when": "Using system fonts"
        },
        {
          "approach": "Use Computer Modern",
          "code": "% Default fonts work with pdflatex",
          "when": "Standard pdflatex"
        },
        {
          "approach": "Install font package",
          "code": "tlmgr install collection-fontsrecommended",
          "when": "Missing standard fonts"
        }
      ]
    },
    {
      "id": "float_placement",
      "pattern": "too many unprocessed floats|float.*lost|Cannot determine size",
      "message": "Figure/table placement issues",
      "cause": "Too many floats or restrictive placement",
      "solutions": [
        {
          "approach": "Use H placement",
          "code": "\\usepackage{float}\n\\begin{figure}[H]  % force here",
          "when": "Force exact position"
        },
        {
          "approach": "Clear floats",
          "code": "\\clearpage  % flush pending floats",
          "when": "Too many floats"
        },
        {
          "approach": "Relax placement",
          "code": "\\begin{figure}[!htbp]  % try all placements",
          "when": "Flexible placement OK"
        }
      ]
    }
  ]
}
