{
  "id": "mathsat",
  "name": "MathSAT",
  "category": "smt_solver",
  "subcategory": "general",

  "description": "SMT solver with strong floating-point and interpolation support",
  "long_description": "MathSAT 5 is an SMT solver developed by FBK (Fondazione Bruno Kessler). It supports a wide range of theories including linear arithmetic, floating-point, and arrays. MathSAT is particularly strong in interpolation, optimization, and AllSMT (model enumeration).",

  "capabilities": [
    "linear_arithmetic",
    "floating_point",
    "bit_vectors",
    "arrays",
    "interpolation",
    "optimization",
    "allsmt",
    "unsat_core"
  ],
  "property_types": ["formula", "assertion", "constraint"],
  "input_languages": ["smtlib2", "cpp", "python"],
  "output_formats": ["sat/unsat", "model", "interpolant", "proof"],

  "installation": {
    "methods": [
      {"type": "download", "command": "Download from https://mathsat.fbk.eu/download.html"},
      {"type": "pip", "command": "pip install mathsat"}
    ],
    "dependencies": [],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://mathsat.fbk.eu/",
    "api_reference": "https://mathsat.fbk.eu/api.html",
    "examples": "https://mathsat.fbk.eu/examples.html"
  },

  "tactics": [
    {
      "name": "interpolation",
      "description": "Generate interpolants",
      "syntax": "(get-interpolant (A) (B))",
      "when_to_use": "For predicate abstraction or CEGAR",
      "examples": ["(get-interpolant (and a b) (or c d))"]
    },
    {
      "name": "optimization",
      "description": "Optimize objective function",
      "syntax": "(minimize obj)",
      "when_to_use": "For optimization modulo theories",
      "examples": ["(minimize (+ x y))"]
    },
    {
      "name": "allsmt",
      "description": "Enumerate all satisfying assignments",
      "syntax": "msat_all_sat",
      "when_to_use": "When all models are needed",
      "examples": ["msat_all_sat(env, predicates, callback)"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "timeout",
      "meaning": "Solver exceeded time limit",
      "common_causes": ["Complex floating-point", "Many quantifiers"],
      "fixes": ["Simplify floating-point operations", "Reduce precision"]
    },
    {
      "pattern": "unsupported",
      "meaning": "Feature not supported",
      "common_causes": ["Non-standard floating-point operations"],
      "fixes": ["Use standard IEEE 754 operations"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["constraint", "assertion"],
    "cli_command": "dashprove verify --backend mathsat"
  },

  "performance": {
    "typical_runtime": "milliseconds to hours",
    "scalability": "Excellent for floating-point and interpolation",
    "memory_usage": "Moderate"
  },

  "comparisons": {
    "similar_tools": ["z3", "cvc5", "yices"],
    "advantages": [
      "Best-in-class interpolation",
      "Strong floating-point support",
      "AllSMT model enumeration"
    ],
    "disadvantages": [
      "Proprietary license for commercial use",
      "Less active community"
    ]
  },

  "metadata": {
    "version": "5.6.10",
    "last_updated": "2025-12-21",
    "maintainer": "FBK",
    "license": "Free for research"
  }
}
