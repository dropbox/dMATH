{
  "id": "cadical",
  "name": "CaDiCaL",
  "category": "sat_solver",
  "subcategory": "cdcl",

  "description": "Award-winning modern CDCL SAT solver",
  "long_description": "CaDiCaL is a modern, award-winning SAT solver developed by Armin Biere. It has won multiple SAT competitions and features advanced techniques including bounded variable addition, inprocessing, and chrono backtracking. CaDiCaL is used as a backend in many verification tools.",

  "capabilities": [
    "sat_solving",
    "cdcl",
    "inprocessing",
    "bounded_variable_addition",
    "chronological_backtracking",
    "proof_logging"
  ],
  "property_types": ["cnf", "boolean_formula"],
  "input_languages": ["dimacs"],
  "output_formats": ["sat/unsat", "model", "drat_proof"],

  "installation": {
    "methods": [
      {"type": "source", "command": "./configure && make"},
      {"type": "cargo", "command": "cargo add cadical"}
    ],
    "dependencies": [],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://github.com/arminbiere/cadical",
    "tutorial": "https://github.com/arminbiere/cadical/blob/master/README.md",
    "api_reference": "https://github.com/arminbiere/cadical/blob/master/src/cadical.hpp",
    "examples": "https://github.com/arminbiere/cadical/tree/master/test"
  },

  "tactics": [],

  "error_patterns": [
    {
      "pattern": "UNSATISFIABLE",
      "meaning": "Formula has no solution",
      "common_causes": ["Contradictory constraints"],
      "fixes": ["Check constraints"]
    },
    {
      "pattern": "invalid",
      "meaning": "Invalid input format",
      "common_causes": ["Malformed DIMACS"],
      "fixes": ["Fix input format"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["boolean_formula"],
    "cli_command": "dashprove verify --backend cadical"
  },

  "performance": {
    "typical_runtime": "milliseconds to hours",
    "scalability": "State-of-the-art",
    "memory_usage": "Moderate"
  },

  "comparisons": {
    "similar_tools": ["minisat", "glucose", "kissat"],
    "advantages": [
      "Competition winner",
      "Advanced techniques",
      "Active development"
    ],
    "disadvantages": [
      "Larger than MiniSat",
      "Complex codebase"
    ]
  },

  "metadata": {
