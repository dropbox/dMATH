{
  "id": "kissat",
  "name": "Kissat",
  "category": "sat_solver",
  "subcategory": "cdcl",

  "description": "State-of-the-art SAT solver, successor to CaDiCaL",
  "long_description": "Kissat is a clean-room reimplementation of CaDiCaL by Armin Biere. It won the SAT Competition 2020 main track and represents the current state-of-the-art in CDCL SAT solving. Kissat incorporates advanced techniques like chronological backtracking, eager subsumption, and vivification.",

  "capabilities": [
    "sat_solving",
    "model_generation",
    "certified_unsat",
    "preprocessing"
  ],
  "property_types": ["cnf", "formula"],
  "input_languages": ["dimacs"],
  "output_formats": ["sat/unsat", "model", "proof"],

  "installation": {
    "methods": [
      {"type": "source", "command": "git clone https://github.com/arminbiere/kissat && cd kissat && ./configure && make"}
    ],
    "dependencies": [],
    "platforms": ["linux", "macos"]
  },

  "documentation": {
    "official": "https://github.com/arminbiere/kissat",
    "api_reference": "https://fmv.jku.at/kissat/",
    "examples": "https://github.com/arminbiere/kissat/tree/main/test"
  },

  "tactics": [
    {
      "name": "default",
      "description": "Default solving mode",
      "syntax": "kissat formula.cnf",
      "when_to_use": "General purpose SAT solving",
      "examples": ["kissat input.cnf"]
    },
    {
      "name": "unsat",
      "description": "Optimized for unsatisfiable instances",
      "syntax": "kissat --unsat formula.cnf",
      "when_to_use": "When expecting UNSAT",
      "examples": ["kissat --unsat input.cnf"]
    },
    {
      "name": "sat",
      "description": "Optimized for satisfiable instances",
      "syntax": "kissat --sat formula.cnf",
      "when_to_use": "When expecting SAT",
      "examples": ["kissat --sat input.cnf"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "UNKNOWN",
      "meaning": "Solver terminated without result",
      "common_causes": ["Timeout", "Interrupt"],
      "fixes": ["Increase time limit"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["constraint"],
    "cli_command": "dashprove verify --backend kissat"
  },

  "performance": {
    "typical_runtime": "milliseconds to hours",
    "scalability": "State-of-the-art performance",
    "memory_usage": "Efficient"
  },

  "comparisons": {
    "similar_tools": ["cadical", "glucose", "minisat"],
    "advantages": [
      "Current state-of-the-art",
      "SAT Competition winner",
      "Clean implementation"
    ],
    "disadvantages": [
      "No incremental interface",
      "Single-threaded only"
    ]
  },

  "metadata": {
    "version": "3.1.1",
    "last_updated": "2025-12-21",
    "maintainer": "Armin Biere",
    "license": "MIT"
  }
}
