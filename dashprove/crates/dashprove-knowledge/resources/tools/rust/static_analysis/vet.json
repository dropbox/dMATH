{
  "id": "vet",
  "name": "cargo-vet",
  "category": "rust_static_analysis",
  "subcategory": "supply_chain",

  "description": "Supply chain security for Rust dependencies via audits",
  "long_description": "cargo-vet helps teams ensure third-party Rust dependencies have been audited by a trusted source. It tracks which crates have been reviewed, shares audits between organizations, and identifies new unaudited dependencies when updating.",

  "capabilities": [
    "audit_tracking",
    "trust_management",
    "audit_sharing",
    "diff_review",
    "import_audits",
    "certification",
    "ci_enforcement",
    "incremental_review"
  ],
  "property_types": ["supply_chain", "audit"],
  "input_languages": ["rust"],
  "output_formats": ["text", "json"],

  "installation": {
    "methods": [
      {"type": "cargo", "command": "cargo install cargo-vet"},
      {"type": "source", "url": "https://github.com/mozilla/cargo-vet"}
    ],
    "dependencies": [],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://mozilla.github.io/cargo-vet/",
    "tutorial": "https://mozilla.github.io/cargo-vet/getting-started.html",
    "api_reference": "https://mozilla.github.io/cargo-vet/reference.html",
    "examples": "https://github.com/nickel-lang/nickel/tree/master/supply-chain"
  },

  "tactics": [
    {
      "name": "init",
      "description": "Initialize cargo-vet",
      "syntax": "cargo vet init",
      "when_to_use": "Setting up supply chain tracking",
      "examples": ["cargo vet init"]
    },
    {
      "name": "check",
      "description": "Check all dependencies audited",
      "syntax": "cargo vet",
      "when_to_use": "CI enforcement",
      "examples": ["cargo vet", "cargo vet check"]
    },
    {
      "name": "certify",
      "description": "Add audit for crate",
      "syntax": "cargo vet certify CRATE VERSION",
      "when_to_use": "Recording completed audit",
      "examples": ["cargo vet certify serde 1.0.152"]
    },
    {
      "name": "diff",
      "description": "Show changes between versions",
      "syntax": "cargo vet diff CRATE OLD NEW",
      "when_to_use": "Reviewing version updates",
      "examples": ["cargo vet diff serde 1.0.150 1.0.152"]
    },
    {
      "name": "import",
      "description": "Import audits from organization",
      "syntax": "cargo vet trust ORG_URL",
      "when_to_use": "Sharing audits across teams",
      "examples": ["cargo vet trust https://example.org/audits.toml"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "unvetted dependencies",
      "meaning": "Dependencies without audits",
      "common_causes": ["New dependency", "Updated version"],
      "fixes": ["Audit and certify", "Import trusted audit"]
    },
    {
      "pattern": "Recommendation: ",
      "meaning": "Suggested audit action",
      "common_causes": ["cargo-vet suggesting review"],
      "fixes": ["Follow recommendation"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["supply_chain"],
    "cli_command": "dashprove verify --backend vet"
  },

  "performance": {
    "typical_runtime": "Seconds",
    "scalability": "Scales with dependency count",
    "memory_usage": "Low"
  },

  "comparisons": {
    "similar_tools": ["cargo-crev", "cargo-audit", "cargo-deny"],
    "advantages": [
      "Organization-level audits",
      "Incremental review of diffs",
      "Audit sharing",
      "Mozilla-backed"
    ],
    "disadvantages": [
      "Requires audit effort",
      "New tool, smaller network"
    ]
  },

  "metadata": {
    "version": "0.9.1",
    "last_updated": "2025-12-20",
    "maintainer": "Mozilla",
    "license": "MIT/Apache-2.0"
  }
}
