{
  "id": "deepchecks",
  "name": "Deepchecks",
  "category": "ai_ml_quality",
  "subcategory": "data_validation",

  "description": "Testing framework for ML data, models, and pipelines",
  "long_description": "Deepchecks is a Python library for testing machine learning models and data. It provides comprehensive checks for data integrity, train-test splits, model performance, and more. Deepchecks helps identify issues before and after model training.",

  "capabilities": [
    "data_integrity_checks",
    "train_test_validation",
    "model_evaluation",
    "distribution_drift",
    "feature_importance",
    "label_quality",
    "duplicate_detection",
    "report_generation"
  ],
  "property_types": ["data_quality", "model_validation"],
  "input_languages": ["python", "pandas", "pytorch", "sklearn"],
  "output_formats": ["html_report", "json", "jupyter_widget"],

  "installation": {
    "methods": [
      {"type": "pip", "command": "pip install deepchecks"},
      {"type": "source", "url": "https://github.com/deepchecks/deepchecks"}
    ],
    "dependencies": ["python", "pandas", "numpy", "scikit-learn"],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://docs.deepchecks.com/",
    "tutorial": "https://docs.deepchecks.com/stable/getting-started/welcome.html",
    "api_reference": "https://docs.deepchecks.com/stable/api/",
    "examples": "https://github.com/deepchecks/deepchecks/tree/main/examples"
  },

  "tactics": [
    {
      "name": "data_integrity",
      "description": "Check data quality",
      "syntax": "data_integrity_suite.run(dataset)",
      "when_to_use": "Before training on new data",
      "examples": ["from deepchecks.tabular.suites import data_integrity\nresult = data_integrity().run(dataset)"]
    },
    {
      "name": "train_test",
      "description": "Validate train-test split",
      "syntax": "train_test_validation_suite.run(train, test)",
      "when_to_use": "After splitting data",
      "examples": ["from deepchecks.tabular.suites import train_test_validation\nresult = train_test_validation().run(train, test)"]
    },
    {
      "name": "model_evaluation",
      "description": "Evaluate model performance",
      "syntax": "model_evaluation_suite.run(train, test, model)",
      "when_to_use": "After model training",
      "examples": ["from deepchecks.tabular.suites import model_evaluation\nresult = model_evaluation().run(train, test, model)"]
    },
    {
      "name": "full_suite",
      "description": "Run all checks",
      "syntax": "full_suite().run(train, test, model)",
      "when_to_use": "Comprehensive validation",
      "examples": ["from deepchecks.tabular.suites import full_suite\nresult = full_suite().run(train, test, model)"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "Data Drift Detected",
      "meaning": "Distribution shift between datasets",
      "common_causes": ["Different data collection periods", "Sampling bias"],
      "fixes": ["Investigate drift source", "Resample data"]
    },
    {
      "pattern": "Duplicate Samples",
      "meaning": "Same samples in train and test",
      "common_causes": ["Incorrect split", "Data leakage"],
      "fixes": ["Fix splitting logic"]
    },
    {
      "pattern": "Label Imbalance",
      "meaning": "Skewed class distribution",
      "common_causes": ["Imbalanced dataset"],
      "fixes": ["Use resampling or class weights"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["data_quality"],
    "cli_command": "dashprove verify --backend deepchecks"
  },

  "performance": {
    "typical_runtime": "Seconds to minutes",
    "scalability": "Handles large datasets with sampling",
    "memory_usage": "Moderate"
  },

  "comparisons": {
    "similar_tools": ["great_expectations", "evidently", "whylogs"],
    "advantages": ["ML-focused", "Comprehensive suites", "Good visualizations", "Active development"],
    "disadvantages": ["ML-specific (not general data)", "Some checks slow"]
  },

  "metadata": {
    "version": "0.18.0",
    "last_updated": "2025-12-20",
    "maintainer": "Deepchecks",
    "license": "AGPL-3.0"
  }
}
