{
  "id": "key",
  "name": "KeY",
  "category": "program_verifier",
  "subcategory": "java",

  "description": "Deductive verification system for Java programs",
  "long_description": "KeY is a deductive verification platform for Java. It uses dynamic logic (JavaDL) to reason about Java programs and JML (Java Modeling Language) for specifications. KeY provides both automatic and interactive proof construction and has been used to verify industrial Java applications.",

  "capabilities": [
    "deductive_verification",
    "dynamic_logic",
    "jml_support",
    "interactive_proving",
    "symbolic_execution"
  ],
  "property_types": ["jml_contract", "invariant", "assertion"],
  "input_languages": ["java", "jml"],
  "output_formats": ["proof", "verification_result"],

  "installation": {
    "methods": [
      {"type": "download", "command": "Download from https://www.key-project.org/"}
    ],
    "dependencies": ["java"],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://www.key-project.org/",
    "book": "https://www.key-project.org/thebook2/",
    "tutorial": "https://www.key-project.org/tutorial/"
  },

  "tactics": [
    {
      "name": "auto",
      "description": "Automatic proof search",
      "syntax": "Click 'Start' in GUI",
      "when_to_use": "For automated verification",
      "examples": ["Strategies -> Auto Mode"]
    },
    {
      "name": "simplify",
      "description": "Simplification rules",
      "syntax": "Apply simplify rules",
      "when_to_use": "To simplify formulas",
      "examples": ["Apply 'andRight' rule"]
    },
    {
      "name": "loop_invariant",
      "description": "Apply loop invariant",
      "syntax": "Select loop and apply invariant rule",
      "when_to_use": "For loops with JML loop_invariant",
      "examples": ["//@ loop_invariant i >= 0 && i <= n;"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "Goal not closeable",
      "meaning": "Proof cannot be completed",
      "common_causes": ["Missing invariant", "Incorrect spec"],
      "fixes": ["Add loop invariant", "Strengthen precondition"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["jml", "contract"],
    "cli_command": "dashprove verify --backend key"
  },

  "performance": {
    "typical_runtime": "seconds to hours",
    "scalability": "Good for Java applications",
    "memory_usage": "Moderate to high"
  },

  "comparisons": {
    "similar_tools": ["openjml", "krakatoa", "jstar"],
    "advantages": [
      "Full Java support",
      "Interactive + automatic",
      "Strong JML integration"
    ],
    "disadvantages": [
      "GUI-centric",
      "Learning curve"
    ]
  },

  "metadata": {
    "version": "2.10.0",
    "last_updated": "2025-12-21",
    "maintainer": "KeY team",
    "license": "GPL-2.0"
  }
}
