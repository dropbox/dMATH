{
  "id": "nnenum",
  "name": "nnenum",
  "category": "neural_network",
  "subcategory": "verifiers",

  "description": "Enumeration-based neural network verifier using star sets",
  "long_description": "nnenum is a high-performance neural network verification tool that uses star set propagation with eager bounds computation. It implements efficient enumeration of linear regions and supports ReLU networks. Won multiple categories in VNN-COMP verification competitions.",

  "capabilities": [
    "star_set_enumeration",
    "eager_bounds",
    "relu_verification",
    "counterexample_generation",
    "parallel_verification",
    "vnnlib_support",
    "onnx_support"
  ],
  "property_types": ["safety", "robustness", "output_bounds"],
  "input_languages": ["onnx", "vnnlib"],
  "output_formats": ["verification_result", "counterexample", "bounds"],

  "installation": {
    "methods": [
      {"type": "pip", "command": "pip install nnenum"},
      {"type": "source", "url": "https://github.com/stanleybak/nnenum"}
    ],
    "dependencies": ["numpy", "scipy", "onnx", "glpk"],
    "platforms": ["linux", "macos"]
  },

  "documentation": {
    "official": "https://github.com/stanleybak/nnenum",
    "tutorial": "https://github.com/stanleybak/nnenum/tree/master/examples",
    "api_reference": "https://github.com/stanleybak/nnenum/blob/master/README.md",
    "examples": "https://github.com/stanleybak/nnenum/tree/master/examples"
  },

  "tactics": [
    {
      "name": "verify",
      "description": "Verify safety property",
      "syntax": "nnenum.verify(network, input_spec, output_spec)",
      "when_to_use": "Checking if property holds",
      "examples": ["result = enumerate(network, init_box, spec)"]
    },
    {
      "name": "enumerate",
      "description": "Enumerate linear regions",
      "syntax": "nnenum.enumerate(network, input_set)",
      "when_to_use": "Finding all activation patterns",
      "examples": ["regions = enumerate_regions(net, box)"]
    },
    {
      "name": "counterexample",
      "description": "Find violating input",
      "syntax": "nnenum.find_counterexample(network, spec)",
      "when_to_use": "When property violated",
      "examples": ["ce = find_violation(net, prop)"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "violated",
      "meaning": "Property does not hold",
      "common_causes": ["Network not robust", "Specification too strict"],
      "fixes": ["Examine counterexample", "Adjust specification"]
    },
    {
      "pattern": "timeout",
      "meaning": "Verification did not complete",
      "common_causes": ["Large network", "Many linear regions"],
      "fixes": ["Increase timeout", "Use approximation"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["neural_robustness", "neural_reachability"],
    "cli_command": "dashprove verify --backend nnenum"
  },

  "performance": {
    "typical_runtime": "Seconds to minutes for small networks",
    "scalability": "Good for small to medium networks",
    "memory_usage": "Moderate"
  },

  "comparisons": {
    "similar_tools": ["marabou", "nnv", "eran"],
    "advantages": [
      "Fast star set propagation",
      "VNN-COMP winner",
      "Good counterexample generation"
    ],
    "disadvantages": [
      "Limited to ReLU networks",
      "Less active development"
    ]
  },

  "metadata": {
    "version": "1.0",
    "last_updated": "2025-12-20",
    "maintainer": "Stanley Bak",
    "license": "BSD"
  }
}
