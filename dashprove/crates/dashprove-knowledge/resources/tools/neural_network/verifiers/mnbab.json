{
  "id": "mnbab",
  "name": "MN-BaB",
  "category": "neural_network",
  "subcategory": "verifiers",

  "description": "Multi-neuron branch-and-bound verifier with joint constraints",
  "long_description": "MN-BaB (Multi-Neuron Branch and Bound) is a neural network verifier that considers joint constraints across multiple neurons for tighter bounds. It uses multi-neuron relaxations within a branch-and-bound framework for complete verification.",

  "capabilities": [
    "multi_neuron_constraints",
    "branch_and_bound",
    "joint_relaxation",
    "complete_verification",
    "relu_verification",
    "gpu_acceleration"
  ],
  "property_types": ["safety", "robustness", "reachability"],
  "input_languages": ["onnx", "pytorch"],
  "output_formats": ["verification_result", "bounds", "counterexample"],

  "installation": {
    "methods": [
      {"type": "source", "url": "https://github.com/eth-sri/mn-bab"}
    ],
    "dependencies": ["pytorch", "gurobi", "numpy"],
    "platforms": ["linux"]
  },

  "documentation": {
    "official": "https://github.com/eth-sri/mn-bab",
    "tutorial": "https://github.com/eth-sri/mn-bab/blob/main/README.md",
    "api_reference": "https://github.com/eth-sri/mn-bab",
    "examples": "https://github.com/eth-sri/mn-bab/tree/main/examples"
  },

  "tactics": [
    {
      "name": "verify",
      "description": "Complete verification with multi-neuron constraints",
      "syntax": "mnbab.verify(network, property)",
      "when_to_use": "When single-neuron bounds are insufficient",
      "examples": ["result = verifier.verify(net, spec)"]
    },
    {
      "name": "bound",
      "description": "Compute multi-neuron bounds",
      "syntax": "mnbab.compute_bounds(network, input_set)",
      "when_to_use": "Getting tighter output bounds",
      "examples": ["bounds = compute_mn_bounds(net, input_region)"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "UNSAT",
      "meaning": "Property violated",
      "common_causes": ["Network not robust", "Property too strict"],
      "fixes": ["Retrain network", "Relax property"]
    },
    {
      "pattern": "timeout",
      "meaning": "Verification incomplete",
      "common_causes": ["Large network", "Complex property"],
      "fixes": ["Increase timeout", "Simplify property"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["neural_robustness"],
    "cli_command": "dashprove verify --backend mnbab"
  },

  "performance": {
    "typical_runtime": "Minutes to hours",
    "scalability": "Better than single-neuron methods for hard instances",
    "memory_usage": "High"
  },

  "comparisons": {
    "similar_tools": ["crown", "venus", "babsr"],
    "advantages": [
      "Tighter bounds via multi-neuron constraints",
      "Complete verification",
      "State-of-the-art on hard benchmarks"
    ],
    "disadvantages": [
      "Complex implementation",
      "Higher overhead than simple methods"
    ]
  },

  "metadata": {
    "version": "1.0",
    "last_updated": "2025-12-20",
    "maintainer": "ETH SRI Lab",
    "license": "MIT"
  }
}
