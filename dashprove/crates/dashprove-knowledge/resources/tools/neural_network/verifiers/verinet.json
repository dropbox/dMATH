{
  "id": "verinet",
  "name": "VeriNet",
  "category": "neural_network",
  "subcategory": "verifiers",

  "description": "Complete neural network verifier using branch-and-bound with GPU acceleration",
  "long_description": "VeriNet is a complete neural network verification tool that combines symbolic interval propagation with branch-and-bound search. It uses GPU acceleration for efficient bound computation and supports various activation functions including ReLU, Sigmoid, and Tanh.",

  "capabilities": [
    "complete_verification",
    "branch_and_bound",
    "gpu_acceleration",
    "symbolic_intervals",
    "relu_verification",
    "sigmoid_verification",
    "tanh_verification",
    "robustness_verification"
  ],
  "property_types": ["safety", "robustness", "local_robustness"],
  "input_languages": ["onnx", "pytorch"],
  "output_formats": ["verification_result", "bounds", "counterexample"],

  "installation": {
    "methods": [
      {"type": "source", "url": "https://github.com/vas-group-imperial/VeriNet"},
      {"type": "pip", "command": "pip install -e ."}
    ],
    "dependencies": ["pytorch", "numpy", "onnx", "cuda"],
    "platforms": ["linux"]
  },

  "documentation": {
    "official": "https://github.com/vas-group-imperial/VeriNet",
    "tutorial": "https://github.com/vas-group-imperial/VeriNet/blob/master/README.md",
    "api_reference": "https://github.com/vas-group-imperial/VeriNet",
    "examples": "https://github.com/vas-group-imperial/VeriNet/tree/master/examples"
  },

  "tactics": [
    {
      "name": "verify",
      "description": "Complete verification",
      "syntax": "verinet.verify(network, input_bounds, property)",
      "when_to_use": "When complete answer is needed",
      "examples": ["result = verifier.verify(net, eps=0.01)"]
    },
    {
      "name": "bound_propagation",
      "description": "Compute output bounds",
      "syntax": "verinet.propagate_bounds(network, input_bounds)",
      "when_to_use": "Quick overapproximation of outputs",
      "examples": ["bounds = propagate(net, input_box)"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "UNSAT",
      "meaning": "Property violated",
      "common_causes": ["Network not robust", "Bounds too loose"],
      "fixes": ["Retrain with adversarial training", "Tighten bounds"]
    },
    {
      "pattern": "GPU out of memory",
      "meaning": "Network too large for GPU",
      "common_causes": ["Large batch size", "Deep network"],
      "fixes": ["Reduce batch size", "Use CPU fallback"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["neural_robustness"],
    "cli_command": "dashprove verify --backend verinet"
  },

  "performance": {
    "typical_runtime": "Seconds to hours",
    "scalability": "Good with GPU acceleration",
    "memory_usage": "High (GPU memory)"
  },

  "comparisons": {
    "similar_tools": ["crown", "marabou", "nnenum"],
    "advantages": [
      "GPU acceleration",
      "Support for non-ReLU activations",
      "Complete verification"
    ],
    "disadvantages": [
      "Requires CUDA",
      "Complex setup"
    ]
  },

  "metadata": {
    "version": "2.0",
    "last_updated": "2025-12-20",
    "maintainer": "Imperial College VAS Group",
    "license": "MIT"
  }
}
