{
  "id": "dnnv",
  "name": "DNNV",
  "category": "neural_network",
  "subcategory": "verifiers",

  "description": "Framework for specifying and verifying DNN properties with multiple backend verifiers",
  "long_description": "DNNV (Deep Neural Network Verification) is a framework that standardizes DNN property specification using DNNP (a Python-based DSL). It can dispatch verification queries to multiple backend verifiers including Marabou, ERAN, nnenum, and others, enabling comparison and portfolio approaches.",

  "capabilities": [
    "property_specification",
    "multi_backend_dispatch",
    "verifier_portfolio",
    "standardized_interface",
    "network_simplification",
    "property_translation"
  ],
  "property_types": ["robustness", "safety", "equivalence", "custom"],
  "input_languages": ["dnnp", "onnx", "pytorch", "tensorflow"],
  "output_formats": ["verification_result", "counterexample"],

  "installation": {
    "methods": [
      {"type": "pip", "command": "pip install dnnv"},
      {"type": "source", "url": "https://github.com/dlshriver/dnnv"}
    ],
    "dependencies": ["numpy", "onnx", "backend_verifiers"],
    "platforms": ["linux", "macos"]
  },

  "documentation": {
    "official": "https://github.com/dlshriver/dnnv",
    "tutorial": "https://dnnv.readthedocs.io/",
    "api_reference": "https://dnnv.readthedocs.io/en/latest/api/",
    "examples": "https://github.com/dlshriver/dnnv/tree/main/examples"
  },

  "tactics": [
    {
      "name": "verify",
      "description": "Verify property with specified backend",
      "syntax": "dnnv property.dnnp --network model.onnx --verifier backend",
      "when_to_use": "Checking DNN properties",
      "examples": ["dnnv robustness.dnnp --network net.onnx --verifier eran"]
    },
    {
      "name": "dnnp_spec",
      "description": "Define property in DNNP",
      "syntax": "Forall(x, Implies(precondition, postcondition))",
      "when_to_use": "Specifying verification properties",
      "examples": ["Forall(x, Implies(x in HyperRectangle(lb, ub), N(x) in Safe))"]
    },
    {
      "name": "simplify",
      "description": "Simplify network before verification",
      "syntax": "dnnv --simplify network.onnx",
      "when_to_use": "Preprocessing for faster verification",
      "examples": ["dnnv --simplify model.onnx -o simplified.onnx"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "violated",
      "meaning": "Property does not hold",
      "common_causes": ["Network not robust", "Property too strict"],
      "fixes": ["Examine counterexample", "Adjust property"]
    },
    {
      "pattern": "backend not found",
      "meaning": "Verifier not installed",
      "common_causes": ["Missing backend", "Path not set"],
      "fixes": ["Install backend verifier", "Set PATH"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["neural_robustness", "neural_reachability"],
    "cli_command": "dashprove verify --backend dnnv"
  },

  "performance": {
    "typical_runtime": "Depends on backend",
    "scalability": "Depends on backend",
    "memory_usage": "Moderate overhead over backend"
  },

  "comparisons": {
    "similar_tools": ["vnnlib", "onnx"],
    "advantages": [
      "Unified interface to multiple verifiers",
      "Expressive property language",
      "Network simplification"
    ],
    "disadvantages": [
      "Additional abstraction layer",
      "Requires backend installation"
    ]
  },

  "metadata": {
    "version": "0.5.0",
    "last_updated": "2025-12-20",
    "maintainer": "David Shriver",
    "license": "MIT"
  }
}
