{
  "id": "fstar",
  "name": "F*",
  "category": "theorem_prover",
  "subcategory": "dependent",

  "description": "Proof-oriented ML dialect with refinement types and effects",
  "long_description": "F* is a proof-oriented programming language developed at Microsoft Research and Inria. It combines dependent types with refinement types and an effect system. F* can extract to OCaml, F#, C, WASM, and Assembly. It's used extensively in Project Everest for verified cryptographic implementations.",

  "capabilities": [
    "dependent_types",
    "refinement_types",
    "effects_system",
    "extraction_to_c",
    "smt_automation",
    "proof_automation"
  ],
  "property_types": ["lemma", "theorem", "refinement"],
  "input_languages": ["fstar"],
  "output_formats": ["proof_term", "ocaml", "c", "wasm"],

  "installation": {
    "methods": [
      {"type": "opam", "command": "opam install fstar"},
      {"type": "nix", "command": "nix-env -i fstar"},
      {"type": "binary", "command": "Download from GitHub releases"}
    ],
    "dependencies": ["z3", "ocaml"],
    "platforms": ["linux", "macos", "windows"]
  },

  "documentation": {
    "official": "https://www.fstar-lang.org/",
    "tutorial": "https://www.fstar-lang.org/tutorial/",
    "api_reference": "https://www.fstar-lang.org/docs/",
    "examples": "https://github.com/FStarLang/FStar/tree/master/examples"
  },

  "tactics": [
    {
      "name": "norm",
      "description": "Normalize terms",
      "syntax": "norm [delta; zeta; iota]",
      "when_to_use": "To unfold and simplify",
      "examples": ["norm [delta_only [`%f]]"]
    },
    {
      "name": "smt",
      "description": "Invoke Z3",
      "syntax": "smt()",
      "when_to_use": "For arithmetic and SMT goals",
      "examples": ["assert_by_tactic p smt"]
    },
    {
      "name": "trefl",
      "description": "Term reflection equality",
      "syntax": "trefl()",
      "when_to_use": "For definitional equality",
      "examples": ["trefl()"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "could not prove",
      "meaning": "Verification condition failed",
      "common_causes": ["Missing precondition", "SMT timeout"],
      "fixes": ["Add assertion", "Strengthen precondition", "Split proof"]
    },
    {
      "pattern": "Subtyping check failed",
      "meaning": "Refinement type mismatch",
      "common_causes": ["Value doesn't satisfy refinement", "Missing proof"],
      "fixes": ["Prove refinement", "Adjust type"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["theorem", "refinement"],
    "cli_command": "dashprove verify --backend fstar"
  },

  "performance": {
    "typical_runtime": "seconds to minutes",
    "scalability": "Good for systems code",
    "memory_usage": "Moderate to high"
  },

  "comparisons": {
    "similar_tools": ["dafny", "liquid_haskell", "verus"],
    "advantages": [
      "Production-ready extraction",
      "SMT automation",
      "Used in real systems (HACL*, EverCrypt)"
    ],
    "disadvantages": [
      "Steep learning curve",
      "Complex effect system"
    ]
  },

  "metadata": {
    "version": "2024.09.05",
    "last_updated": "2025-12-21",
    "maintainer": "Microsoft Research & Inria",
    "license": "Apache-2.0"
  }
}
