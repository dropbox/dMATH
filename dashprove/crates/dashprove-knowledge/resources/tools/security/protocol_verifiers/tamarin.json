{
  "id": "tamarin",
  "name": "Tamarin Prover",
  "category": "security",
  "subcategory": "protocol_verifiers",

  "description": "Security protocol verification with symbolic model for unbounded sessions",
  "long_description": "Tamarin is a powerful tool for symbolic modeling and analysis of security protocols. It supports both falsification and unbounded verification in the symbolic model, using multiset rewriting rules. Tamarin can analyze protocols under various adversary models and has been used to verify TLS 1.3, 5G-AKA, and other important protocols.",

  "capabilities": [
    "unbounded_verification",
    "symbolic_model",
    "multiset_rewriting",
    "equational_theories",
    "observational_equivalence",
    "trace_properties",
    "attack_finding",
    "interactive_proofs"
  ],
  "property_types": ["secrecy", "authentication", "injective_agreement", "non_injective_agreement"],
  "input_languages": ["spthy"],
  "output_formats": ["proof", "attack_trace", "lemma_status"],

  "installation": {
    "methods": [
      {"type": "brew", "command": "brew install tamarin-prover"},
      {"type": "apt", "command": "apt install tamarin-prover"},
      {"type": "source", "url": "https://github.com/tamarin-prover/tamarin-prover"}
    ],
    "dependencies": ["haskell", "maude", "graphviz"],
    "platforms": ["linux", "macos"]
  },

  "documentation": {
    "official": "https://tamarin-prover.github.io/",
    "tutorial": "https://tamarin-prover.github.io/manual/",
    "api_reference": "https://tamarin-prover.github.io/manual/book/",
    "examples": "https://github.com/tamarin-prover/tamarin-prover/tree/develop/examples"
  },

  "tactics": [
    {
      "name": "prove",
      "description": "Verify all lemmas",
      "syntax": "tamarin-prover --prove model.spthy",
      "when_to_use": "Automated proof of protocol properties",
      "examples": ["tamarin-prover --prove protocol.spthy"]
    },
    {
      "name": "interactive",
      "description": "Interactive proof mode",
      "syntax": "tamarin-prover interactive model.spthy",
      "when_to_use": "Manual proof guidance",
      "examples": ["tamarin-prover interactive protocol.spthy"]
    },
    {
      "name": "diff_equiv",
      "description": "Check observational equivalence",
      "syntax": "tamarin-prover --diff model.spthy",
      "when_to_use": "Verifying privacy properties",
      "examples": ["tamarin-prover --diff --prove voting.spthy"]
    },
    {
      "name": "heuristic",
      "description": "Set proof heuristic",
      "syntax": "tamarin-prover --heuristic=S model.spthy",
      "when_to_use": "Tuning automatic proof search",
      "examples": ["tamarin-prover --prove --heuristic=O protocol.spthy"]
    }
  ],

  "error_patterns": [
    {
      "pattern": "attack found",
      "meaning": "Protocol violates property",
      "common_causes": ["Design flaw", "Missing check"],
      "fixes": ["Examine attack trace", "Add protocol step", "Strengthen property"]
    },
    {
      "pattern": "non-termination",
      "meaning": "Proof search did not terminate",
      "common_causes": ["Complex protocol", "Wrong heuristic"],
      "fixes": ["Use interactive mode", "Add proof hints", "Change heuristic"]
    },
    {
      "pattern": "partial deconstructions",
      "meaning": "Tool cannot simplify term",
      "common_causes": ["Unusual equational theory", "Complex terms"],
      "fixes": ["Simplify model", "Add explicit rules"]
    }
  ],

  "integration": {
    "dashprove_backend": true,
    "usl_property_types": ["security_protocol"],
    "cli_command": "dashprove verify --backend tamarin"
  },

  "performance": {
    "typical_runtime": "Seconds to hours",
    "scalability": "Unbounded sessions (symbolic)",
    "memory_usage": "Moderate to high"
  },

  "comparisons": {
    "similar_tools": ["proverif", "verifpal", "scyther"],
    "advantages": [
      "Unbounded verification",
      "Interactive proof mode",
      "Flexible equational theories",
      "Observational equivalence"
    ],
    "disadvantages": [
      "Steeper learning curve",
      "Sometimes requires manual guidance"
    ]
  },

  "metadata": {
    "version": "1.8.0",
    "last_updated": "2025-12-20",
    "maintainer": "ETH Zurich / CISPA",
    "license": "GPL-3.0"
  }
}
