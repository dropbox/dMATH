; QF_BV benchmark: puzzle_16
(set-logic QF_BV)
(declare-fun v0 () (_ BitVec 16))
(declare-fun v1 () (_ BitVec 16))
(declare-fun v2 () (_ BitVec 16))
(assert (= (bvxor v2 v1) #xcc9f))
